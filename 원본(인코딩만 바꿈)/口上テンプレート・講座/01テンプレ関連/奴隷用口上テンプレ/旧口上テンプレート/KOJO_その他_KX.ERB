;初めてA絶頂　CFLAG:243
;-------------------------------------------------
IF NOWEX:2 > 0 && CFLAG:243 == 0
	CFLAG:243 = 1
ENDIF

;-------------------------------------------------
;初めてB絶頂　CFLAG:244
;-------------------------------------------------
IF NOWEX:3 > 0 && CFLAG:244 == 0
	CFLAG:244 = 1
ENDIF

;-------------------------------------------------
;絶頂　CFLAG:245
;これいらなくないかな…だって毎回出っぱなしでちょっとうざｒｙ
;-------------------------------------------------
IF TFLAG:29 > 0
	CALL PALAM_MESSAGE_B
ENDIF

;-------------------------------------------------
;射精　CFLAG:246
;同上
;-------------------------------------------------
;射精している
IF TFLAG:10 > 0
ENDIF

;-------------------------------------------------
;ドライオーガズム
;-------------------------------------------------
;空射精している
IF NOWEX:41
	CALL PALAM_MESSAGE_A3
ENDIF

;-------------------------------------------------
;空射精　※リボンパッチによる追加
;-------------------------------------------------
;空射精している
IF NOWEX:40
	CALL PALAM_MESSAGE_A2
	;空射精回7以上
	IF TFLAG:802 & 32768
	;空射精3回以上
	ELSEIF TFLAG:802 & 4096
	;空射精1回以上
	ELSEIF TFLAG:802 & 1024
	ENDIF
	;空射精の回数をカウント
	IF TFLAG:802 & 32768
		TFLAG:802 |= 65536
	ELSEIF TFLAG:802 & 16384
		TFLAG:802 |= 32768
		SIF TFLAG:10 >= 2
			TFLAG:802 |= 65536
	ELSEIF TFLAG:802 & 8192
		TFLAG:802 |= 16384
		SIF TFLAG:10 >= 2
			TFLAG:802 |= 32768
	ELSEIF TFLAG:802 & 4096
		TFLAG:802 |= 8192
		SIF TFLAG:10 >= 2
			TFLAG:802 |= 16384
	ELSEIF TFLAG:802 & 2048
		TFLAG:802 |= 4096
		SIF TFLAG:10 >= 2
			TFLAG:802 |= 8192
	ELSEIF TFLAG:802 & 1024
		TFLAG:802 |= 2048
		SIF TFLAG:10 >= 2
			TFLAG:802 |= 4096
	ELSE
		TFLAG:802 |= 1024
		SIF TFLAG:10 >= 2
			TFLAG:802 |= 2048
	ENDIF
ENDIF

;-------------------------------------------------
;噴乳　CFLAG:247
;-------------------------------------------------
;噴乳している
IF TFLAG:11 > 0
	CFLAG:247 = 1
ENDIF



