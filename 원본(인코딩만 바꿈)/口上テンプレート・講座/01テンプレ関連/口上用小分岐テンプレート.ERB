	;-------------------------------------------------
	;調教対象のマゾっ気
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ABL:マゾっ気
		SELECTCASE ABL:マゾっ気
			;マゾっ気５以上
			CASE IS >= 5
				PRINTFORMW 
				PRINTFORMW 
			;マゾっ気３以上
			CASE IS >= 3
				PRINTFORMW 
				PRINTFORMW 
			;マゾっ気１以上
			CASE IS >= 1
				PRINTFORMW 
				PRINTFORMW 
		ENDSELECT
		RETURN LOCAL
	ENDIF

