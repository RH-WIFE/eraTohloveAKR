@END_PROSTITUTION
DRAWLINE
PRINTFORML %CALLNAME:A%が帰ってきた

;保存しておいたもの呼び出し
B = CFLAG:A:18
S = CFLAG:A:19

;イベント口上の表示
T = TARGET
TARGET = A
TFLAG:13 = 69
CALL KOJO_MESSAGE_EVENT
TARGET = T




;--------------------------------------------------------
;経験の加算処理と表示
C = 0
D = 0
E = 0
F = 0
G = 0
L = 0
Q = 0
R = 0
U = 0
;--------------------------------------------------------
PRINTFORML 【%CALLNAME:A%の経験と珠】

;-----------------------------
;娼館奉公経験
;-----------------------------
EXP:A:娼館奉公経験 += B
PRINTFORML 娼館奉公経験＋{B}


;-----------------------------
;性交、絶頂系
C = 0
E = 0
F = 0
;-----------------------------
;C = 回数と快Ｃの珠用
IF !TEQUIP:A:貞操帯 && (CFLAG:A:160 || CFLAG:A:169 || CFLAG:A:170 || CFLAG:A:175)
	;性的奉仕（受け/攻め）、逆セックス
	C += CFLAG:A:160+CFLAG:A:169+CFLAG:A:170
	
	;U = 絶頂回数保存
	U += C/3
	
	;習得の珠用
	Q += (CFLAG:A:169+CFLAG:A:170)*(ABL:A:欲望+1)
	;快Ｃの珠用
	C += C*(ABL:A:Ｃ感覚+1)
ENDIF

;E = 回数と快Ｖの珠用
IF !TEQUIP:A:貞操帯 && VAGINA(A) && (CFLAG:A:160 || CFLAG:A:161 || CFLAG:A:173)
	;性的奉仕（受け）、Ｖセックス
	E += CFLAG:A:160+CFLAG:A:161
	
	;U = 絶頂回数保存
	U += E/3
	
	EXP:A:Ｖ経験 += E
	PRINTFORML %EXPNAME:0%＋{E}
	
	;快Ｖの珠用
	E += E*(ABL:A:Ｖ感覚+1)
ENDIF

;F = 回数と快Ａの珠用
IF CFLAG:A:160 || CFLAG:A:162 || CFLAG:A:168 || CFLAG:A:174
	;性的奉仕（受け）、Ａセックス、浣腸プレイ
	F += CFLAG:A:160
	F += CFLAG:A:162
	F += CFLAG:A:168/3
	
	;U = 絶頂回数保存
	U += F/3
	
	EXP:A:Ａ経験 += F
	PRINTFORML %EXPNAME:1%＋{F}
	
	;快Ａの珠用
	F += F*(ABL:A:Ａ感覚+1)
		
	;苦痛の珠用
	SIF CFLAG:A:168
		D += CFLAG:A:168
ENDIF

;L = 性交経験
IF CFLAG:A:161 || CFLAG:A:162 || CFLAG:A:170
	L = CFLAG:A:161+CFLAG:A:162+CFLAG:A:170
	
	EXP:A:性交経験 += L
	PRINTFORML 性交経験＋{L}
	
	IF CFLAG:A:170
		EXP:A:挿入経験 += L
		PRINTFORML 挿入経験＋{L}
	ENDIF
	
	;童貞喪失　逆セックスの時のみ
	IF CFLAG:A:175
		PRINTFORML 娼館奉公で童貞喪失
		PRINTFORML 異常経験＋１
		EXP:A:異常経験 += 1
		TALENT:A:童貞 = 0
		CSTR:A:11 = 娼館奉公
		
		;不快の珠
		SIF TALENT:A:恋慕
			G += E+F
		SIF TALENT:A:親愛
			G += (E+F)*2
	ENDIF
	
	;処女喪失
	IF CFLAG:A:173 && TALENT:A:処女
		PRINTFORML 娼館奉公で処女喪失
		PRINTFORML 異常経験＋１
		EXP:A:異常経験 += 1
		TALENT:A:処女 = 0
		CSTR:A:10 = 娼館奉公
		
		;苦痛の珠用
		D += 10
		
		;不快の珠
		SIF TALENT:A:恋慕
			G += (E+F)*3
		SIF TALENT:A:親愛
			G += (E+F)*5
	ELSEIF CFLAG:A:173 && TALENT:A:再生処女
		PRINTFORML 娼館奉公で再生処女喪失
		TALENT:A:再生処女 = 0
		
		;苦痛の珠用
		D += 5
		
		;不快の珠
		SIF TALENT:A:恋慕
			G += (E+F)*2
		SIF TALENT:A:親愛
			G += (E+F)*3
	ENDIF
	
	;膣射経験
	IF E && (CFLAG:A:108 == 0 || TEQUIP:A:貞操帯 == 0)
		EXP:A:膣射経験 += E/2
		;娼館などの客と奴隷間の中田氏カウント用
		CFLAG:A:105 += E/2
		PRINTFORML %EXPNAME:5%＋{E/2}
		
		;不快の珠
		SIF TALENT:A:恋慕
			G += (E+F)*2
		SIF TALENT:A:親愛
			G += (E+F)*3
	ENDIF
	
	;バックバージン喪失
	IF CFLAG:A:174
		PRINTFORML 娼館奉公でバックバージン喪失
		PRINTFORML 異常経験＋１
		EXP:A:異常経験 += 1
		TALENT:A:バックバージン = 0
		CSTR:A:12 = 娼館奉公
		
		;苦痛の珠用
		D += 10
		
		;不快の珠
		SIF TALENT:A:恋慕
			G += (E+F)*2
		SIF TALENT:A:親愛
			G += (E+F)*3
	ENDIF
	
	;腸射経験
	IF F && CFLAG:A:108 == 0
		EXP:A:腸射経験 += F/2
		;娼館などの客と奴隷間の腸中田氏カウント用
		CFLAG:A:705 += F/2
		PRINTFORML %EXPNAME:14%＋{F/2}
		
		;不快の珠
		SIF TALENT:A:恋慕
			G += (E+F)*2
		SIF TALENT:A:親愛
			G += (E+F)*3
	ENDIF
	
	;U = 絶頂回数保存
	U += L/3
ENDIF

;U = 絶頂回数
IF U
	EXP:A:絶頂経験 += U
	PRINTFORML 絶頂経験＋{U}
	
	IF PENIS(A)
		IF TALENT:A:子供サイズ
			EXP:A:空射精経験 += U
			PRINTFORML 空射精経験＋{U}
		ELSEIF TEQUIP:A:貞操帯
			EXP:A:射精経験 += U/2
			PRINTFORML 射精経験＋{U/2}
		ELSE
			EXP:A:射精経験 += U
			PRINTFORML 射精経験＋{U}
		ENDIF
	ENDIF
ENDIF


;-----------------------------
;奉仕系
;-----------------------------
IF CFLAG:A:160 || CFLAG:A:169
	L = CFLAG:A:160+CFLAG:A:169
	
	IF CFLAG:A:160
		EXP:A:フェラ経験 += L
		PRINTFORML フェラ経験＋{L}
		EXP:A:精液経験 += L/2
		PRINTFORML 精液経験＋{L/2}
	ENDIF
	
	EXP:A:奉仕快楽経験 += L*ABL:A:奉仕精神
	PRINTFORML 奉仕快楽経験＋{L*ABL:A:奉仕精神}
	
	;習得の珠用
	Q += L*(ABL:A:奉仕精神+1)
	
	;不快の珠
	SIF TALENT:A:恋慕
		G += E+F
	SIF TALENT:A:親愛
		G += (E+F)*2
ENDIF


;-----------------------------
;ＳＭ系
;-----------------------------
;攻め
L = 0
IF CFLAG:A:171 || CFLAG:A:172
	L += CFLAG:A:171
	L += CFLAG:A:172*2
	
	IF ABL:A:サドっ気 >= 8 || TALENT:A:サド == 2
		L *= 3
	ELSEIF ABL:A:サドっ気 >= 5 || TALENT:A:サド
		L *= 2
	ELSEIF ABL:A:サドっ気 >= 3
	ELSEIF ABL:A:サドっ気 == 2 || TALENT:A:小悪魔
		L /= 2
	ELSEIF ABL:A:サドっ気 == 1
		L /= 3
	ELSE
		L /= 5
	ENDIF
	
	IF L > 0
		EXP:A:嗜虐快楽経験 += L
		PRINTFORML 嗜虐快楽経験＋{L}
	ENDIF
	
	;習得の珠用
	Q += L*(ABL:A:サドっ気+1)
ENDIF

;受け
L = 0
IF CFLAG:A:163 || CFLAG:A:164
	L += CFLAG:A:163
	L += CFLAG:A:164*3
	
	EXP:A:緊縛経験 += (CFLAG:A:163+CFLAG:A:164)/2
	PRINTFORML 緊縛経験＋{(CFLAG:A:163+CFLAG:A:164)/2}
	
	IF ABL:A:マゾっ気 >= 8 || TALENT:A:マゾ == 2
		L *= 3
	ELSEIF ABL:A:マゾっ気 >= 5 || TALENT:A:マゾ
		L *= 2
	ELSEIF ABL:A:マゾっ気 >= 3
	ELSEIF ABL:A:マゾっ気 == 2 || TALENT:A:倒錯的
		L /= 2
	ELSEIF ABL:A:マゾっ気 == 1
		L /= 3
	ELSE
		L /= 5
	ENDIF
	
	IF L > 0
		EXP:A:苦痛快楽経験 += L
		PRINTFORML 苦痛快楽経験＋{L}
	ENDIF
	
	;習得の珠用
	Q += L*(ABL:A:マゾっ気+1)
	
	;苦痛、恐怖の珠用
	D += L
ENDIF


;-----------------------------
;スカトロ系
L = 0
;-----------------------------
IF CFLAG:A:166 || CFLAG:A:167 || CFLAG:A:168
	;浣腸プレイ
	IF CFLAG:A:168
		;ハードＳＭは数が増える
		IF CFLAG:A:164
			L += CFLAG:A:168*2
			
			;苦痛、恐怖の珠用
			D += CFLAG:A:168*2
		ELSE
			L += CFLAG:A:168
			
			;苦痛、恐怖の珠用
			D += CFLAG:A:168
		ENDIF
		
		EXP:A:浣腸経験 += CFLAG:A:168*2
		PRINTFORML 浣腸経験＋{CFLAG:A:168*2}
		EXP:A:排便経験 += CFLAG:A:168
		PRINTFORML 排便経験＋{CFLAG:A:168}
		
		;不快の珠
		G += (10-ABL:A:スカ中毒)*2
	ENDIF
	
	;飲尿プレイ
	IF CFLAG:A:167
		L += CFLAG:A:167
		
		EXP:A:飲尿経験 += CFLAG:A:167
		PRINTFORML 飲尿経験＋{CFLAG:A:167}
		
		;不快の珠
		G += (10-ABL:A:スカ中毒)*2
	ENDIF
	
	;放尿鑑賞
	IF CFLAG:A:166
		L += CFLAG:A:166
		
		EXP:A:放尿経験 += CFLAG:A:166
		PRINTFORML 放尿経験＋{CFLAG:A:166}
		
		;不快の珠
		G += 10-ABL:A:スカ中毒
	ENDIF
	
	IF !EXP:A:スカ経験
		PRINTFORML 異常経験＋１
		EXP:A:異常経験 += 1
	ENDIF
	
	EXP:A:スカ経験 += L
	PRINTFORML スカ経験＋{L}
ENDIF


;-----------------------------
;恥情
