IF TEQUIP:A:貞操帯
	CFLAG:A:602 += B*5
	SIF TALENT:PLAYER:千里眼
		PRINTFORML 禁欲ポイント＋{B*5}
ELSE
	CFLAG:A:602 = 0
ENDIF

