;====================================================================
;プレイ内容の判定
;====================================================================
@PROSTITUTION_PLAY_CHECK

;--------------------------------------------------------
;人数とプレイ内容の計算
;最大人数 = 朝に出したなら12人、夜に出したなら8人
B = 0

;性的奉仕(受)	CFLAG:A:160
;Ｖセックス		CFLAG:A:161
;Ａセックス		CFLAG:A:162
;ソフトSM(受)	CFLAG:A:163
;ハードSM(受)	CFLAG:A:164
;羞恥心			CFLAG:A:165
;放尿鑑賞		CFLAG:A:166
;飲尿プレイ		CFLAG:A:167
;浣腸プレイ		CFLAG:A:168
;性的奉仕(攻)	CFLAG:A:169
;逆セックス		CFLAG:A:170
;ソフトSM(攻)	CFLAG:A:171
;ハードSM(攻)	CFLAG:A:172
;処女貫通		CFLAG:A:173
;Ａ処女貫通		CFLAG:A:174
;童貞食い		CFLAG:A:175

CFLAG:A:160 = 0
CFLAG:A:161 = 0
CFLAG:A:162 = 0
CFLAG:A:163 = 0
CFLAG:A:164 = 0
CFLAG:A:165 = 0
CFLAG:A:166 = 0
CFLAG:A:167 = 0
CFLAG:A:168 = 0
CFLAG:A:169 = 0
CFLAG:A:170 = 0
CFLAG:A:171 = 0
CFLAG:A:172 = 0
CFLAG:A:173 = 0
CFLAG:A:174 = 0
CFLAG:A:175 = 0
;--------------------------------------------------------

REPEAT 12
	;最大人数超えたら抜ける
	IF (B == 12 && TIME == 0) || (B == 8 && TIME != 0)
		BREAK
	;体力ないと客がつかない。客がつかないと体力回復
	ELSEIF BASE:A:体力 < 500
		IF TALENT:A:回復早い || TALENT:A:娼婦 || TALENT:A:隷属 || ABL:A:マゾっ気 >= 5
			BASE:A:体力 += 300
		ELSEIF TALENT:A:回復遅い || ABL:A:マゾっ気 < 3
			BASE:A:体力 += 100
		ELSE
			BASE:A:体力 += 200
		ENDIF
	;なんもなければ客を選出
	ELSE
		;-----------------------
		;人数を増やす
		;-----------------------
		B += 1
		
		LOCAL:1 = 0
		LOCAL:2 = 0
		;-----------------------
		;奴隷の受け(LOCAL:1)攻め(LOCAL:2)保存
		;-----------------------
		IF TALENT:A:攻め
			LOCAL:2 = 1
		ELSEIF TALENT:A:受け || TALENT:A:オメガ
			LOCAL:1 = 1
		ELSE
			IF !RAND:5
				LOCAL:2 = 1
			ELSE
				LOCAL:1 = 1
			ENDIF
		ENDIF
		
		;-----------------------
		;客のドＳ(LOCAL:3)、Ｓ(LOCAL:4)、普通(LOCAL:5)、Ｍ(LOCAL:6)
		;-----------------------
		;奴隷が攻め
		IF LOCAL:2
			;Ｍの客が増える
			IF RAND:10 < ABL:A:サドっ気
				LOCAL:6 = 1
			;でなければ普通の客
			ELSE
				LOCAL:5 = 1
			ENDIF
		;奴隷が受け
		ELSE
			K:1 = RAND:10
			;ドＳ
			IF K:1 < 2
				LOCAL:3 = 1
			;Ｓの客が増える
			ELSEIF K:1 < ABL:A:マゾっ気
				LOCAL:4 = 1
			;でなければ普通の客
			ELSE
				LOCAL:5 = 1
			ENDIF
		ENDIF
		
		;それぞれのプレイが有ったかどうか判定
		;-----------------------
		;性的奉仕は絶対にある
		;-----------------------
		;奴隷攻め
		IF LOCAL:2
			CFLAG:A:169 += 1
			BASE:A:体力 -= 150
		;奴隷受け
		ELSE
			CFLAG:A:160 += 1
			BASE:A:体力 -= 100
		ENDIF
		
		;-----------------------
		;ハードSM（オプション有、ランダム1/3）
		;-----------------------
		IF (FLAG:160 & 8) && RAND:2 == 0
			;奴隷攻め
			IF LOCAL:2
				CFLAG:A:172 += 1
			;奴隷受け
			ELSE
				CFLAG:A:164 += 1
			ENDIF
			;余裕
			IF (LOCAL:1 && (ABL:A:マゾっ気 >= 5 || TALENT:A:マゾ)) || (LOCAL:2 && (ABL:A:サドっ気 >= 5 || TALENT:A:サド))
				BASE:A:体力 -= 300
			;疲れる
			ELSEIF (LOCAL:1 && (ABL:A:マゾっ気 < 3 || !TALENT:A:倒錯的)) || (LOCAL:2 && (ABL:A:サドっ気 < 3 || TALENT:A:小悪魔))
				BASE:A:体力 -= 800
			ELSE
				BASE:A:体力 -= 500
			ENDIF
			
			;-----------------------
			;浣腸プレイ（オプション有、ランダム1/5、奴隷受け）
			;羞恥(CFLAG:A:165)も感じる
			;-----------------------
			IF (FLAG:160 & 768) && RAND:4 == 0 && LOCAL:1
				CFLAG:A:168 += 1
				;余裕
				IF ABL:A:マゾっ気 >= 5 || TALENT:A:マゾ || EXP:A:浣腸経験 >= 50 || ABL:A:スカ中毒 >= 3
					BASE:A:体力 -= 200
					CFLAG:A:165 += 1
				;疲れる
				ELSEIF ABL:A:マゾっ気 < 3 || EXP:A:浣腸経験 < 20
					BASE:A:体力 -= 500
					CFLAG:A:165 += 5
				ELSE
					BASE:A:体力 -= 350
					CFLAG:A:165 += 3
				ENDIF
			ENDIF
			
			;-----------------------
			;飲尿プレイ（オプション有、ランダム1/5、奴隷受け）
			;-----------------------
			IF (FLAG:160 & 128) && RAND:4 == 0 && LOCAL:1
				CFLAG:A:167 += 1
				;余裕
				IF ABL:A:マゾっ気 >= 5 || TALENT:A:マゾ || EXP:A:飲尿経験 >= 50 || ABL:A:スカ中毒 >= 3
					BASE:A:体力 -= 50
				;疲れる
				ELSEIF ABL:A:マゾっ気 < 3 || EXP:A:飲尿経験 < 10
					BASE:A:体力 -= 300
				ELSE
					BASE:A:体力 -= 150
				ENDIF
			ENDIF
			
			;-----------------------
			;放尿鑑賞（オプション有、ランダム1/5、奴隷受け）
			;羞恥(CFLAG:A:165)も感じる
			;-----------------------
			IF (FLAG:160 & 192) && RAND:4 == 0 && LOCAL:1
				CFLAG:A:166 += 1
				;余裕
				IF ABL:A:マゾっ気 >= 5 || TALENT:A:マゾ || EXP:A:放尿経験 >= 50 || ABL:A:スカ中毒 >= 3
					BASE:A:体力 -= 50
					CFLAG:A:165 += 1
				;疲れる
				ELSEIF ABL:A:マゾっ気 < 3 || EXP:A:放尿経験 < 20
					BASE:A:体力 -= 200
					CFLAG:A:165 += 3
				ELSE
					BASE:A:体力 -= 100
					CFLAG:A:165 += 2
				ENDIF
			ENDIF
			
			;-----------------------
			;羞恥プレイ（奴隷受けのみ）
			;-----------------------
			IF LOCAL:1
				IF RAND:3 == 0
					CFLAG:A:165 += 5
				ELSEIF RAND:2 == 0
					CFLAG:A:165 += 3
				ELSE
					CFLAG:A:165 += 1
				ENDIF
			ENDIF
		;-----------------------
		;ソフトSM（ランダム1/3）
		;-----------------------
		ELSEIF RAND:2 == 0
			;奴隷攻め
			IF LOCAL:2
				CFLAG:A:171 += 1
			;奴隷受け
			ELSE
				CFLAG:A:163 += 1
			ENDIF
			;余裕
			IF (LOCAL:1 && (ABL:A:マゾっ気 >= 5 || TALENT:A:マゾ)) || (LOCAL:2 && (ABL:A:サドっ気 >= 5 || TALENT:A:サド))
				BASE:A:体力 -= 100
			;疲れる
			ELSEIF (LOCAL:1 && (ABL:A:マゾっ気 < 3 || TALENT:A:倒錯的)) || (LOCAL:2 && (ABL:A:サドっ気 < 3 || TALENT:A:小悪魔))
				BASE:A:体力 -= 300
			ELSE
				BASE:A:体力 -= 200
			ENDIF
			
			;-----------------------
			;放尿鑑賞（オプション有、ランダム1/5、奴隷受け）
			;羞恥(CFLAG:A:165)も感じる
			;-----------------------
			IF (FLAG:160 & 192) && RAND:4 == 0 && LOCAL:1
				CFLAG:A:166 += 1
				;余裕
				IF ABL:A:マゾっ気 >= 5 || TALENT:A:マゾ || EXP:A:放尿経験 >= 50 || ABL:A:スカ中毒 >= 3
					BASE:A:体力 -= 50
					CFLAG:A:165 += 1
				;疲れる
				ELSEIF ABL:A:マゾっ気 < 3 || TALENT:A:倒錯的 || EXP:A:放尿経験 < 20
					BASE:A:体力 -= 200
					CFLAG:A:165 += 3
				ELSE
					BASE:A:体力 -= 100
					CFLAG:A:165 += 2
				ENDIF
			ENDIF
			
			;-----------------------
			;羞恥プレイ（奴隷受けのみ）
			;-----------------------
			IF LOCAL:1
				IF RAND:3 == 0
					CFLAG:A:165 += 5
				ELSEIF RAND:2 == 0
					CFLAG:A:165 += 3
				ELSE
					CFLAG:A:165 += 1
				ENDIF
			ENDIF
		ENDIF
		
		;本番があるか判定
		;奴隷攻め
		IF LOCAL:2
			;本番無し（奴隷童貞でオプションがない、もしくはランダム1/5）
			IF (TALENT:A:童貞 && (FLAG:160 & 4) == 0) || RAND:4 == 0
				BASE:A:体力 -= 100
			;-----------------------
			;本番有り
			;-----------------------
			ELSE
				CFLAG:A:170 += 1
				BASE:A:体力 -= 200
				;-----------------------
				;童貞食い
				;-----------------------
				IF TALENT:A:童貞 && (FLAG:160 & 4)
					CFLAG:A:175 += 1
					BASE:A:体力 -= 200
				ENDIF
			ENDIF
		;奴隷受け
		ELSE
			;Ｖ本番無し（奴隷処女でオプションがない、もしくはランダム1/5、もしくはヴァギナない）
			IF !VAGINA(A) || ((TALENT:A:処女 || TALENT:A:再生処女) && (FLAG:160 & 1) == 0) || RAND:4 == 0
				SIF VAGINA(A)
					BASE:A:体力 -= 100
			;-----------------------
			;Ｖ本番有り
			;-----------------------
			ELSE
				CFLAG:A:161 += 1
				BASE:A:体力 -= 200
				;-----------------------
				;処女姦通
				;-----------------------
				IF (TALENT:A:処女 || TALENT:A:再生処女) && (FLAG:160 & 1)
					CFLAG:A:173 += 1
					BASE:A:体力 -= 300
				ENDIF
			ENDIF
			
			;Ａ本番無し（奴隷処女でオプションがない、もしくはランダム1/5(女は3/4)）
			IF (TALENT:A:バックバージン && (FLAG:160 & 2) == 0) || (VAGINA(A) && RAND:3 > 0) || (RAND:4 == 0)
				BASE:A:体力 -= 100
			;-----------------------
			;Ａ本番有り
			;-----------------------
			ELSE
				CFLAG:A:162 += 1
				BASE:A:体力 -= 200
				;-----------------------
				;バックバージン姦通
				;-----------------------
				IF TALENT:A:バックバージン && (FLAG:160 & 2)
					CFLAG:A:174 += 1
					BASE:A:体力 -= 300
				ENDIF
			ENDIF
		ENDIF
		
		;羞恥補正
		IF RAND:3 == 0
			CFLAG:A:165 += 3
		ELSEIF RAND:2 == 0
			CFLAG:A:165 += 2
		ELSE
			CFLAG:A:165 += 1
		ENDIF
		IF TALENT:A:恥じらい
			CFLAG:A:165 *= 2
		ELSEIF TALENT:A:恥薄い
			CFLAG:A:165 /= 2
		ENDIF
		SIF CFLAG:A:165 < 1
			CFLAG:A:165 = 1
		
		SIF BASE:A:体力 < 1
			BASE:A:体力 = 1
	ENDIF
REND




;====================================================================
;帰ってきたときの処理
