IF C > 0
	JUEL:A:快Ｃ += C*50
	PRINTFORML 快Ｃの珠＋{C*50}
ENDIF
IF E > 0
	JUEL:A:快Ｖ += E*30
	PRINTFORML 快Ｖの珠＋{E*30}
ENDIF
IF F > 0
	JUEL:A:快Ａ += F*30
	PRINTFORML 快Ａの珠＋{F*30}
ENDIF

JUEL:A:恭順 += B*100
PRINTFORML 恭順の珠＋{B*100}

