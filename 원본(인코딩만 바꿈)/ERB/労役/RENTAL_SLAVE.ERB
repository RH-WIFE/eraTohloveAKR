	JUEL:A:苦痛 += D*30
	PRINTFORML 苦痛の珠＋{D*30}
	JUEL:A:恐怖 += D*15
	PRINTFORML 恐怖の珠＋{D*15}
ENDIF
IF G > 0
	JUEL:A:不快 += G*50
	PRINTFORML 不快の珠＋{G*50}
ENDIF

IF D > 0 || G > 0 || E > 0 || F > 0
	JUEL:A:否定 += ((D+G+E+F)*200)/ABL:A:従順
	PRINTFORML 否定の珠＋{((D+G+E+F)*200)/ABL:A:従順}
ENDIF


;--------------------------------------------------------
;体力の減少、ストレス、禁欲ポイント
;--------------------------------------------------------

;禁欲ポイント
IF TEQUIP:A:貞操帯 || CFLAG:A:152 & 4352
	CFLAG:A:602 += B*5
	SIF TALENT:PLAYER:千里眼
		PRINTFORML 禁欲ポイント＋{B*5}
ELSE
	CFLAG:A:602 = 0
ENDIF

;ストレス値の付加
CFLAG:A:10 += B*2
SIF TALENT:PLAYER:千里眼
	PRINTFORML 疲労度＋{B}


;-----------------------------
;報酬額の加算
;-----------------------------
;最後に金額の加算
;獣姦（奴隷受けのみ）
SIF CFLAG:A:175
	S += B*5000
;スカトロ大（奴隷受けのみ）
SIF CFLAG:A:179
	S += B*5000
;ハードSM
SIF CFLAG:A:170
	S += B*3500
;複数プレイ
SIF CFLAG:A:173 || CFLAG:A:174
	S += B*2000
;スカトロ小（奴隷受けのみ）
SIF CFLAG:A:176 || CFLAG:A:177
	S += B*1000

;満足してもらえなかった場合はむしろ減らす
;ハードＳＭ攻め
SIF CFLAG:A:171 && ABL:A:サドっ気 < 4
	S -= B*5000
;ソフトＳＭ攻め
SIF CFLAG:A:169 && ABL:A:サドっ気 < 2
	S -= B*3500

PRINTFORMW %CALLNAME:A%のレンタル額として{S}朱を得ました
MONEY += S

;フラグのリセット
CFLAG:A:12 = 0
CFLAG:A:13 = 0
CFLAG:A:14 = 0
CFLAG:A:15 = 0


RETURN 1




