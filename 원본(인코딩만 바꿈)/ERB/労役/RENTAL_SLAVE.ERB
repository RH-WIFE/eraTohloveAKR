;====================================================================
@RENTAL_PLAY_CHECK

;-----------------------
;奴隷の受け(LOCAL:1)攻め(LOCAL:2)保存
;-----------------------
IF TALENT:A:攻め
	LOCAL:2 = 1
ELSEIF TALENT:A:受け || TALENT:A:オメガ
	LOCAL:1 = 1
ELSE
	IF !RAND:5
		LOCAL:2 = 1
	ELSE
		LOCAL:1 = 1
	ENDIF
ENDIF

;-----------------------
;客のドＳ(LOCAL:3)、Ｓ(LOCAL:4)、普通(LOCAL:5)、Ｍ(LOCAL:6)
;-----------------------
;奴隷が攻め
IF LOCAL:2
	;Ｍの客が増える
	IF RAND:10 < ABL:A:サドっ気
		LOCAL:6 = 1
	;でなければ普通の客
	ELSE
		LOCAL:5 = 1
	ENDIF
;奴隷が受け
ELSE
	K:1 = RAND:10
	;ドＳ
	IF K:1 < 2
		LOCAL:3 = 1
	;Ｓの客が増える
	ELSEIF K:1 < ABL:A:マゾっ気
		LOCAL:4 = 1
	;でなければ普通の客
	ELSE
		LOCAL:5 = 1
	ENDIF
ENDIF

;-----------------------
;メインとなるプレイの判定
;-----------------------
;獣姦（奴隷受けのみ）
IF RAND:12 == 0 && (FLAG:160 & 32) && LOCAL:1
	CFLAG:A:152 |= 1
;スカトロ（奴隷受けのみ）
ELSEIF RAND:11 == 0 && (FLAG:160 & 960) && LOCAL:1
	CFLAG:A:152 |= 2
;ハードSM
ELSEIF RAND:10 == 0 && (FLAG:160 & 8) && (LOCAL:4 || LOCAL:6)
	;客がドＳ
	IF LOCAL:4
		CFLAG:A:152 |= 8
	;客がＭ
	ELSE
		CFLAG:A:152 |= 4
	ENDIF
;複数プレイ
ELSEIF RAND:9 == 0 && (FLAG:160 & 16)
	;奴隷攻め
	IF LOCAL:2
		CFLAG:A:152 |= 64
	;奴隷受け
	ELSE
		CFLAG:A:152 |= 128
	ENDIF
;ソフトSM
ELSEIF RAND:8 == 0 && (LOCAL:3 || LOCAL:6)
	;客がＳ
	IF LOCAL:3
		CFLAG:A:152 |= 32
	;客がＭ
	ELSE
		CFLAG:A:152 |= 16
	ENDIF
;フェチ。挿入なし
ELSEIF RAND:7 == 0
	CFLAG:A:152 |= 256
;羞恥系
ELSEIF RAND:6 == 0
	CFLAG:A:152 |= 512
;快楽責め
ELSEIF RAND:5 == 0
	;ランダムで焦らし責めに
	IF TEQUIP:A:貞操帯 || !RAND:3
		CFLAG:A:152 |= 4096
	ELSE
		CFLAG:A:152 |= 1024
	ENDIF
;何もなかった（いちゃいちゃ含む）
ELSE
	CFLAG:A:152 = 0
ENDIF


