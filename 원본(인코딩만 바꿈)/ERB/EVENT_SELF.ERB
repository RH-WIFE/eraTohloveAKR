;SELF.ERB、奴隷からの奉仕などのイベント
;-------------------------------------------------
;朝に起こるイベント
;-------------------------------------------------
@EVENT_NEWDAY_SELF
;調教対象が居ない場合は終了
SIF TARGET < 0
	RETURN 0
;全体判定、陥落素質が無い場合もしくは反発刻印があるときは関数終了
SIF (TALENT:恋慕 == 0 && TALENT:淫乱 == 0 && TALENT:服従 == 0 && TALENT:親愛 == 0 && TALENT:娼婦 == 0 && TALENT:隷属 == 0) || MARK:反発刻印
	RETURN 0
;気絶していたら関数終了
SIF TALENT:気絶
	RETURN 0

;朝イベントキャンセルフラグが立ってる場合は終了
SIF CFLAG:42
	RETURN 0

VARSET LOCAL
LOCAL:1 = 10
;イベント決定
FOR LOCAL, 1, 10
	CALLFORM SELF_MORNING_ABLE{LOCAL}
	IF RESULT
		LOCAL:(LOCAL:1) = LOCAL
		LOCAL:1 += 1
	ENDIF
NEXT
LOCAL:1 -= 10
SIF LOCAL:1 == 0
	RETURN 0
LOCAL:2 = LOCAL:(RAND:(LOCAL:1) + 10)
IF LOCAL:2 == 7 || LOCAL:2 == 8
	CALLFORM MORNING_EVENT_{LOCAL:2}
ELSE
	CALLFORM SELF_MORNING_{LOCAL:2}
ENDIF

