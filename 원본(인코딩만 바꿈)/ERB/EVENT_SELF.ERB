						JUEL:潤滑 += A*300/2
						JUEL:恭順 += A*250/2
						JUEL:抑鬱 += A*300/2
						;ペニスがある、子供サイズで無いと射精経験も上昇
						IF PENIS(TARGET) && TALENT:子供サイズ == 0
							EXP:射精経験  += A / 4
							;禁欲ポイント減らす
							CFLAG:602 = 0
						ENDIF
						;母乳体質だと噴乳経験も上昇
						SIF TALENT:母乳体質
							EXP:噴乳経験 += A / 4
					ENDIF
				ELSE
				
					;屈服が3か反感が1以下
					IF MARK:屈服刻印 == 3 || MARK:反発刻印 < 2
						;自慰よりも精液の方が好きなら求愛に行きやすい(まあ抱かないけど)
						IF ABL:精液中毒 - ABL:自慰中毒 + 6 > RAND:12
							Z = 9
							;CALL KOJO_MESSAGE_EVENT
						
							PRINTFORML %CALLNAME%は%CALLNAME:MASTER%を求め、這いより、すがり付いてきた。
							PRINTFORML %CALLNAME:MASTER%はその手を振り払い、部屋を出て行く。
							PRINTFORML 　
							PRINTFORML 開け放たれたままの%CALLNAME%から、再び媚声が漏れ始めた。

							EXP:自慰経験 += A/2
							JUEL:快Ｃ += 500*A/2
							;前につっこんでた
							IF TEQUIP:バイブ
								JUEL:快Ｖ += 400*A/2
								EXP:Ｖ経験 += A
							ENDIF
							;後ろにつっこんでた
							IF TEQUIP:アナルバイブ
								JUEL:快Ａ += 400*A/2
								EXP:Ａ経験 += A
							ENDIF
								
							JUEL:潤滑 += A*300/2
							JUEL:恭順 += A*250/2
							JUEL:欲情 += A*600/2
							JUEL:習得 += A*400/2
							JUEL:抑鬱 += A*300/2
							;ペニスがある、子供サイズで無いと射精経験も上昇
							IF PENIS(TARGET) && TALENT:子供サイズ == 0
								EXP:射精経験  += A/4
								;禁欲ポイント減らす
								CFLAG:602 = 0
							ENDIF
							;母乳体質だと噴乳経験も上昇
							SIF TALENT:母乳体質
								EXP:噴乳経験 += A/4
						
						ELSE
						
							;自慰の方が好き
							
							;( 欲望 + 露出 + マゾが11以上か 解放か 恥薄い) かつはじらいをもっていない
							IF (ABL:欲望 + ABL:露出癖 + ABL:マゾっ気 > 10 || TALENT:恥薄い || TALENT:快感に素直) && TALENT:恥じらい == 0
							
								Z = 10
								;CALL KOJO_MESSAGE_EVENT
							
								PRINTFORML %CALLNAME%は股を大きく開き、%CALLNAME:MASTER%に見てくださいと懇願すると、
								PRINTFORML より大きな声で一人喘ぎ始めた。
								PRINTFORML 　
								PRINTFORML 折角なので気がすむまでやらせることにした。
								EXP:自慰経験 += A/2
								EXP:調教自慰経験 += A/2
								JUEL:欲情 += A*500/2
								JUEL:習得 += A*300/2
							ELSE
							
								Z = 11
								;CALL KOJO_MESSAGE_EVENT
								
								PRINTFORML そのまま続けるように命じると、
								PRINTFORML %CALLNAME%はおそるおそる足を開くと、羞恥で顔を真っ赤に染め、
								PRINTFORML 涙目になりながらも、指を動かし、小さな声をあげ続けた。
								EXP:自慰経験 += A/2
								EXP:調教自慰経験 += A/2
								JUEL:欲情 += A*600/2
								JUEL:習得 += A*600/2
							ENDIF
							
							JUEL:快Ｃ += 500*A/2
							;前につっこんでた
							IF TEQUIP:バイブ
								JUEL:快Ｖ += 400*A/2
								EXP:Ｖ経験 += A*2/2
							ENDIF
							;後ろにつっこんでた
							IF TEQUIP:アナルバイブ
								JUEL:快Ａ += 400*A/2
								EXP:Ａ経験 += A
							ENDIF
								
							JUEL:潤滑 += A*300/2
							JUEL:恭順 += A*250/2
							JUEL:抑鬱 += A*300/2
							;ペニスがある、子供サイズで無いと射精経験も上昇
							IF PENIS(TARGET) && TALENT:子供サイズ == 0
								EXP:射精経験  += A / 4
								;禁欲ポイント減らす
								CFLAG:602 = 0
							ENDIF
							;母乳体質だと噴乳経験も上昇
							SIF TALENT:母乳体質
								EXP:噴乳経験 += A / 4
						ENDIF
					ELSE
						
						Z = 14
						;CALL KOJO_MESSAGE_EVENT
					
						PRINTFORML %CALLNAME%は悲鳴を上げると、布団にもぐりこんでしまったが、
						PRINTFORML そのまま続けるように命じると、
						PRINTFORML 羞恥と悔恨に顔を歪めながらも、指を動かし、声をあげ続けた。
						JUEL:欲情 += A*300/3
						JUEL:習得 += A*300/3
						EXP:自慰経験 += A/3
						EXP:調教自慰経験 += A/3
						JUEL:快Ｃ += 500*A/3
						;前につっこんでた
						IF TEQUIP:バイブ
							JUEL:快Ｖ += 400*A/3
							EXP:Ｖ経験 += A
						ENDIF
						;後ろにつっこんでた
						IF TEQUIP:アナルバイブ
							JUEL:快Ａ += 400*A/3
							EXP:Ａ経験 += A
						ENDIF
							
						JUEL:潤滑 += A*300/3
						JUEL:恭順 += A*250/3
						JUEL:抑鬱 += A*300/3
						;ペニスがある、子供サイズで無いと射精経験も上昇
						IF PENIS(TARGET) && TALENT:子供サイズ == 0
							EXP:射精経験  += A / 6
							;禁欲ポイント減らす
							CFLAG:602 = 0
						ENDIF
						;母乳体質だと噴乳経験も上昇
						SIF TALENT:母乳体質
							EXP:噴乳経験 += A / 6

					ENDIF
				
				ENDIF
				
				PRINTW 　
			
			ENDIF
			
		ELSE
		;ばれなかった
		
			;自慰の報告　従順+露出+マゾ-反発刻印*2
			B = ABL:従順 + ABL:露出癖 + ABL:マゾっ気 - ( MARK:反発刻印 * 2 )
			;素直か幼稚だと言いやすい
			SIF TALENT:素直 || TALENT:幼稚
				B += 2
				
			PRINTL 　
			;報告値が10以上で反発刻印2未満か弱味があると報告する
			IF B >= 10 && (MARK:反発刻印 < 2 || TALENT:弱味)
				Z = 0
				;CALL KOJO_MESSAGE_EVENT
				PRINTFORML 自慰をしたら報告するように言いつけておいたところ、
				PRINTV A
				PRINTFORML 回自慰をしたことを%CALLNAME%は報告してきた。
				JUEL:欲情 += A*200
			ELSE
				PRINTFORM %CALLNAME:MASTER%が部屋を出て行った後、%CALLNAME%は
				PRINTV A
				PRINTFORML 回自慰をしたようだ。
			ENDIF
		
		ENDIF
		
		
		PRINTW 
		CALL PALAMCHANGE
		
		PRINTW 
		RETURN 0
	ENDIF
ENDIF

