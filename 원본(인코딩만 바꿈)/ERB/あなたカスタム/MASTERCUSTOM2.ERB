;Ｒから移植
@MASTER_CUSTOM2(ARG)
;-----------------------------------------------------------
;表示部分
;-----------------------------------------------------------
PRINTFORML %CALLNAME:ARG%のステータスを設定出来ます。
PRINTL 変更したい項目を選んでください。
PRINTW (極端な設定により難易度が大きく変わる場合もありますのでご注意ください)
PRINTL 
WHILE 1
	DRAWLINE
	LOCAL:1 = 0
	LOCAL:2 = GETCOLOR()
	LOCAL:4 = GETSTYLE()
	PRINT 名前： 
	PRINTBUTTON @"[{LOCAL:1,2}] %NAME:ARG%", LOCAL:1++
	PRINT 　　　呼名： 
	PRINTBUTTON @"[{LOCAL:1,2}] %CALLNAME:ARG%", LOCAL:1++
	PRINTL 
	PRINTL □ 素質 □-----------------------------------------------------------------------
	PRINT 性別： 
	SETCOLOR MALE(ARG) == 0 ? 0x00FF00 # 0xA0A0A0
	PRINTBUTTON @"[{LOCAL:1,2}] 女の子      　　", LOCAL:1++
	SETCOLOR TALENT:ARG:ふたなり ? 0x00FF00 # 0xA0A0A0
	PRINTFORM [{LOCAL:1++,2}] %TALENTNAME:139,12,LEFT%　　
	SETCOLOR TALENT:ARG:半陰半陽 ? 0x00FF00 # 0xA0A0A0
	PRINTFORM [{LOCAL:1++,2}] %TALENTNAME:137,12,LEFT%　　
	SETCOLOR TALENT:ARG:オトコ ? 0x00FF00 # 0xA0A0A0
	PRINTFORML [{LOCAL:1++,2}] %TALENTNAME:140,12,LEFT%
	SETCOLOR LOCAL:2

	PRINT 　　　 
	SETCOLOR !TALENT:ARG:受け && !TALENT:ARG:攻め ? 0x00FF00 # 0xA0A0A0
	PRINTBUTTON @"[{LOCAL:1,2}] リバ      　　　", LOCAL:1++
	SETCOLOR TALENT:ARG:受け ? 0x00FF00 # 0xA0A0A0
	PRINTFORM [{LOCAL:1++,2}] %TALENTNAME:157,12,LEFT%　　
	SETCOLOR TALENT:ARG:攻め ? 0x00FF00 # 0xA0A0A0
	PRINTFORML [{LOCAL:1++,2}] %TALENTNAME:158,12,LEFT%　　
	SETCOLOR LOCAL:2
	
	PRINT 　　　 
	SETCOLOR TALENT:ARG:処女 ? 0x00FF00 # 0xA0A0A0
	SIF TALENT:ARG:オトコ
		FONTSTYLE 4
	PRINTBUTTON @"[{LOCAL:1,2}] 処女      　　　", LOCAL:1++
	FONTSTYLE LOCAL:4
	SETCOLOR TALENT:ARG:童貞 ? 0x00FF00 # 0xA0A0A0
	SIF MALE(ARG) == 0
		FONTSTYLE 4
	PRINTFORM [{LOCAL:1++,2}] %TALENTNAME:160,12,LEFT%　　
	FONTSTYLE LOCAL:4
	SETCOLOR TALENT:ARG:バックバージン ? 0x00FF00 # 0xA0A0A0
	PRINTFORML [{LOCAL:1++,2}] %TALENTNAME:161,12,LEFT%　　
	SETCOLOR LOCAL:2

	LOCAL:3 = 4
	PRINT 性格： 
	FOR LOCAL, 10, 169
		IF LOCAL == 10
			;最初はPRINTBUTTONを使わないと、『性格：』とくっつく
			LOCAL:3--
			SETCOLOR TALENT:ARG:LOCAL ? 0x00FF00 # 0xA0A0A0
			PRINTBUTTON @"[{LOCAL:1,2}] %GET_TALENTNAME(ARG, LOCAL),12,LEFT%", LOCAL:1++
			CONTINUE
		ELSEIF LOCAL == 70
		ELSEIF LOCAL:3
			;空白
			PRINT 　
		ELSE
			;改行
			PRINTL 
			PRINT 　　　 
			LOCAL:3 = 4
		ENDIF
		LOCAL:3--
		SETCOLOR TALENT:ARG:LOCAL ? 0x00FF00 # 0xA0A0A0
		PRINTFORM [{LOCAL:1++,2}] %GET_TALENTNAME(ARG, LOCAL),12,LEFT%
		;性格に分類されない素質を飛ばす
		SELECTCASE LOCAL
			CASE 18, 28
				LOCAL += 1
			CASE 36
				LOCAL += 26
			CASE 66 
				LOCAL += 3
			CASE 71
				LOCAL += 10
			CASE 82
				LOCAL += 4
			CASE 88
				LOCAL += 62
			CASE 151
				LOCAL += 14
		ENDSELECT
	NEXT
	PRINTL 
	SETCOLOR LOCAL:2

	LOCAL:3 = 4
	LOCAL:4 = GETSTYLE()
	PRINT 体質： 
	FOR LOCAL, 40, 151
		IF LOCAL == 40
			LOCAL:3--
			SETCOLOR TALENT:ARG:LOCAL ? 0x00FF00 # 0xA0A0A0
			PRINTBUTTON @"[{LOCAL:1,2}] %GET_TALENTNAME(ARG, LOCAL),12,LEFT%", LOCAL:1++
			CONTINUE
		ELSEIF LOCAL:3
			PRINT 　
		ELSE
			PRINTL 
			PRINT 　　　 
			LOCAL:3 = 4
		ENDIF
		LOCAL:3--
		;女の子/オトコで選択できない素質に取り消し線を入れてみる
		SELECTCASE LOCAL
			CASE 114 TO 117, 119, 152
				SIF MALE(ARG) == 0
					FONTSTYLE 4
			CASE 75, 103, 104
				SIF VAGINA(ARG)
					FONTSTYLE 4
			CASE 109 TO 112
				SIF MALE(ARG)
					FONTSTYLE 4
		ENDSELECT
		SETCOLOR TALENT:ARG:LOCAL ? 0x00FF00 # 0xA0A0A0
		PRINTFORM [{LOCAL:1++,2}] %GET_TALENTNAME(ARG, LOCAL),12,LEFT%
		FONTSTYLE LOCAL:4
		SELECTCASE LOCAL
			CASE 62, 120, 143
				LOCAL += 9
			CASE 44
				LOCAL += 11
			CASE 57
				LOCAL += 2
			CASE 72, 84
				LOCAL += 7
			CASE 81
				LOCAL += 1
			CASE 94
				LOCAL += 5
			CASE 131
				LOCAL += 10
		ENDSELECT
	NEXT
	PRINTL 
	SETCOLOR LOCAL:2

;	PRINT 技能： 
;	SETCOLOR TALENT:ARG:習得早い ? 0x00FF00 # 0xA0A0A0
;	PRINTBUTTON @"[{LOCAL:1,2}] %TALENTNAME:50,12,LEFT%　", LOCAL:1++
;	SETCOLOR TALENT:ARG:習得遅い ? 0x00FF00 # 0xA0A0A0
;	PRINTFORML [{LOCAL:1++,2}] %TALENTNAME:51,12,LEFT%
;	SETCOLOR LOCAL:2

	PRINTL □ 経験 □-----------------------------------------------------------------------
	LOCAL:1 = 100
	PRINT 性経験： 
	SETCOLOR !LOCAL:10 ? 0x00FF00 # 0xA0A0A0
	PRINTBUTTON @"[{LOCAL:1,3}] なし　　", LOCAL:1++
	SETCOLOR LOCAL:10 == 1 ? 0x00FF00 # 0xA0A0A0
	PRINTFORM [{LOCAL:1++,3}] 少し　　
	SETCOLOR LOCAL:10 > 1 ? 0x00FF00 # 0xA0A0A0
	PRINTFORML [{LOCAL:1++,3}] 豊富
	SETCOLOR LOCAL:2

	DRAWLINE
	ALIGNMENT RIGHT
	PRINT [997] - 設定モード変更　　
	PRINT [998] - リセット　　
	PRINTL  [999] - 設定完了　
	ALIGNMENT LEFT

;-----------------------------------------------------------
;入力処理部分
;-----------------------------------------------------------
	$INPUT_LOOP
	INPUT
	SELECTCASE RESULT
		;名前の変更
		CASE 0
			PRINTL 名前を入力してください。
			INPUTS
			NAME:ARG = %RESULTS%
		CASE 1
			PRINTL あだ名を入力してください。
			INPUTS
			CALLNAME:ARG = %RESULTS%
;-----------------------------------------------------------
		;女の子→ふたなり/半陰半陽/オトコ/子供サイズ/短小/巨根/馬並み/包茎/絶倫/童貞をOFF
		CASE 2
			TALENT:ARG:半陰半陽 = 0
			TALENT:ARG:ふたなり = 0
			TALENT:ARG:オトコ = 0
			TALENT:ARG:子供サイズ = 0
			TALENT:ARG:短小 = 0
			TALENT:ARG:巨根 = 0
			TALENT:ARG:馬並み = 0
			TALENT:ARG:包茎 = 0
			TALENT:ARG:絶倫 = 0
			TALENT:ARG:童貞 = 0
		;ふたなり→ふたなりをON。半陰半陽/オトコをOFF
		CASE 3
			TALENT:ARG:ふたなり = 1
			TALENT:ARG:半陰半陽 = 0
			TALENT:ARG:オトコ = 0
		;半陰半陽→半陰半陽をON。ふたなり/オトコ/貧乳/絶壁/巨乳/爆乳をOFF
		CASE 4
			TALENT:ARG:半陰半陽 = 1
			TALENT:ARG:ふたなり = 0
			TALENT:ARG:オトコ = 0
			TALENT:ARG:貧乳 = 0
			TALENT:ARG:絶壁 = 0
			TALENT:ARG:巨乳 = 0
			TALENT:ARG:爆乳 = 0
		;オトコ→オトコをON。ふたなり/半陰半陽/処女/淫壺/Ｖ鈍感/Ｖ敏感/貧乳/絶壁/巨乳/爆乳をOFF
		CASE 5
			TALENT:ARG:オトコ = 1
			TALENT:ARG:半陰半陽 = 0
			TALENT:ARG:ふたなり = 0
			TALENT:ARG:処女 = 0
			TALENT:ARG:淫壺 = 0
			TALENT:ARG:Ｖ鈍感 = 0
			TALENT:ARG:Ｖ敏感 = 0
			TALENT:ARG:貧乳 = 0
			TALENT:ARG:絶壁 = 0
			TALENT:ARG:巨乳 = 0
			TALENT:ARG:爆乳 = 0
;-----------------------------------------------------------
		;リバ→攻め、受けをオフ
		CASE 6
			TALENT:ARG:受け = 0
			TALENT:ARG:攻め = 0
		;受け→受けを反転、攻めをオフ
		CASE 7
			TALENT:ARG:受け = !TALENT:ARG:受け
			TALENT:ARG:攻め = 0
		;攻め→攻めを反転。受けをオフ
		CASE 8
			TALENT:ARG:受け = 0
			TALENT:ARG:攻め = !TALENT:ARG:攻め
		;処女→処女を反転
		CASE 9
			IF VAGINA(ARG) == 0
				CLEARLINE 1
				REUSELASTLINE ないですよね？
				GOTO INPUT_LOOP
			ENDIF
			TALENT:ARG:処女 = !TALENT:ARG:処女
		;童貞→童貞を反転
		CASE 10
			IF PENIS(ARG) == 0
				CLEARLINE 1
				REUSELASTLINE ついてませんよね？
				GOTO INPUT_LOOP
			ENDIF
			TALENT:ARG:童貞 = !TALENT:ARG:童貞
		;バックバージン→バックバージンを反転
		CASE 11
			TALENT:ARG:バックバージン = !TALENT:ARG:バックバージン
;-----------------------------------------------------------
		;臆病→臆病を反転。
		CASE 12
			TALENT:ARG:臆病 = !TALENT:ARG:臆病
		;反抗的→反抗的を反転。素直をOFF
		CASE 13
			TALENT:ARG:反抗的 = !TALENT:ARG:反抗的
			TALENT:ARG:素直 = 0
		;気丈→気丈を反転。
		CASE 14
			TALENT:ARG:気丈 = !TALENT:ARG:気丈
		;素直→素直を反転。反抗的をOFF
		CASE 15
			TALENT:ARG:反抗的 = 0
			TALENT:ARG:素直 = !TALENT:ARG:素直
		;大人しい→大人しいを反転。
		CASE 16
			TALENT:ARG:大人しい = !TALENT:ARG:大人しい
		;プライド高い→プライド高いを反転。プライド低いをOFF
		CASE 17
			TALENT:ARG:プライド高い = !TALENT:ARG:プライド高い
			TALENT:ARG:プライド低い = 0
		;生意気→生意気を反転。
		CASE 18
			TALENT:ARG:生意気 = !TALENT:ARG:生意気
		;プライド低い→プライド低いを反転。プライド高いをOFF
		CASE 19
			TALENT:ARG:プライド高い = 0
			TALENT:ARG:プライド低い = !TALENT:ARG:プライド低い
		;ツンデレ→ツンデレを反転。
		CASE 20
			TALENT:ARG:ツンデレ = !TALENT:ARG:ツンデレ
		;自制的→自制的を反転。
		CASE 21
			TALENT:ARG:自制心 = !TALENT:ARG:自制心
		;無関心→無関心を反転。好奇心をOFF
		CASE 22
			TALENT:ARG:無関心 = !TALENT:ARG:無関心
			TALENT:ARG:好奇心 = 0
		;好奇心→好奇心を反転。無関心をOFF
		CASE 24
			TALENT:ARG:無関心 = 0
			TALENT:ARG:好奇心 = !TALENT:ARG:好奇心
		;感情乏しい→感情乏しいを反転。
		CASE 23
			TALENT:ARG:感情乏しい = !TALENT:ARG:感情乏しい
		;保守的→保守的を反転。
		CASE 25
			TALENT:ARG:保守的 = !TALENT:ARG:保守的
		;楽観的→楽観的を反転。悲観的をOFF
		CASE 26
			TALENT:ARG:楽観的 = !TALENT:ARG:楽観的
			TALENT:ARG:悲観的 = 0
		;悲観的→悲観的を反転。楽観的をOFF
		CASE 27
			TALENT:ARG:楽観的 = 0
			TALENT:ARG:悲観的 = !TALENT:ARG:悲観的
		;一線越えない→一線越えないを反転
		CASE 28
			TALENT:ARG:一線越えない = !TALENT:ARG:一線越えない
		;目立ちたがり→目立ちたがりを反転
		CASE 29
			TALENT:ARG:目立ちたがり = !TALENT:ARG:目立ちたがり
		;貞操観念→貞操観念を反転。貞操無頓着をOFF
		CASE 30
			TALENT:ARG:貞操観念 = !TALENT:ARG:貞操観念
			TALENT:ARG:貞操無頓着 = 0
		;貞操無頓着→貞操無頓着を反転。貞操観念をOFF
		CASE 31
			TALENT:ARG:貞操観念 = 0
			TALENT:ARG:貞操無頓着 = !TALENT:ARG:貞操無頓着
		;抑圧→抑圧を反転。解放をOFF
		CASE 32
			TALENT:ARG:抑圧 = !TALENT:ARG:抑圧
			TALENT:ARG:解放 = 0
		;解放→解放を反転。抑圧をOFF
		CASE 33
			TALENT:ARG:抑圧 = 0
			TALENT:ARG:解放 = !TALENT:ARG:解放
		;抵抗→抵抗を反転
		CASE 34
			TALENT:ARG:抵抗 = !TALENT:ARG:抵抗
		;恥じらい→恥じらいを反転。恥薄いをOFF
		CASE 35
			TALENT:ARG:恥じらい = !TALENT:ARG:恥じらい
			TALENT:ARG:恥薄い = 0
		;恥薄い→恥薄いを反転。恥じらいをOFF
		CASE 36
			TALENT:ARG:恥じらい = 0
			TALENT:ARG:恥薄い = !TALENT:ARG:恥薄い
		;献身的→献身的を反転。
		CASE 37
			TALENT:ARG:献身的 = !TALENT:ARG:献身的
		;汚れ無視→汚れ無視を反転。
		CASE 38
			TALENT:ARG:汚れ無視 = !TALENT:ARG:汚れ無視
		;スキンシップ好き→スキンシップ好きを反転。スキンシップ苦手をOFF
		CASE 39
			TALENT:ARG:スキンシップ好き = !TALENT:ARG:スキンシップ好き
			TALENT:ARG:スキンシップ苦手 = 0
		;スキンシップ苦手→スキンシップ苦手を反転。スキンシップ好きをOFF
		CASE 40
			TALENT:ARG:スキンシップ好き = 0
			TALENT:ARG:スキンシップ苦手 = !TALENT:ARG:スキンシップ苦手
		;快感に素直→快感に素直を反転。快感の否定をオフ
		CASE 41
			TALENT:ARG:快感に素直 = !TALENT:ARG:快感に素直
			TALENT:ARG:快感の否定 = 0
		;快感の否定→快感の否定を反転。快感に素直をOFF
		CASE 42
			TALENT:ARG:快感の否定 = !TALENT:ARG:快感の否定
			TALENT:ARG:快感に素直 = 0
		;男嫌い→男嫌いを反転。両刀、男好きをOFF
		CASE 43
			TALENT:ARG:男嫌い = !TALENT:ARG:男嫌い
			TALENT:ARG:両刀 = 0
			TALENT:ARG:男好き = 0
		;小悪魔→小悪魔を反転。
		CASE 44
			TALENT:ARG:小悪魔 = !TALENT:ARG:小悪魔
		;女嫌い→女嫌いを反転。両刀、女好きをOFF
		CASE 45
			TALENT:ARG:女嫌い = !TALENT:ARG:女嫌い
			TALENT:ARG:両刀 = 0
			TALENT:ARG:女好き = 0
		;幼稚→幼稚を反転。
		CASE 46
			TALENT:ARG:幼稚 = !TALENT:ARG:幼稚
		;女好き→女好きを反転。女嫌いをOFF
		CASE 47
			TALENT:ARG:女好き = !TALENT:ARG:女好き
			TALENT:ARG:女嫌い = 0
		;男好き→男好きを反転。男嫌いをOFF
		CASE 48
			TALENT:ARG:男好き = !TALENT:ARG:男好き
			TALENT:ARG:男嫌い = 0
		;子供好き→子供好きを反転。
		CASE 49
			TALENT:ARG:子供好き = !TALENT:ARG:子供好き
;-----------------------------------------------------------
		;痛みに弱い→痛みに弱いを反転。痛みに弱いをオフ
		CASE 50
			TALENT:ARG:痛みに強い = 0
			TALENT:ARG:痛みに弱い = !TALENT:ARG:痛みに弱い
		;痛みに強い→痛みに強いを反転。痛みに弱いをオフ
		CASE 51
			TALENT:ARG:痛みに強い = !TALENT:ARG:痛みに強い
			TALENT:ARG:痛みに弱い = 0
		;濡れやすい→濡れやすいを反転。濡れにくいをOFF
		CASE 52
			TALENT:ARG:濡れにくい = 0
			TALENT:ARG:濡れやすい = !TALENT:ARG:濡れやすい
		;濡れにくい→濡れにくいを反転。濡れやすいをOFF
		CASE 53
			TALENT:ARG:濡れにくい = !TALENT:ARG:濡れにくい
			TALENT:ARG:濡れやすい = 0
		;メシマズ→メシマズを反転。料理上手をOFF
		CASE 54
			TALENT:ARG:料理上手 = 0
			TALENT:ARG:メシマズ = !TALENT:ARG:メシマズ
		;薬毒耐性→薬毒耐性を反転。
		CASE 55
			TALENT:ARG:薬毒耐性 = !TALENT:ARG:薬毒耐性
		;おもらし癖→おもらし癖を反転。
		CASE 56
			TALENT:ARG:おもらし癖 = !TALENT:ARG:おもらし癖
		;自慰しやすい→自慰しやすいを反転。
		CASE 57
			TALENT:ARG:自慰しやすい = !TALENT:ARG:自慰しやすい
		;汚臭鈍感→汚臭鈍感を反転。汚臭敏感をOFF
		CASE 58
			TALENT:ARG:汚臭鈍感 = !TALENT:ARG:汚臭鈍感
			TALENT:ARG:汚臭敏感 = 0
		;汚臭敏感→汚臭敏感を反転。汚臭鈍感をOFF
		CASE 59
			TALENT:ARG:汚臭鈍感 = 0
			TALENT:ARG:汚臭敏感 = !TALENT:ARG:汚臭敏感
		;中毒しやすい→中毒しやすいを反転。
		CASE 60
			TALENT:ARG:中毒しやすい = !TALENT:ARG:中毒しやすい
		;倒錯的→倒錯的を反転
		CASE 61
			TALENT:ARG:倒錯的 = !TALENT:ARG:倒錯的
		;両刀→両刀を反転、男嫌い、女嫌いをオフ
		CASE 62
			TALENT:ARG:両刀 = !TALENT:ARG:両刀
			TALENT:ARG:男嫌い = 0
			TALENT:ARG:女嫌い = 0
		;サド→サドを反転。マゾをOFF
		CASE 63
			TALENT:ARG:サド = !TALENT:ARG:サド
			TALENT:ARG:マゾ = 0
		;マゾ→マゾを反転。サドをOFF
		CASE 64
			TALENT:ARG:サド = 0
			TALENT:ARG:マゾ = !TALENT:ARG:マゾ
		;魅力→魅力を反転。
		CASE 65
			TALENT:ARG:魅力 = !TALENT:ARG:魅力
		;魅惑→魅惑を反転。
		CASE 66
			TALENT:ARG:魅惑 = !TALENT:ARG:魅惑
		;謎の魅力→謎の魅力を反転。
		CASE 67
			TALENT:ARG:謎の魅力 = !TALENT:ARG:謎の魅力
		;小柄体型→小柄体型を反転。小人体型、大柄体型をOFF
		CASE 68
			TALENT:ARG:大柄体型 = 0
			TALENT:ARG:小人体型 = 0
			TALENT:ARG:小柄体型 = !TALENT:ARG:小柄体型
		;Ｃ鈍感→Ｃ鈍感を反転。Ｃ敏感をOFFＣ
		CASE 69
			TALENT:ARG:Ｃ敏感 = !TALENT:ARG:Ｃ鈍感
			TALENT:ARG:Ｃ敏感 = 0
		;敏感→Ｃ敏感を反転。Ｃ鈍感をOFF
		CASE 70
			TALENT:ARG:Ｃ鈍感 = 0
			TALENT:ARG:Ｃ敏感 = !TALENT:ARG:Ｃ敏感
		;Ｖ鈍感→オトコの場合は入力エラー。Ｖ鈍感を反転。Ｖ敏感をOFFＶ
		CASE 71
			IF TALENT:ARG:オトコ
				CLEARLINE 1
				REUSELASTLINE オトコですよね？
				GOTO INPUT_LOOP
			ENDIF
			TALENT:ARG:Ｖ鈍感 = !TALENT:ARG:Ｖ鈍感
			TALENT:ARG:Ｖ敏感 = 0
