;-------------------------------------------------
;二輪挿しアナル
;派生系コマンド
;-------------------------------------------------
@COM518

PRINTL 二輪挿しアナル
STR:0 = 二輪挿しアナル

SELECTCOM = 518
CALL KOJO_MESSAGE_COM

;ストッキングびりびりフラグ
IF TEQUIP:特殊２ == 5 || TEQUIP:特殊２ == 6
	TFLAG:55 = 1
ENDIF

;処女喪失フラグ
SIF TALENT:バックバージン
	TFLAG:161 |= 1

;調教者が童貞だった場合の喪失フラグ
SIF TALENT:PLAYER:童貞
	TFLAG:160 |= 2

;調教者が童貞だった場合の喪失フラグ
SIF TALENT:ASSI:童貞
	TFLAG:160 |= 3


;-------------------------------------------------
;射精ゲージチェック（主人）
;-------------------------------------------------
B = 1000

CALL SHOOT_GAUGE_CHECK_3PM

;再装填処理中は射精ゲージは20分の1に
SIF CFLAG:MASTER:9 > 0
	B /= 20

SIF PENIS(PLAYER)
	BASE:MASTER:射精 += B

;-------------------------------------------------
;射精ゲージチェック（助手）
;-------------------------------------------------
B = 1000

CALL SHOOT_GAUGE_CHECK_3PA

;再装填処理中は射精ゲージは20分の1に
SIF CFLAG:ASSI:9 > 0
	B /= 20

SIF PENIS(ASSI)
	BASE:ASSI:射精 += B

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 300
LOSEBASE:気力 += 200

SOURCE:快Ａ += 500

;Ａ拡張経験を見る
IF EXP:Ａ拡張経験 >= 50
	TIMES SOURCE:快Ａ , 2.00
ELSEIF EXP:Ａ拡張経験 > 40
	TIMES SOURCE:快Ａ , 1.80
ELSEIF EXP:Ａ拡張経験 > 30
	TIMES SOURCE:快Ａ , 1.50
ELSEIF EXP:Ａ拡張経験 > 20
	TIMES SOURCE:快Ａ , 1.20
ENDIF

SOURCE:情愛 += 300
;スキンシップ好きだと情愛のソース追加
SIF TALENT:スキンシップ好き
	SOURCE:情愛 += 50

SOURCE:性行動 += 250
SOURCE:痛み += 1200
SOURCE:中毒充足 += 500
SOURCE:露出 += 500
SOURCE:屈従 += 500
SOURCE:逸脱 += 800
SOURCE:反発 += 500

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 500

;貞操観念持ちで恋慕、親愛持ち以外には常に反発のソースを追加する（セックス系のみ）
SIF TALENT:貞操観念 && (TALENT:恋慕 == 0 && TALENT:親愛 == 0)
	SOURCE:反発 += 1000

;Aへの大きな苦痛は先に処理しておく
;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES SOURCE:痛み , 3.00
	TIMES SOURCE:反発 , 4.00
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:痛み , 2.50
	TIMES SOURCE:反発 , 3.50
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES SOURCE:痛み , 2.40
	TIMES SOURCE:反発 , 3.00
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES SOURCE:痛み , 2.00
	TIMES SOURCE:反発 , 2.50
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES SOURCE:痛み , 1.80
	TIMES SOURCE:反発 , 2.00
ELSEIF EXP:Ａ経験 >= EXPLV:5
	TIMES SOURCE:痛み , 1.60
	TIMES SOURCE:反発 , 1.50
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:痛み , 3.00
	TIMES SOURCE:反発 , 2.00
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:痛み , 2.50
	TIMES SOURCE:反発 , 1.50
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:痛み , 2.00
	TIMES SOURCE:反発 , 1.20
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:痛み , 1.50
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:痛み , 1.00
ENDIF


;セックス中毒を見る
IF ABL:セックス中毒 == 1
	TIMES SOURCE:中毒充足 , 0.00
ELSEIF ABL:セックス中毒 == 2
	TIMES SOURCE:中毒充足 , 0.30
ELSEIF ABL:セックス中毒 == 3
	TIMES SOURCE:中毒充足 , 0.60
ELSEIF ABL:セックス中毒 == 4
	TIMES SOURCE:中毒充足 , 0.90
ELSEIF ABL:セックス中毒 == 5
	TIMES SOURCE:中毒充足 , 1.20
ELSEIF ABL:セックス中毒 == 6
	TIMES SOURCE:中毒充足 , 1.50
ELSEIF ABL:セックス中毒 == 7
	TIMES SOURCE:中毒充足 , 1.80
ELSEIF ABL:セックス中毒 == 8
	TIMES SOURCE:中毒充足 , 2.30
ELSEIF ABL:セックス中毒 == 9
	TIMES SOURCE:中毒充足 , 3.00
ELSEIF ABL:セックス中毒 >= 10
	TIMES SOURCE:中毒充足 , 5.00
ENDIF

;-------------------------------------------------
;射精チェック１（主人）
;-------------------------------------------------
CALL SAMEN_CHECK(MASTER,1)

;射精時の処理
IF E >= 1
	;射精先タイプの設定
	TFLAG:703 = 2
	;射精先選択の可・不可
	TFLAG:704 = 1
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;射精チェック２（助手）
;-------------------------------------------------
CALL SAMEN_CHECK(ASSI)

;射精時の処理
IF E >= 1
	;射精先タイプの設定
	TFLAG:703 = 2
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷のＡと腸内⇔調教者のＣの汚れが移動
;ペニスバンドを使う場合、汚れは移動しない
IF PENIS(PLAYER)
	STAIN:腸内 |= STAIN:MASTER:ペニス
	STAIN:MASTER:ペニス |= STAIN:腸内
	STAIN:アナル |= STAIN:MASTER:ペニス
	STAIN:MASTER:ペニス |= STAIN:アナル
ENDIF

;奴隷のＡ⇔助手のペニスの汚れが移動
;ペニスバンドを使う場合、汚れは移動しない
IF PENIS(ASSI)
	STAIN:アナル |= STAIN:ASSI:ペニス
	STAIN:ASSI:ペニス |= STAIN:アナル
	STAIN:腸内 |= STAIN:ASSI:ペニス
	STAIN:ASSI:ペニス |= STAIN:腸内
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;Ａ経験
EXP:Ａ経験 += 10
PRINTL Ａ経験＋１０

;Ａ拡張経験
IF EXP:Ａ拡張経験 == 0
	EXP:異常経験 += 1
	PRINTL Ａ拡張初経験で異常経験＋１
ENDIF
EXP:Ａ拡張経験 += 2
PRINTL Ａ拡張経験＋2

;性交経験
EXP:MASTER:性交経験 += 1
EXP:ASSI:性交経験 += 1
EXP:性交経験 += 2
PRINTS EXPNAME:4
PRINTL +2

;挿入経験
EXP:MASTER:挿入経験 += 1
EXP:ASSI:挿入経験 += 1

;レズ経験
A = 5
B = NO:MASTER + 1099
C = NO:ASSI + 1099
D = 8

IF MALE(ASSI) == 0 && MALE(MASTER) == 0 && MALE(ASSI) == 0
	PRINTFORML %EXPNAME:40%+{D}
	EXP:レズ経験 += D
ELSEIF MALE(ASSI) == 0 && MALE(MASTER) == 0
	PRINTFORML %EXPNAME:40%+{A}
	EXP:レズ経験 += A
ELSEIF MALE(ASSI) == 0 && MALE(ASSI) == 0
	PRINTFORML %EXPNAME:40%+{A}
	EXP:レズ経験 += A
ELSEIF MALE(TARGET) && MALE(MASTER) && MALE(ASSI)
	PRINTFORML %EXPNAME:41%+{D}
	EXP:ゲイ経験 += D
ELSEIF MALE(TARGET) && MALE(MASTER)
	PRINTFORML %EXPNAME:41%+{A}
	EXP:ゲイ経験 += A
ELSEIF MALE(TARGET) && MALE(ASSI)
	PRINTFORML %EXPNAME:41%+{A}
	EXP:ゲイ経験 += A
ENDIF
;調教者を記録
CFLAG:B += A
CFLAG:C += A

;依存度ベクトル
TFLAG:33 = 1

RETURN 1



