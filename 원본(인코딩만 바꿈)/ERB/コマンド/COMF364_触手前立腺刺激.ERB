;-------------------------------------------------
;触手前立腺刺激
;触手系コマンド
;-------------------------------------------------
@COM364

PRINTL 触手前立腺刺激
STR:0 = 触手前立腺刺激

CALL KOJO_MESSAGE_COM

C = 1

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 50
LOSEBASE:気力 += 100

IF EXP:触手経験 < EXPLV:1
	TIMES LOSEBASE:体力 , 1.10
	TIMES LOSEBASE:気力 , 2.00
ELSEIF EXP:触手経験 < EXPLV:2
	TIMES LOSEBASE:体力 , 0.90
	TIMES LOSEBASE:気力 , 1.80
ELSEIF EXP:触手経験 < EXPLV:3
	TIMES LOSEBASE:体力 , 0.80
	TIMES LOSEBASE:気力 , 1.50
ELSEIF EXP:触手経験 < EXPLV:4
	TIMES LOSEBASE:体力 , 0.70
	TIMES LOSEBASE:気力 , 1.20
ELSEIF EXP:触手経験 < EXPLV:5
	TIMES LOSEBASE:体力 , 0.60
	TIMES LOSEBASE:気力 , 1.00
ELSE
	TIMES LOSEBASE:体力 , 0.50
	TIMES LOSEBASE:気力 , 0.50
ENDIF

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:快Ａ += 150
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:快Ａ += 200
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:快Ａ += 600
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:快Ａ += 2000
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:快Ａ += 2600
ELSE
	SOURCE:快Ａ += 3400
ENDIF

;EXP:Ａ経験をみる
;バックバージンはありえない
IF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:快Ａ , 0.60
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES SOURCE:快Ａ , 1.00
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES SOURCE:快Ａ , 1.20
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES SOURCE:快Ａ , 1.30
ELSE
	TIMES SOURCE:快Ａ , 1.80
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.10
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ａ , 0.40
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ａ , 1.00
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ａ , 1.40
ELSE
	TIMES SOURCE:快Ａ , 1.80
ENDIF

SOURCE:中毒充足 += 200
SOURCE:液体 += 500
SOURCE:逸脱 += 2000

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:屈従 += 100
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:屈従 += 180
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:屈従 += 250
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:屈従 += 380
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:屈従 += 500
ELSE
	SOURCE:屈従 += 800
ENDIF

;EXP:Ａ経験をみる
;バックバージンはありえない
IF EXP:Ａ経験 < EXPLV:2
	SOURCE:痛み += 100
ELSEIF EXP:Ａ経験 < EXPLV:3
	SOURCE:痛み += 10
ELSEIF EXP:Ａ経験 < EXPLV:4
	SOURCE:痛み += 0
ELSEIF EXP:Ａ経験 < EXPLV:5
	SOURCE:痛み += 0
ELSE
	SOURCE:痛み += 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	SOURCE:痛み += 1000
	TIMES SOURCE:痛み , 3.00
ELSEIF PALAM:潤滑 < PALAMLV:2
	SOURCE:痛み += 300
	TIMES SOURCE:痛み , 1.00
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:痛み , 0.50
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:痛み , 0.20
ELSE
	TIMES SOURCE:痛み , 0.10
ENDIF

;小人体形
SIF TALENT:小人体型
	TIMES SOURCE:痛み , 2.00

;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 500

;貞操観念
IF TALENT:貞操観念
	TIMES SOURCE:屈従 , 0.60
	SOURCE:反発 += 1000
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:中毒充足 , 0.90
	TIMES SOURCE:液体 , 0.90
	TIMES SOURCE:反発 , 1.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:中毒充足 , 1.00
	TIMES SOURCE:液体 , 1.00
	TIMES SOURCE:反発 , 1.50
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:中毒充足 , 1.10
	TIMES SOURCE:液体 , 1.10
	TIMES SOURCE:反発 , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:中毒充足 , 1.20
	TIMES SOURCE:液体 , 1.20
	TIMES SOURCE:反発 , 0.60
ELSE
	TIMES SOURCE:中毒充足 , 1.30
	TIMES SOURCE:液体 , 1.30
	TIMES SOURCE:反発 , 0.30
ENDIF

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:Ａ鈍感
	TIMES SOURCE:痛み , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TALENT:Ａ敏感
	TIMES SOURCE:痛み , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

SOURCE:性行動 += B/2
SOURCE:不潔 += A*150
SOURCE:恐れ += B/4

IF TEQUIP:アナルバイブ == 2
	C += 1
ELSEIF TEQUIP:アナルバイブ == 3
	C += 2
	TEQUIP:アナルバイブ = 2
ELSE
	TEQUIP:アナルバイブ = 2
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;触手挿入なら、触手体液がアナルに付く
IF TEQUIP:触手
	STAIN:アナル |= 32
	STAIN:腸内 |= 32
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ａ経験 += C
PRINTFORML Ａ経験＋{C}
T = 1

;触手装着中は依存度ベクトル強制変更
;依存度ベクトル
TFLAG:33 = 5

;依存度補正値(服従で1、隷属で2　強制変更なので、これまでの数値をリセット）
IF TALENT:服従 && ASSIPLAY == 0
	TFLAG:30 = 2
ELSEIF TALENT:隷属 && ASSIPLAY == 0
	TFLAG:30 = 4
ENDIF

RETURN 1



;--------------------------------------------------
;触手前立腺責め責め
;--------------------------------------------------
@EQUIP_COM364
PRINTL ＜触手前立腺責め責め中＞

LOSEBASE:体力 += 30
LOSEBASE:気力 += 50

IF TEQUIP:アナルバイブ == 3
	A = 25
ELSEIF TEQUIP:アナルバイブ == 2
	A = 20
ELSE
	A = 12
ENDIF

;EXP:触手経験をみる
IF EXP:触手経験 < EXPLV:1
	TIMES A , 2.00
	D = 0
ELSEIF EXP:触手経験 < EXPLV:2
	TIMES A , 1.50
	D = 25
ELSEIF EXP:触手経験 < EXPLV:3
	TIMES A , 1.20
	D = 60
ELSEIF EXP:触手経験 < EXPLV:4
	TIMES A , 1.00
	D = 180
ELSEIF EXP:触手経験 < EXPLV:5
	TIMES A , 0.80
	D = 450
ELSE
	TIMES A , 0.60
	D = 1000
ENDIF

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	A = 40
ELSEIF ABL:Ａ感覚 == 1
	A = 120
ELSEIF ABL:Ａ感覚 == 2
	A = 300
ELSEIF ABL:Ａ感覚 == 3
	A = 500
ELSEIF ABL:Ａ感覚 == 4
	A = 650
ELSE
	A = 850
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:2
	TIMES A , 0.60
	B = 150
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES A , 1.00
	B = 20
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES A , 1.20
	B = 0
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES A , 1.40
	B = 0
ELSE
	TIMES A , 1.60
	B = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES A , 0.10
	B += 400
	TIMES B , 3.00
	C += 1000
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES A , 0.40
	B += 150
	TIMES B , 1.00
	C += 200
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES A , 1.00
	TIMES B , 0.50
	C += 80
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES A , 1.40
	TIMES B , 0.20
ELSE
	TIMES A , 1.80
	TIMES B , 0.10
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES A , 1.10
ELSE
	TIMES A , 1.20
ENDIF

;ABL:従順をみる
IF ABL:従順 == 0
	TIMES A , 0.80
ELSEIF ABL:従順 == 1
	TIMES A , 0.90
ELSEIF ABL:従順 == 2
	TIMES A , 1.00
ELSEIF ABL:従順 == 3
	TIMES A , 1.10
ELSEIF ABL:従順 == 4
	TIMES A , 1.20
ELSE
	TIMES A , 1.30
ENDIF

;小人体型
SIF TALENT:小人体型
	TIMES B , 2.00

;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 500

SOURCE:快Ａ += A/2
SOURCE:性行動 += A/5
SOURCE:痛み += B
SOURCE:不潔 += D*50
SOURCE:逸脱 += C/2
SOURCE:反発 += C

SIF FLAG:6
	PRINTFORML 触手が腸壁を撫でまわしているのか、ときおり%CALLNAME%の下腹部がぽこっと動いている…
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ａ経験 += 2
PRINTL 
PRINTFORML %EXPNAME:1%＋２
T += 2

RETURN 1


