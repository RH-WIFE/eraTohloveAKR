;-------------------------------------------------
;足コキする
;奉仕する系コマンド
;-------------------------------------------------
@COM242

IF TEQUIP:野外プレイ || TEQUIP:地下室
	PRINTL 靴コキする
	STR:0 = 靴コキする
ELSE
	PRINTL 足コキする
	STR:0 = 足コキする
ENDIF

CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 10
LOSEBASE:気力 += 50

SOURCE:快Ｃ += 80

IF TEQUIP:野外プレイ || TEQUIP:地下室
	SOURCE:痛み += 1500
ELSEIF ABL:PLAYER:サドっ気 >= 3
	SOURCE:痛み += 200
ENDIF

SIF TEQUIP:野外プレイ || TEQUIP:地下室
	SOURCE:恐れ += 350

SOURCE:達成感 += 200
SOURCE:露出 += 300
SOURCE:屈従 += 300
SOURCE:逸脱 += 350
SOURCE:反発 += 350


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 400

;苦痛を伴わないマゾっ気及びマゾの処理は先にしておく
;マゾっ気を見る
IF ABL:マゾっ気 == 0
	TIMES SOURCE:達成感 , 0.80
	TIMES SOURCE:屈従 , 0.80
	SOURCE:液体 += 0
	SOURCE:欲情 += 0
ELSEIF ABL:マゾっ気 == 1
	TIMES SOURCE:達成感 , 1.00
	TIMES SOURCE:屈従 , 1.00
	SOURCE:液体 += 20
	SOURCE:欲情 += 30
ELSEIF ABL:マゾっ気 == 2
	TIMES SOURCE:達成感 , 1.20
	TIMES SOURCE:屈従 , 1.20
	SOURCE:液体 += 40
	SOURCE:欲情 += 70
ELSEIF ABL:マゾっ気 == 3
	TIMES SOURCE:達成感 , 1.40
	TIMES SOURCE:屈従 , 1.40
	SOURCE:液体 += 70
	SOURCE:欲情 += 120
ELSEIF ABL:マゾっ気 == 4
	TIMES SOURCE:達成感 , 1.50
	TIMES SOURCE:屈従 , 1.50
	SOURCE:液体 += 110
	SOURCE:欲情 += 180
ELSEIF ABL:マゾっ気 == 5
	TIMES SOURCE:達成感 , 1.70
	TIMES SOURCE:屈従 , 1.70
	SOURCE:液体 += 150
	SOURCE:欲情 += 250
ELSEIF ABL:マゾっ気 == 6
	TIMES SOURCE:達成感 , 1.90
	TIMES SOURCE:屈従 , 1.90
	SOURCE:液体 += 200
	SOURCE:欲情 += 300
ELSEIF ABL:マゾっ気 == 7
	TIMES SOURCE:達成感 , 2.10
	TIMES SOURCE:屈従 , 2.10
	SOURCE:液体 += 250
	SOURCE:欲情 += 350
ELSEIF ABL:マゾっ気 == 8
	TIMES SOURCE:達成感 , 2.30
	TIMES SOURCE:屈従 , 2.30
	SOURCE:液体 += 300
	SOURCE:欲情 += 400
ELSEIF ABL:マゾっ気 == 9
	TIMES SOURCE:達成感 , 2.50
	TIMES SOURCE:屈従 , 2.50
	SOURCE:液体 += 350
	SOURCE:欲情 += 450
ELSEIF ABL:マゾっ気 >= 10
	TIMES SOURCE:達成感 , 3.00
	TIMES SOURCE:屈従 , 3.00
	SOURCE:液体 += 500
	SOURCE:欲情 += 500
ENDIF

;マゾを見る
IF TALENT:マゾ
	TIMES SOURCE:達成感 , 2.50
	TIMES SOURCE:屈従 , 2.50
	TIMES SOURCE:液体 , 1.20
	TIMES SOURCE:欲情 , 1.20
ENDIF

;倒錯的
SIF TALENT:倒錯的
	TIMES SOURCE:達成感 , 1.80


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;ゲイ経験とか加算
CALL COM_EXP(4)

;依存度ベクトル
P = PLAYER
IF RELATION:P > 100
	TFLAG:33 = 2
ELSEIF RELATION:P < 100
	TFLAG:33 = 3
ELSE
	TFLAG:33 = 1
ENDIF

;依存度補正値(相性100↑かつ淫乱で1、娼婦で2）
P = PLAYER
IF TALENT:淫乱 && RELATION:P > 100
	TFLAG:30 += 1
ELSEIF TALENT:娼婦 && RELATION:P > 100
	TFLAG:30 += 2
ENDIF

;調教者がサド持ちならパラメータに応じた嗜虐快楽経験と苦痛快楽経験も入れる（地の文から判断）
IF TALENT:PLAYER:サド || TEQUIP:地下室
	E = 1
	L = 0
	;マゾっ気で苦痛快楽経験
	IF ABL:PLAYER:9 <= 3
		E += 0
		L += 0
	ELSEIF ABL:PLAYER:9 >= 4
		E += 1
		L += 2
	ENDIF
	;調教者のサドっ気に応じて嗜虐快楽経験
	IF ABL:PLAYER:10 <= 4
		E += 0
		L += 0
	ELSEIF ABL:PLAYER:10 >= 5
		E += 2
		L += 1
	ENDIF
	IF L > 0
		PRINT 苦痛快楽経験+
		PRINTVL L
		EXP:苦痛快楽経験 += L
		TFLAG:27 = L
	ENDIF
	EXP:PLAYER:嗜虐快楽経験 += E
	PRINTFORM %CALLNAME:PLAYER%の%EXPNAME:32%+
	PRINTVL E
ENDIF


RETURN 1

