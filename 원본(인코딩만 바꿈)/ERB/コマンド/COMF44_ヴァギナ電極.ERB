;-------------------------------------------------
;ヴァギナ電極
;Ｖ系調教コマンド
;-------------------------------------------------
@COM44

PRINTL ヴァギナ電極
STR:0 = ヴァギナ電極
CALL KOJO_MESSAGE_COM


;ストッキングびりびりフラグ
IF TEQUIP:特殊２ == 5 || TEQUIP:特殊２ == 6
	TFLAG:55 = 1
ENDIF


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
SOURCE:快Ｖ += 500

IF TEQUIP:地下室 || TFLAG:56 == 3
	;消費体力、気力
	LOSEBASE:体力 += 300
	LOSEBASE:気力 += 450
	
	SOURCE:痛み += 5000
	SOURCE:恐れ += 3000
	SOURCE:屈従 += 1600
ELSEIF TFLAG:56 == 2
	;消費体力、気力
	LOSEBASE:体力 += 200
	LOSEBASE:気力 += 350
	
	SOURCE:痛み += 3500
	SOURCE:恐れ += 1500
	SOURCE:屈従 += 1200
ELSE
	;消費体力、気力
	LOSEBASE:体力 += 100
	LOSEBASE:気力 += 250
	
	SOURCE:痛み += 1500
	SOURCE:恐れ += 500
	SOURCE:屈従 += 800
ENDIF

SOURCE:露出 += 300
SOURCE:逸脱 += 1200
SOURCE:反発 += 1200


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 1200

;Vへの苦痛は先に処理しておく
;EXP:Ｖ経験をみる（処女の反感は別途処理済みなので省略）
IF EXP:Ｖ経験 < EXPLV:1
	TIMES SOURCE:痛み , 2.50
ELSEIF EXP:Ｖ経験 < EXPLV:2
	TIMES SOURCE:痛み , 0.80
	TIMES SOURCE:反発 , 1.20
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES SOURCE:痛み , 0.40
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES SOURCE:痛み , 0.10
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES SOURCE:痛み , 0.00
ELSEIF EXP:Ｖ経験 >= EXPLV:5
	TIMES SOURCE:痛み , 0.00
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:痛み , 1.20
	TIMES SOURCE:反発 , 1.50
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:痛み , 0.70
	TIMES SOURCE:反発 , 1.20
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:痛み , 0.40
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:痛み , 0.10
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:痛み , 0.10
ENDIF



;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;V経験
EXP:Ｖ経験 += 2
PRINTL Ｖ経験＋２

;電極初体験のとき、異常経験追加
IF EXP:電極経験 == 0
	EXP:異常経験 += 1
	PRINTL 電極初経験で異常経験＋１
ENDIF
EXP:電極経験 += 1

;ゲイ経験とか加算
CALL COM_EXP(1)

;依存度ベクトル
TFLAG:33 = 5

;依存度補正値(服従で1、隷属で2）
IF TALENT:服従 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:隷属 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1


