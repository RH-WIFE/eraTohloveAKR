;--------------------------------------------------------
;レズホモ調教XX
;--------------------------------------------------------
L = 0
S = 0

;レズ調教
IF (!MALE(PLAYER) && !MALE(TARGET)) || (MALE(PLAYER) && MALE(TARGET))
	IF !MALE(PLAYER) && !MALE(TARGET)
		;ABL:レズっ気
		IF ABL:レズっ気 > 3
			L += ABL:レズっ気
		ELSEIF ABL:レズっ気 < 3
			S += ABL:レズっ気
		ENDIF
		;ABL:レズ中毒
		SIF ABL:レズ中毒
			L += ABL:レズ中毒 * 3
	ELSE
		;ABL:ホモっ気
		IF ABL:ホモっ気 > 3
			L += ABL:ホモっ気
		ELSEIF ABL:ホモっ気 < 3
			S += ABL:ホモっ気
		ENDIF
		;ABL:ゲイ中毒
		SIF ABL:ゲイ中毒
			L += ABL:ゲイ中毒*3
	ENDIF
	
	;TALENT:両刀
	SIF TALENT:両刀
		L += 10
	;好奇心
	SIF TALENT:好奇心
		L += 7
	;保守的
	SIF TALENT:保守的
		S += 13
	
	IF L
		PRINTFORM  + 
		A += L
		IF !MALE(PLAYER) && !MALE(TARGET)
			PRINTFORM レズっ気有り({L})
		ELSE
			PRINTFORM ホモっ気有り({L})
		ENDIF
	ELSEIF S
		PRINTFORM  - 
		A -= S
		IF !MALE(PLAYER) && !MALE(TARGET)
			PRINTFORM レズっ気無し({S})
		ELSE
			PRINTFORM ホモっ気無し({S})
		ENDIF
	ENDIF
ELSE
	SIF ABL:レズ中毒
		S += ABL:レズ中毒*2
	SIF ABL:ゲイ中毒
		S += ABL:ゲイ中毒*2
	
	;好奇心
	SIF TALENT:好奇心
		L += 5
	;保守的
	SIF TALENT:保守的
		S += 10
	
	IF L
		A += L
		PRINTFORM  + 
		PRINTFORM 好奇心({L})
	ELSEIF S
		A -= S
		PRINTFORM  - 
		IF ABL:レズ中毒
			PRINTFORM レズ中毒({S})
		ELSEIF ABL:ゲイ中毒
			PRINTFORM ゲイ中毒({S})
		ELSEIF TALENT:保守的
			PRINTFORM 保守的({S})
		ENDIF
	ENDIF
ENDIF


