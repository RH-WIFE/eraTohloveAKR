;-------------------------------------------------
;触手貫通
;触手-拷問系コマンド
;-------------------------------------------------
@COM371

PRINTL 触手貫通
STR:0 = 触手貫通

CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 800
LOSEBASE:気力 += 800

IF EXP:触手経験 < EXPLV:1
	TIMES LOSEBASE:体力 , 1.10
	TIMES LOSEBASE:気力 , 2.00
ELSEIF EXP:触手経験 < EXPLV:2
	TIMES LOSEBASE:体力 , 0.90
	TIMES LOSEBASE:気力 , 1.80
ELSEIF EXP:触手経験 < EXPLV:3
	TIMES LOSEBASE:体力 , 0.80
	TIMES LOSEBASE:気力 , 1.50
ELSEIF EXP:触手経験 < EXPLV:4
	TIMES LOSEBASE:体力 , 0.70
	TIMES LOSEBASE:気力 , 1.20
ELSEIF EXP:触手経験 < EXPLV:5
	TIMES LOSEBASE:体力 , 0.60
	TIMES LOSEBASE:気力 , 1.00
ELSE
	TIMES LOSEBASE:体力 , 0.50
	TIMES LOSEBASE:気力 , 0.50
ENDIF

IF EXP:貫通経験 < EXPLV:1
	TIMES LOSEBASE:体力 , 1.10
	TIMES LOSEBASE:気力 , 2.00
ELSEIF EXP:貫通経験 < EXPLV:2
	TIMES LOSEBASE:体力 , 0.90
	TIMES LOSEBASE:気力 , 1.80
ELSEIF EXP:貫通経験 < EXPLV:3
	TIMES LOSEBASE:体力 , 0.80
	TIMES LOSEBASE:気力 , 1.50
ELSEIF EXP:貫通経験 < EXPLV:4
	TIMES LOSEBASE:体力 , 0.70
	TIMES LOSEBASE:気力 , 1.20
ELSEIF EXP:貫通経験 < EXPLV:5
	TIMES LOSEBASE:体力 , 0.60
	TIMES LOSEBASE:気力 , 1.00
ELSE
	TIMES LOSEBASE:体力 , 0.50
	TIMES LOSEBASE:気力 , 0.50
ENDIF

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:快Ａ += 50
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:快Ａ += 150
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:快Ａ += 300
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:快Ａ += 600
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:快Ａ += 800
ELSE
	SOURCE:快Ａ += 1000
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.40
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ａ , 0.80
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ａ , 1.00
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ａ , 1.40
ELSE
	TIMES SOURCE:快Ａ , 1.80
ENDIF

SOURCE:痛み += 8000
SOURCE:恐れ += 16500

SOURCE:露出 += 1000
SOURCE:屈従 += 3000
SOURCE:逸脱 += 5600



;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;触手経験を見る
IF EXP:触手経験 < EXPLV:1
	SOURCE:痛み += 1000
ELSEIF EXP:触手経験 < EXPLV:2
	SOURCE:痛み += 800
ELSEIF EXP:触手経験 < EXPLV:3
	SOURCE:痛み += 500
ELSEIF EXP:触手経験 < EXPLV:4
	SOURCE:痛み += 100
ELSEIF EXP:触手経験 < EXPLV:5
	SOURCE:痛み += 50
ENDIF

;貫通経験を見る
IF EXP:貫通経験 < EXPLV:1
	SOURCE:中毒充足 += 1500
	TIMES SOURCE:恐れ , 1.80
ELSEIF EXP:貫通経験 < EXPLV:2
	SOURCE:中毒充足 += 1700
	TIMES SOURCE:恐れ , 1.50
ELSEIF EXP:貫通経験 < EXPLV:3
	SOURCE:中毒充足 += 2000
	TIMES SOURCE:恐れ , 1.20
ELSEIF EXP:貫通経験 < EXPLV:4
	SOURCE:中毒充足 += 2500
ELSEIF EXP:貫通経験 < EXPLV:5
	SOURCE:中毒充足 += 3300
	TIMES SOURCE:恐れ , 0.80
ELSE
	SOURCE:中毒充足 += 4200
	TIMES SOURCE:恐れ , 0.50
ENDIF

;ストレスいっぱい増加
CFLAG:10 += RAND:50

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;汚れ処理
STAIN:アナル |= 32
STAIN:腸内 |= 32
STAIN:口 |= 32


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
IF EXP:貫通経験 == 0
	EXP:異常経験 += 1
	PRINTL 貫通初経験で異常経験＋1
ENDIF

EXP:Ａ経験 += 25
PRINTL Ａ経験＋25

T = 1

;依存度ベクトル
TFLAG:33 = 5

;依存度補正値(服従で1、隷属で2）
IF TALENT:服従 && ASSIPLAY == 0
	TFLAG:30 = 2
ELSEIF TALENT:隷属 && ASSIPLAY == 0
	TFLAG:30 = 4
ENDIF

RETURN 1

