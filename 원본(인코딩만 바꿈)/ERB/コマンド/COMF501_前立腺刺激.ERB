;-------------------------------------------------
;前立腺刺激
;派生系コマンド
;-------------------------------------------------
@COM501

PRINTL 前立腺刺激
STR:0 = 前立腺刺激

SELECTCOM = 501
CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 30
LOSEBASE:気力 += 50

SOURCE:快Ａ += 180

SOURCE:痛み += 60
SOURCE:欲情 += 100
SOURCE:露出 += 80
SOURCE:逸脱 += 100
SOURCE:反発 += 100


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 100

;処女だった場合は、苦痛のソースと反発のソースを追加する
IF TALENT:バックバージン == 1
	SOURCE:痛み += 2000
	SOURCE:反発 += 3000
ENDIF

;Vへの苦痛は先に処理しておく
;EXP:Ａ経験をみる（処女の反感は別途処理済みなので省略）
IF EXP:Ａ経験 < EXPLV:1
	TIMES SOURCE:痛み , 2.50
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:痛み , 0.80
	TIMES SOURCE:反発 , 1.20
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES SOURCE:痛み , 0.40
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES SOURCE:痛み , 0.10
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES SOURCE:痛み , 0.00
ELSEIF EXP:Ａ経験 >= EXPLV:5
	TIMES SOURCE:痛み , 0.00
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:痛み , 1.20
	TIMES SOURCE:反発 , 1.50
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:痛み , 0.70
	TIMES SOURCE:反発 , 1.20
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:痛み , 0.40
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:痛み , 0.10
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:痛み , 0.10
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;調教者の手⇔奴隷のＡ腸内の汚れが移動
;道具を使用する場合は汚れは移動しない
IF TEQUIP:拡張バルーン || TEQUIP:アナルバイブ || TEQUIP:アナルビーズ || TEQUIP:ローターＡ挿入
ELSE
	CALL COM_STAIN
ENDIF


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ａ経験 += 2
PRINTL Ａ経験＋２

;ゲイ経験とか加算
CALL COM_EXP(2)

;依存度ベクトル
TFLAG:33 = 3


RETURN 1
