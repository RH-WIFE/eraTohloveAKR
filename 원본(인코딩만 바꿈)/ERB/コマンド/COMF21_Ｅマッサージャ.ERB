;-------------------------------------------------
;Ｅマッサージャ
;Ｃ系調教コマンド
;-------------------------------------------------
@COM21

PRINTL Ｅマッサージャ
STR:0 = Ｅマッサージャ
CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 10
LOSEBASE:気力 += 50

SOURCE:快Ｃ += 300
;着衣（ズボン系）のとき
SIF BOTTOMS(TARGET)
	TIMES SOURCE:快Ｃ , 0.60
SIF TEQUIP:下着（下） || TEQUIP:特殊２ == 5 || TEQUIP:特殊２ == 6
	TIMES SOURCE:快Ｃ , 0.90

SOURCE:露出 += 40
SOURCE:逸脱 += 60
SOURCE:反発 += 60

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 60


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;ゲイ経験とか加算
CALL COM_EXP(1)

;依存度ベクトル
TFLAG:33 = 1


RETURN 1




