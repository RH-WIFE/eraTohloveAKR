;-------------------------------------------------
;何もしない
;基本系コマンド
;-------------------------------------------------
@COM2

PRINTL 何もしない
STR:0 = 何もしない
CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 -= 10
LOSEBASE:気力 -= 10

SOURCE:露出 += 10
SOURCE:逸脱 += 10


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;苦痛を伴わないマゾっ気及びマゾの伴う処理は先にしておく
;マゾっ気を見る
IF ABL:マゾっ気 == 0
	TIMES SOURCE:露出 , 0.80
	SOURCE:液体 += 0
	SOURCE:欲情 += 0
ELSEIF ABL:マゾっ気 == 1
	TIMES SOURCE:露出 , 1.00
	SOURCE:液体 += 20
	SOURCE:欲情 += 30
ELSEIF ABL:マゾっ気 == 2
	TIMES SOURCE:露出 , 1.20
	SOURCE:液体 += 40
	SOURCE:欲情 += 70
ELSEIF ABL:マゾっ気 == 3
	TIMES SOURCE:露出 , 1.40
	SOURCE:液体 += 70
	SOURCE:欲情 += 120
ELSEIF ABL:マゾっ気 == 4
	TIMES SOURCE:露出 , 1.50
	SOURCE:液体 += 110
	SOURCE:欲情 += 180
ELSEIF ABL:マゾっ気 == 5
	TIMES SOURCE:露出 , 1.70
	SOURCE:液体 += 150
	SOURCE:欲情 += 250
ELSEIF ABL:マゾっ気 == 6
	TIMES SOURCE:露出 , 1.90
	SOURCE:液体 += 200
	SOURCE:欲情 += 300
ELSEIF ABL:マゾっ気 == 7
	TIMES SOURCE:露出 , 2.10
	SOURCE:液体 += 250
	SOURCE:欲情 += 350
ELSEIF ABL:マゾっ気 == 8
	TIMES SOURCE:露出 , 2.30
	SOURCE:液体 += 300
	SOURCE:欲情 += 400
ELSEIF ABL:マゾっ気 == 9
	TIMES SOURCE:露出 , 2.50
	SOURCE:液体 += 350
	SOURCE:欲情 += 450
ELSEIF ABL:マゾっ気 >= 10
	TIMES SOURCE:露出 , 3.00
	SOURCE:液体 += 500
	SOURCE:欲情 += 500
ENDIF

;マゾを見る
IF TALENT:マゾ
	TIMES SOURCE:露出 , 2.50
	TIMES SOURCE:液体 , 1.20
	TIMES SOURCE:欲情 , 1.20
ENDIF


;-------------------------------------------------
;経験の処理
;-------------------------------------------------

;依存度ベクトル
TFLAG:33 = 0

RETURN 1






