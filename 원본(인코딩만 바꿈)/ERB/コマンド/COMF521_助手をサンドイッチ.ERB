;-------------------------------------------------
;助手をサンドイッチ
;派生系コマンド
;-------------------------------------------------
@COM521

PRINTL 助手をサンドイッチ
STR:0 = 助手をサンドイッチ

SELECTCOM = 521
CALL KOJO_MESSAGE_COM

;ストッキングびりびりフラグ
IF TEQUIP:特殊２ == 5 || TEQUIP:特殊２ == 6
	TFLAG:55 = 1
ENDIF


;処女喪失フラグ
SIF (TALENT:ASSI:処女 || TALENT:ASSI:再生処女) && (TFLAG:40 == 11 || TFLAG:49 == 11)
	TFLAG:19 |= 3

;処女喪失フラグ
SIF TALENT:ASSI:バックバージン && (TFLAG:40 == 12 || TFLAG:49 == 12)
	TFLAG:161 |= 3

;調教者が童貞だった場合の喪失フラグ
SIF TALENT:童貞 && (TFLAG:41 == 19 || TFLAG:49 == 12)
	TFLAG:160 |= 1

;調教者が童貞だった場合の喪失フラグ
SIF TALENT:PLAYER:童貞 && (TFLAG:40 == 11 || TFLAG:40 == 12)
	TFLAG:160 |= 2


;-------------------------------------------------
;射精ゲージチェック（主人）
;-------------------------------------------------
B = 1000

CALL SHOOT_GAUGE_CHECK_3PM

;再装填処理中は射精ゲージは20分の1に
SIF CFLAG:MASTER:9 > 0
	B /= 20

SIF PENIS(PLAYER)
	BASE:MASTER:射精 += B

;-------------------------------------------------
;射精ゲージチェック（助手）
;-------------------------------------------------
B = 1000

CALL SHOOT_GAUGE_CHECK_3PA

;再装填処理中は射精ゲージは20分の1に
SIF CFLAG:ASSI:9 > 0
	B /= 20

SIF PENIS(ASSI)
	BASE:ASSI:射精 += B

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
LOSEBASE:体力 += 180
LOSEBASE:気力 += 300

SOURCE:快Ｃ += 350

SOURCE:情愛 += 100
;スキンシップ好きだと情愛のソース追加
SIF TALENT:スキンシップ好き
	SOURCE:情愛 += 50

SOURCE:性行動 += 520
SOURCE:達成感 += 150
SOURCE:中毒充足 += 500
SOURCE:屈従 += 500
SOURCE:逸脱 += 500
SOURCE:反発 += 500

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 500

;貞操観念持ちで恋慕、親愛持ち以外には常に反発のソースを追加する（セックス系のみ）
SIF TALENT:貞操観念 && (TALENT:恋慕 == 0 && TALENT:親愛 == 0)
	SOURCE:反発 += 1000

;馬並み持ちで、調教者が小人体型だった場合、逸脱ソース追加
IF TALENT:ASSI:小人体型 && TALENT:馬並み
	SOURCE:逸脱 += 500
ENDIF

;セックス中毒を見る
IF ABL:セックス中毒 == 1
	TIMES SOURCE:中毒充足 , 0.00
ELSEIF ABL:セックス中毒 == 2
	TIMES SOURCE:中毒充足 , 0.30
ELSEIF ABL:セックス中毒 == 3
	TIMES SOURCE:中毒充足 , 0.60
ELSEIF ABL:セックス中毒 == 4
	TIMES SOURCE:中毒充足 , 0.90
ELSEIF ABL:セックス中毒 == 5
	TIMES SOURCE:中毒充足 , 1.20
ELSEIF ABL:セックス中毒 == 6
	TIMES SOURCE:中毒充足 , 1.50
ELSEIF ABL:セックス中毒 == 7
	TIMES SOURCE:中毒充足 , 1.80
ELSEIF ABL:セックス中毒 == 8
	TIMES SOURCE:中毒充足 , 2.30
ELSEIF ABL:セックス中毒 == 9
	TIMES SOURCE:中毒充足 , 3.00
ELSEIF ABL:セックス中毒 >= 10
	TIMES SOURCE:中毒充足 , 5.00
ENDIF


;-------------------------------------------------
;射精チェック１（主人）
;-------------------------------------------------
CALL SAMEN_CHECK(MASTER,1)

;射精時の処理
IF E >= 1
	;射精先タイプの設定
	IF TFLAG:40 == 11
		TFLAG:703 = 1
	ELSE
		TFLAG:703 = 2
	ENDIF
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;助手を犯すで主人が射精したフラグ
IF TFLAG:40 == 11 || TFLAG:40 == 12
	IF E == 2
		TFLAG:7 = 2
	ELSEIF E == 1
		TFLAG:7 = 1
	ENDIF
ENDIF

;-------------------------------------------------
;射精チェック２（助手）
;-------------------------------------------------
CALL SAMEN_CHECK(ASSI)

;射精時の処理
IF E >= 1
	;射精先タイプの設定
	IF TFLAG:49 == 13
		TFLAG:703 = 4
	ELSE
		TFLAG:703 = 6
	ENDIF
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;汚れの処理
;挿入している部位によって、ソースの数値が変化する
;-------------------------------------------------
IF TFLAG:40 == 11 || TFLAG:49 == 11
	;処女の場合、Ｖと膣内に破瓜の血の汚れを付加
	IF TALENT:ASSI:処女 || TALENT:ASSI:再生処女
		STAIN:ASSI:膣内 |= 64
		STAIN:ASSI:ヴァギナ |= 64
	ENDIF
ENDIF

;主人が助手のＶに挿入
IF TFLAG:40 == 11 && PENIS(PLAYER)
	STAIN:MASTER:ペニス |= STAIN:ASSI:ヴァギナ
	STAIN:ASSI:ヴァギナ |= STAIN:MASTER:ペニス
	STAIN:MASTER:ペニス |= STAIN:ASSI:膣内
	STAIN:ASSI:膣内 |= STAIN:MASTER:ペニス
ENDIF

;主人が助手のＡに挿入
IF TFLAG:40 == 12 && PENIS(PLAYER)
	STAIN:MASTER:ペニス |= STAIN:ASSI:アナル
	STAIN:ASSI:アナル |= STAIN:MASTER:ペニス
	STAIN:MASTER:ペニス |= STAIN:ASSI:腸内
	STAIN:ASSI:腸内 |= STAIN:MASTER:ペニス
ENDIF

;奴隷が助手のＶに挿入
IF TFLAG:49 == 11 && PENIS(TARGET)
	STAIN:ペニス |= STAIN:ASSI:ヴァギナ
	STAIN:ASSI:ヴァギナ |= STAIN:ペニス
	STAIN:ペニス |= STAIN:ASSI:膣内
	STAIN:ASSI:膣内 |= STAIN:ペニス
ENDIF

;奴隷が助手のＡに挿入
IF TFLAG:49 == 12 && PENIS(TARGET)
	STAIN:ペニス |= STAIN:ASSI:アナル
	STAIN:ASSI:アナル |= STAIN:ペニス
	STAIN:ペニス |= STAIN:ASSI:腸内
	STAIN:ASSI:腸内 |= STAIN:ペニス
ENDIF

;奴隷→助手Ｃ
IF TFLAG:49 == 13
	STAIN:口 |= STAIN:ASSI:ペニス
	STAIN:ASSI:ペニス = 2
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;助手がＶに挿入されてる
IF TFLAG:4 == 11 || TFLAG:4 == 11
	;調教者が馬並みで奴隷が小人体型だった場合、Ｖ拡張経験
	IF TALENT:ASSI:小人体型 && TALENT:MASTER:馬並み && TFLAG:40 == 11
		IF EXP:ASSI:Ｖ拡張経験 == 0
			EXP:ASSI:異常経験 += 1
			PRINTFORML Ｖ拡張初経験で異常経験＋１
		ENDIF
		EXP:ASSI:Ｖ拡張経験 += 1
		PRINTFORML Ｖ拡張経験＋１
	ENDIF
	IF TALENT:ASSI:小人体型 && TALENT:馬並み && TFLAG:49 == 11
		IF EXP:ASSI:Ｖ拡張経験 == 0
			EXP:ASSI:異常経験 += 1
			PRINTFORML Ｖ拡張初経験で異常経験＋１
		ENDIF
		EXP:ASSI:Ｖ拡張経験 += 1
		PRINTFORML Ｖ拡張経験＋１
	ENDIF
	EXP:ASSI:Ｖ経験 += 1
	PRINTFORML %CALLNAME:ASSI%のＶ経験＋１
ENDIF

;助手がＡに挿入されてる
IF TFLAG:4 == 12 || TFLAG:4 == 12
	;調教者が馬並みで奴隷が小人体型だった場合、Ａ拡張経験
	IF TALENT:ASSI:小人体型 && TALENT:MASTER:馬並み && TFLAG:40 == 12
		IF EXP:ASSI:Ａ拡張経験 == 0
			EXP:ASSI:異常経験 += 1
			PRINTFORML Ａ拡張初経験で異常経験＋１
		ENDIF
		EXP:ASSI:Ａ拡張経験 += 1
		PRINTFORML Ａ拡張経験＋１
	ENDIF
	IF TALENT:ASSI:小人体型 && TALENT:馬並み && TFLAG:49 == 12
		IF EXP:ASSI:Ａ拡張経験 == 0
			EXP:ASSI:異常経験 += 1
			PRINTFORML Ａ拡張初経験で異常経験＋１
		ENDIF
		EXP:ASSI:Ａ拡張経験 += 1
		PRINTFORML Ａ拡張経験＋１
	ENDIF
	EXP:ASSI:Ａ経験 += 1
	PRINTFORML %CALLNAME:ASSI%のＡ経験＋１
ENDIF

;挿入してるか、されてれば性交経験
IF TFLAG:49 == 11 || TFLAG:49 == 12
	EXP:性交経験 += 1
	PRINTFORML %CALLNAME%の性交経験＋1
	
	EXP:挿入経験 += 1
ENDIF
IF TFLAG:40 == 11 || TFLAG:40 == 12 || TFLAG:49 == 11 || TFLAG:49 == 12
	EXP:ASSI:性交経験 += 1
	PRINTFORML %CALLNAME:ASSI%の性交経験＋1
ENDIF
IF TFLAG:40 == 11 || TFLAG:40 == 12
	EXP:MASTER:性交経験 += 1
	PRINTFORML %CALLNAME:PLAYER%の性交経験＋1
	
	EXP:MASTER:挿入経験 += 1
ENDIF

;奉仕系
IF TFLAG:49 == 13
	;奉仕快楽フラグ
	TFLAG:100 = 1
	
	;フェラ経験
	IF TFLAG:49 == 8 || TFLAG:49 == 10 || TFLAG:49 == 13
		EXP:フェラ経験 += 2
		PRINTFORML %EXPNAME:22%＋２
	ENDIF

	;奴隷が猫舌持ちの場合は、奴隷に嗜虐快楽経験を追加
	IF TALENT:TARGET:猫舌
		EXP:嗜虐快楽経験 += 1
		PRINTFORML %EXPNAME:32%＋１
	ENDIF
ENDIF

;ゲイ経験とか加算
CALL COM_EXP(7)

;依存度ベクトル
TFLAG:33 = 1


RETURN 1


