;-------------------------------------------------
;胸揉み
;Ｂ系調教コマンド
;-------------------------------------------------
@COM11

;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
;奴隷が母乳体質の場合、搾乳に飛ぶ
IF TALENT:母乳体質
	CALL COM_ABLE502
	SIF RESULT == 1
		JUMP COM502
ENDIF

PRINTL 胸揉み
STR:0 = 胸揉み
CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 5
LOSEBASE:気力 += 20

SOURCE:快Ｂ += 50
;着衣プレイ
SIF TEQUIP:下着（上） || TOPS(TARGET)
	TIMES SOURCE:快Ｂ , 0.70

SOURCE:情愛 += 50
;スキンシップ好きだと情愛のソース追加
SIF TALENT:スキンシップ好き
	SOURCE:情愛 += 50
;幼児／幼児退行もしくは小人体型だと情愛のソース追加
SIF TALENT:幼児／幼児退行 || TALENT:小人体型
	TIMES SOURCE:情愛 , 1.20

;地下室にいると苦痛のソース追加
SIF TEQUIP:地下室
	SOURCE:痛み += 200

SOURCE:反発 += 10


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 10



;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;ゲイ経験とか加算
CALL COM_EXP(5)

;依存度ベクトル
TFLAG:33 = 2


RETURN 1



