;-------------------------------------------------
;胸愛撫命令
;助手命令系コマンド
;-------------------------------------------------
@COM402

PRINTL 胸愛撫命令
STR:0 = 胸愛撫命令
CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 5
LOSEBASE:気力 += 50

SOURCE:快Ｂ += 80
SIF ABL:ASSI:技巧
	SOURCE:快Ｂ += 10*ABL:ASSI:技巧

SOURCE:露出 += 20
SOURCE:逸脱 += 30
SOURCE:反発 += 30


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 30
;助手の技巧により快Ｂソース追加、下手すぎると痛みのソース追加
IF ABL:ASSI:2 == 0
	SOURCE:痛み += 100
ELSEIF ABL:ASSI:2 == 1
	SOURCE:痛み += 50
ENDIF
;助手が小人体型で、調教対象が小人体型でなく倒錯的でもないと逸脱のソース追加
SIF TALENT:小人体型 == 0 && TALENT:ASSI:小人体型 && TALENT:倒錯的
	SOURCE:逸脱 += 50


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の胸⇔助手の手の汚れが移動
STAIN:5 |= STAIN:ASSI:1
STAIN:ASSI:1 |= STAIN:5


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;ゲイ経験とか加算
CALL COM_EXP(3)

;依存度ベクトル
TFLAG:33 = 1


RETURN 1


