;-------------------------------------------------
;触手アナル産卵
;触手系コマンド
;-------------------------------------------------
@COM370

PRINTL 触手アナル産卵
STR:0 = 触手アナル産卵

;着脱の処理
IF TEQUIP:触手腸内産卵 == 0
	TFLAG:56 = 2
	TEQUIP:アナルバイブ = 1
	TEQUIP:触手腸内産卵 += 1
ELSE
	PRINTL どうしますか？
	;Ａ拡張経験が少ないと卵は追加不可能
	IF EXP:Ａ拡張経験 >= 500 && TEQUIP:触手腸内産卵 < 6
		PRINTL [0] さらに卵を産ませる
	ELSEIF EXP:Ａ拡張経験 >= 200 && TEQUIP:触手腸内産卵 < 5
		PRINTL [0] さらに卵を産ませる
	ELSEIF EXP:Ａ拡張経験 >= 50 && TEQUIP:触手腸内産卵 < 4
		PRINTL [0] さらに卵を産ませる
	ELSEIF EXP:Ａ拡張経験 >= 20 && TEQUIP:触手腸内産卵 < 3
		PRINTL [0] さらに卵を産ませる
	ELSEIF EXP:Ａ拡張経験 >= 4 && TEQUIP:触手腸内産卵 < 2
		PRINTL [0] さらに卵を産ませる
	ENDIF
	IF TEQUIP:触手腸内産卵 && TEQUIP:触手膣内産卵
		PRINTL [1] アナル栓だけ抜いてやる
		PRINTL [2] 両方の栓を抜いてやる
	ELSE
		PRINTL [1] 抜いてやる
	ENDIF
	$INPUT_LOOP_enema
	INPUT
	;さらに卵を産ませる
	IF RESULT == 0
		TFLAG:56 = 3
		TEQUIP:触手腸内産卵 += 1
	;片方だけ抜いてやる
	ELSEIF RESULT == 1
		IF TEQUIP:触手腸内産卵 && TEQUIP:触手膣内産卵
			TFLAG:56 = 1
		ELSE
			TFLAG:56 = 0
		ENDIF
	;両方の栓を抜いてやる
	ELSEIF RESULT == 2
		TFLAG:56 = 0
	ELSE
		GOTO INPUT_LOOP_enema
	ENDIF
ENDIF

CALL KOJO_MESSAGE_COM

;消費体力、気力
LOSEBASE:体力 += 100
LOSEBASE:気力 += 150

IF EXP:触手経験 < EXPLV:1
	TIMES LOSEBASE:体力 , 1.10
	TIMES LOSEBASE:気力 , 2.00
ELSEIF EXP:触手経験 < EXPLV:2
	TIMES LOSEBASE:体力 , 0.90
	TIMES LOSEBASE:気力 , 1.80
ELSEIF EXP:触手経験 < EXPLV:3
	TIMES LOSEBASE:体力 , 0.80
	TIMES LOSEBASE:気力 , 1.50
ELSEIF EXP:触手経験 < EXPLV:4
	TIMES LOSEBASE:体力 , 0.70
	TIMES LOSEBASE:気力 , 1.20
ELSEIF EXP:触手経験 < EXPLV:5
	TIMES LOSEBASE:体力 , 0.60
	TIMES LOSEBASE:気力 , 1.00
ELSE
	TIMES LOSEBASE:体力 , 0.50
	TIMES LOSEBASE:気力 , 0.50
ENDIF

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:快Ａ += 80
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:快Ａ += 250
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:快Ａ += 600
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:快Ａ += 1000
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:快Ａ += 1300
ELSE
	SOURCE:快Ａ += 1700
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.40
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ａ , 0.80
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ａ , 1.00
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ａ , 1.40
ELSE
	TIMES SOURCE:快Ａ , 1.80
ENDIF

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:屈従 += 300
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:屈従 += 800
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:屈従 += 1400
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:屈従 += 1800
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:屈従 += 2100
ELSE
	SOURCE:屈従 += 2400
ENDIF

;ABL:マゾっ気をみる
IF ABL:マゾっ気 < 1
	SOURCE:痛み += 2000
	SOURCE:不潔 += 1000
	SOURCE:屈従 += 200
	SOURCE:逸脱 += 1000
	SOURCE:反発 += 2000
ELSEIF ABL:マゾっ気 <2
	SOURCE:痛み += 1600
	SOURCE:不潔 += 2000
	SOURCE:屈従 += 500
	SOURCE:逸脱 += 1000
	SOURCE:反発 += 1000
ELSEIF ABL:マゾっ気 < 3
	SOURCE:痛み += 1200
	SOURCE:不潔 += 1000
	SOURCE:屈従 += 800
	SOURCE:逸脱 += 1000
	SOURCE:反発 += 500
ELSEIF ABL:マゾっ気 < 4
	SOURCE:痛み += 800
	SOURCE:不潔 += 1000
	SOURCE:屈従 += 1200
	SOURCE:逸脱 += 1000
	SOURCE:反発 += 100
ELSEIF ABL:マゾっ気 < 5
	SOURCE:痛み += 600
	SOURCE:不潔 += 1000
	SOURCE:屈従 += 1500
	SOURCE:逸脱 += 1000
	SOURCE:反発 += 0
ELSE
	SOURCE:痛み += 400
	SOURCE:不潔 += 1000
	SOURCE:屈従 += 2000
	SOURCE:逸脱 += 1000
	SOURCE:反発 += 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	SOURCE:痛み += 800
ELSEIF PALAM:潤滑 < PALAMLV:2
	SOURCE:痛み += 500
ELSEIF PALAM:潤滑 < PALAMLV:3
	SOURCE:痛み += 300
ELSEIF PALAM:潤滑 < PALAMLV:4
	SOURCE:痛み += 120
ELSE
	SOURCE:痛み += 100
ENDIF

;小人体形
SIF TALENT:小人体型
	TIMES SOURCE:痛み , 2.00

;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 500

;貞操観念持ちで恋慕、親愛持ち以外には常に反発のソースを追加する（セックス系のみ）
SIF TALENT:貞操観念 && (TALENT:恋慕 == 0 && TALENT:親愛 == 0)
	SOURCE:反発 += 1000

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:Ａ鈍感
	TIMES SOURCE:痛み , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TALENT:Ａ敏感
	TIMES SOURCE:痛み , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

SOURCE:恐れ += SOURCE:屈従


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
STAIN:腸内 |= 32


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ａ経験 += 15
PRINTL Ａ経験＋15
T = 1


RETURN 1


;--------------------------------------------------
;触手腸内産卵中
;--------------------------------------------------
@EQUIP_COM370
PRINTL ＜触手腸内産卵中＞
CALL EQUIP_MESSAGE_COM308

LOSEBASE:体力 += 30
LOSEBASE:気力 += 50

A = 120
B = 100

;触手経験
IF EXP:触手経験 < EXPLV:1
	TIMES A , 1.00
	TIMES B , 1.40
ELSEIF EXP:触手経験 < EXPLV:2
	TIMES A , 0.90
	TIMES B , 1.20
ELSEIF EXP:触手経験 < EXPLV:3
	TIMES A , 0.80
	TIMES B , 1.00
ELSEIF EXP:触手経験 < EXPLV:4
	TIMES A , 0.70
	TIMES B , 0.80
ELSEIF EXP:触手経験 < EXPLV:5
	TIMES A , 0.60
	TIMES B , 0.60
ELSE
	TIMES A , 0.50
	TIMES B , 0.40
ENDIF

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	A = 80
	B = 300
ELSEIF ABL:Ａ感覚 == 1
	A = 250
	B = 800
ELSEIF ABL:Ａ感覚 == 2
	A = 600
	B = 1400
ELSEIF ABL:Ａ感覚 == 3
	A = 1000
	B = 1800
ELSEIF ABL:Ａ感覚 == 4
	A = 1300
	B = 2100
ELSE
	A = 1700
	B = 2400
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES A , 0.50
	C = 2000
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES A , 1.00
	C = 300
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES A , 1.10
	C = 50
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES A , 1.20
	C = 10
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES A , 1.40
	C = 0
ELSE
	TIMES A , 1.60
	C = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES A , 0.40
	C += 800
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES A , 0.80
	C += 500
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES A , 1.00
	C += 300
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES A , 1.40
	C += 120
ELSE
	TIMES A , 1.80
	C += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES A , 1.10
ELSE
	TIMES A , 1.20
ENDIF

;ABL:従順をみる
IF ABL:従順 == 0
	TIMES A , 0.80
ELSEIF ABL:従順 == 1
	TIMES A , 0.90
ELSEIF ABL:従順 == 2
	TIMES A , 1.00
ELSEIF ABL:従順 == 3
	TIMES A , 1.10
ELSEIF ABL:従順 == 4
	TIMES A , 1.20
ELSE
	TIMES A , 1.30
ENDIF

;小人体形
SIF TALENT:小人体型
	TIMES C , 2.00

;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 500

;貞操観念持ちで恋慕、親愛持ち以外には常に反発のソースを追加する（セックス系のみ）
SIF TALENT:貞操観念 && (TALENT:恋慕 == 0 && TALENT:親愛 == 0)
	SOURCE:反発 += 1000

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:Ａ鈍感
	TIMES SOURCE:痛み , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TALENT:Ａ敏感
	TIMES SOURCE:痛み , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

SOURCE:快Ａ += A
SOURCE:屈従 += B
SOURCE:痛み += C
SOURCE:逸脱 += B*2
SOURCE:恐れ += B

EXP:Ａ経験 += 3
PRINTL Ａ経験＋3
SIF TEQUIP:触手
	T += 1

;触手装着中は依存度ベクトル強制変更
;依存度ベクトル
TFLAG:33 = 5

;依存度補正値(服従で1、隷属で2　強制変更なので、これまでの数値をリセット）
IF TALENT:服従 && ASSIPLAY == 0
	TFLAG:30 = 2
ELSEIF TALENT:隷属 && ASSIPLAY == 0
	TFLAG:30 = 4
ENDIF


;--------------------------------------------------
;装着時メッセージ
;--------------------------------------------------
@EQUIP_MESSAGE_COM308
SIF FLAG:6 == 0
	RETURN 0

PRINTL 
PRINTFORML %CALLNAME%は菊門から触手の卵を押し込まれ、栓をされた状態で責め続けられている
PRINTL 
IF ABL:マゾっ気 == 0
	PRINTFORMW %CALLNAME%は腸内の異物感に顔を真っ青にしている…
ELSEIF ABL:マゾっ気 == 1
	PRINTFORM %CALLNAME%は脂汗を浮かべながらも異物感を誤魔化す為か、しきりに
	IF TEQUIP:縄
		PRINTW 腰を左右に振っている
	ELSE
		PRINTW 尻部を撫でている
	ENDIF
ELSEIF ABL:マゾっ気 == 2
	PRINTFORMW %CALLNAME%は苦しそうな表情をしながらも、顔を紅潮させもじもじとお尻を振っている
ELSEIF ABL:マゾっ気 == 3
	PRINTFORMW %CALLNAME%は排泄感に苛まれながらも、時折恍惚とした表情を浮かべている
ELSEIF ABL:マゾっ気 == 4
	PRINTFORMW %CALLNAME%はうっとりした表情を浮かべ、時々卵が腸内の壁に当たっている刺激を愉しんでいる
ELSE
	PRINTFORML %CALLNAME%は排泄感を心ゆくまで味わっているようだ
	PRINTFORMW %CALLNAME%は背徳感を湛えた笑みを浮かべ、何かを期待するような眼で%CALLNAME:MASTER%を見上げている
ENDIF

