;-------------------------------------------------
;授乳手コキ
;派生系コマンド
;-------------------------------------------------
@COM528

PRINTL 授乳手コキ
STR:0 = 授乳手コキ
SELECTCOM = 528

;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;実行判定操作
CALL KOJO_DENIAL_PLUS

;合計を表示(14以上で実行)
PRINT  = 
PRINTV A

V = 14
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

;実行不可口上/実行判定操作
SIF A < V
	CALL KOJO_DENIAL

WAIT

;実行できない
SIF A < V
	RETURN 0

CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
B = 650

CALL SHOOT_GAUGE_CHECK_MT

;再装填処理中は射精ゲージは20分の1に
SIF CFLAG:PLAYER:9 > 0
	B /= 20

SIF PENIS(PLAYER)
	BASE:PLAYER:射精 += B

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 25
LOSEBASE:気力 += 80

SOURCE:快Ｂ += 100

SOURCE:情愛 += 500

;スキンシップ好きだと情愛のソース追加
SIF TALENT:スキンシップ好き
	SOURCE:情愛 += 50

;母性持ちだと情愛のソース追加
SIF TALENT:母性
	TIMES SOURCE:情愛 , 2.00

SOURCE:達成感 += 500
SOURCE:恭順 += 1000
SOURCE:屈従 += 1000
SOURCE:逸脱 += 100
SOURCE:反発 += 100

;上のほうで計算した汚れデータ
SOURCE:不潔 += Y*10 + 60


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN

;調教者の口←奴隷の体へ汚れが移動
STAIN:PLAYER:口 |= STAIN:胸
STAIN:胸 = 0

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 600

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK(PLAYER,1)

;射精時の処理
IF E >= 1
	;射精先タイプの設定
	TFLAG:703 = 3
	;射精先選択の可・不可
	TFLAG:704 = 1
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;ゲイ経験とか加算
CALL COM_EXP(5)

;愛情経験
IF (TALENT:恋慕 == 1 || TALENT:親愛 == 1) && ASSIPLAY == 0
	EXP:愛情経験 += 1
	PRINTL 愛情経験+１
ENDIF

;依存度ベクトル
TFLAG:33 = 4

;依存度補正値(主人かつ恋慕で1、親愛で2）
IF TALENT:恋慕 && ASSIPLAY == 0
	TFLAG:30 += 2
ELSEIF TALENT:親愛 && ASSIPLAY == 0
	TFLAG:30 += 4
ENDIF

TFLAG:100 = 1


RETURN 1



