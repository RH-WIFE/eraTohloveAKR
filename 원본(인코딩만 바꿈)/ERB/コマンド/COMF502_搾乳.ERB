;-------------------------------------------------
;搾乳
;派生系コマンド
;-------------------------------------------------
@COM502

PRINTL 搾乳
STR:0 = 搾乳

SELECTCOM = 502
CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 30
LOSEBASE:気力 += 50

SOURCE:快Ｂ += 200

SOURCE:情愛 += 50
;スキンシップ好きだと情愛のソース追加
SIF TALENT:スキンシップ好き
	SOURCE:情愛 += 50

;幼児／幼児退行もしくは小人体型だと情愛のソース追加
SIF TALENT:幼児／幼児退行 || TALENT:小人体型
	TIMES SOURCE:情愛 , 1.20
;母性持ちだと情愛のソース追加
SIF TALENT:母性
	TIMES SOURCE:情愛 , 2.00

;地下室にいると苦痛のソース追加
SIF TEQUIP:地下室
	SOURCE:痛み += 150

SOURCE:露出 += 100
SOURCE:不潔 += 50
SOURCE:反発 += 100


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 100

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;ゲイ経験とか加算
CALL COM_EXP(5)

;依存度ベクトル
TFLAG:33 = 1


RETURN 1



