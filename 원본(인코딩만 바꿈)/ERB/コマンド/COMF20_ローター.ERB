;-------------------------------------------------
;ローター
;Ｃ系調教コマンド
;-------------------------------------------------
@COM20

IF PENIS(TARGET)
	PRINTL ペニスローター
	STR:0 = ペニスローター
ELSE
	PRINTL クリトリスローター
	STR:0 = クリトリスローター
ENDIF

CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 5
LOSEBASE:気力 += 30

SOURCE:快Ｃ += 150
;着衣（ズボン系）のとき
SIF BOTTOMS(TARGET) >= 4
	TIMES SOURCE:快Ｃ , 0.60
SIF TEQUIP:下着（下） || TEQUIP:特殊２ == 5 || TEQUIP:特殊２ == 6
	TIMES SOURCE:快Ｃ , 0.90

SOURCE:露出 += 20
SOURCE:逸脱 += 30
SOURCE:反発 += 30


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 30


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;ゲイ経験とか加算
CALL COM_EXP(1)

;依存度ベクトル
TFLAG:33 = 1


RETURN 1




