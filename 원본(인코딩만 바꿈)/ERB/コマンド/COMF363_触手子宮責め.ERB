;-------------------------------------------------
;触手子宮責め
;触手系コマンド
;-------------------------------------------------
@COM363

PRINTL 触手子宮責め
STR:0 = 触手子宮責め

CALL KOJO_MESSAGE_COM

C = 2

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 75
LOSEBASE:気力 += 150

IF EXP:触手経験 < EXPLV:1
	TIMES LOSEBASE:体力 , 1.10
	TIMES LOSEBASE:気力 , 2.00
ELSEIF EXP:触手経験 < EXPLV:2
	TIMES LOSEBASE:体力 , 0.90
	TIMES LOSEBASE:気力 , 1.80
ELSEIF EXP:触手経験 < EXPLV:3
	TIMES LOSEBASE:体力 , 0.80
	TIMES LOSEBASE:気力 , 1.50
ELSEIF EXP:触手経験 < EXPLV:4
	TIMES LOSEBASE:体力 , 0.70
	TIMES LOSEBASE:気力 , 1.20
ELSEIF EXP:触手経験 < EXPLV:5
	TIMES LOSEBASE:体力 , 0.60
	TIMES LOSEBASE:気力 , 1.00
ELSE
	TIMES LOSEBASE:体力 , 0.50
	TIMES LOSEBASE:気力 , 0.50
ENDIF



;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:快Ｖ += 10
	SOURCE:屈従 += 20
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:快Ｖ += 150
	SOURCE:屈従 += 120
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:快Ｖ += 600
	SOURCE:屈従 += 500
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:快Ｖ += 1800
	SOURCE:屈従 += 1200
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:快Ｖ += 2400
	SOURCE:屈従 += 1800
ELSE
	SOURCE:快Ｖ += 3200
	SOURCE:屈従 += 2400
ENDIF

;EXP:Ｖ経験をみる
IF EXP:Ｖ経験 < EXPLV:1
	TIMES SOURCE:快Ｖ , 0.20
ELSEIF EXP:Ｖ経験 < EXPLV:2
	TIMES SOURCE:快Ｖ , 0.50
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES SOURCE:快Ｖ , 1.00
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES SOURCE:快Ｖ , 1.20
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES SOURCE:快Ｖ , 1.60
ELSE
	TIMES SOURCE:快Ｖ , 1.80
ENDIF

;PALAM:潤滑をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:快Ｖ , 0.10
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:快Ｖ , 0.20
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:快Ｖ , 0.60
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:快Ｖ , 1.00
ELSE
	TIMES SOURCE:快Ｖ , 2.00
ENDIF


SOURCE:中毒充足 += 200
SOURCE:液体 += 500
SOURCE:逸脱 += 2000
SOURCE:反発 += 500

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:屈従 += 20
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:屈従 += 120
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:屈従 += 500
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:屈従 += 1200
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:屈従 += 1800
ELSE
	SOURCE:屈従 += 2400
ENDIF

;EXP:Ｖ経験をみる
IF EXP:Ｖ経験 < EXPLV:1
	TIMES SOURCE:屈従, 0.20
	SOURCE:痛み += 300
ELSEIF EXP:Ｖ経験 < EXPLV:2
	TIMES SOURCE:屈従, 0.50
	SOURCE:痛み += 160
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES SOURCE:屈従, 0.80
	SOURCE:痛み += 100
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES SOURCE:屈従, 1.00
	SOURCE:痛み += 60
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES SOURCE:屈従, 1.20
	SOURCE:痛み += 20
ELSE
	TIMES SOURCE:屈従, 1.50
	SOURCE:痛み += 0
ENDIF

;小人体形
SIF TALENT:小人体型
	TIMES SOURCE:痛み , 2.00

;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 500

;貞操観念持ちで恋慕、親愛持ち以外には常に反発のソースを追加する（セックス系のみ）
SIF TALENT:貞操観念 && (TALENT:恋慕 == 0 && TALENT:親愛 == 0)
	SOURCE:反発 += 1000

;貞操観念
IF TALENT:貞操観念
	IF TALENT:処女 == 1
		TIMES SOURCE:屈従 , 0.60
		TIMES SOURCE:逸脱, 5.00
		SOURCE:反発 += 2000
	ELSE
		TIMES SOURCE:屈従 , 0.60
		SOURCE:反発 += 1000
	ENDIF
;貞操無頓着
ELSEIF TALENT:貞操無頓着
	IF TALENT:処女 == 1
		TIMES SOURCE:屈従 , 0.60
		SOURCE:反発 += 300
	ENDIF
ELSE
	SIF TALENT:処女 == 1
		SOURCE:反発 += 3000
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:中毒充足 , 0.90
	TIMES SOURCE:液体 , 0.90
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:中毒充足 , 1.00
	TIMES SOURCE:液体 , 1.00
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:中毒充足 , 1.10
	TIMES SOURCE:液体 , 1.10
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:中毒充足 , 1.20
	TIMES SOURCE:液体 , 1.20
ELSE
	TIMES SOURCE:中毒充足 , 1.30
	TIMES SOURCE:液体 , 1.30
ENDIF

;Ｖ敏感、鈍感をみる
;快Ｖ自体のチェックは後でまとめてやる
IF TALENT:Ｖ鈍感
	TIMES SOURCE:痛み , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TALENT:Ｖ敏感
	TIMES SOURCE:痛み , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

SOURCE:性行動 += B/2
SOURCE:不潔 += A*150
SOURCE:恐れ += B/4

IF TEQUIP:バイブ == 2
	C += 2
ELSEIF TEQUIP:バイブ == 3
	C += 3
ENDIF
TEQUIP:バイブ = 3


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;触手挿入なら、触手体液が膣内に付く
IF TEQUIP:触手
	STAIN:ヴァギナ |= 32
	STAIN:膣内 |= 32
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ｖ経験 += C
EXP:Ｖ拡張経験 += 1
PRINTL 
PRINTFORML %EXPNAME:0%＋{C}
PRINTFORML %EXPNAME:52%＋1

;触手装着中は依存度ベクトル強制変更
;依存度ベクトル
TFLAG:33 = 5

;依存度補正値(服従で1、隷属で2　強制変更なので、これまでの数値をリセット）
IF TALENT:服従 && ASSIPLAY == 0
	TFLAG:30 = 2
ELSEIF TALENT:隷属 && ASSIPLAY == 0
	TFLAG:30 = 4
ENDIF

RETURN 1



;--------------------------------------------------
;触手挿入中(子宮責め)
;--------------------------------------------------
@EQUIP_COM363
PRINTL ＜触手子宮責め中＞

LOSEBASE:体力 += 30
LOSEBASE:気力 += 50

IF TEQUIP:バイブ == 3
	A = 35
ELSEIF TEQUIP:バイブ == 2
	A = 25
ELSE
	A = 15
ENDIF

;EXP:触手経験をみる
IF EXP:触手経験 < EXPLV:1
	TIMES A , 2.00
	D = 0
ELSEIF EXP:触手経験 < EXPLV:1
	TIMES A , 1.50
	D = 25
ELSEIF EXP:触手経験 < EXPLV:2
	TIMES A , 1.20
	D = 60
ELSEIF EXP:触手経験 < EXPLV:3
	TIMES A , 1.00
	D = 180
ELSEIF EXP:触手経験 < EXPLV:4
	TIMES A , 0.80
	D = 450
ELSE
	TIMES A , 0.60
	D = 1000
ENDIF

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	A = 100
ELSEIF ABL:Ｖ感覚 == 1
	A = 200
ELSEIF ABL:Ｖ感覚 == 2
	A = 400
ELSEIF ABL:Ｖ感覚 == 3
	A = 600
ELSEIF ABL:Ｖ感覚 == 4
	A = 800
ELSE
	A = 1000
ENDIF

;EXP:Ｖ経験をみる
;処女はありえない
IF EXP:Ｖ経験 < EXPLV:2
	TIMES A , 0.60
	B = 150
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES A , 1.00
	B = 20
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES A , 1.20
	B = 0
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES A , 1.40
	B = 0
ELSE
	TIMES A , 1.60
	B = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES A , 0.10
	B += 400
	TIMES B , 3.00
	C += 1000
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES A , 0.40
	B += 150
	TIMES B , 1.00
	C += 200
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES A , 1.00
	TIMES B , 0.50
	C += 80
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES A , 1.40
	TIMES B , 0.20
ELSE
	TIMES A , 1.80
	TIMES B , 0.10
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES A , 1.10
ELSE
	TIMES A , 1.20
ENDIF

;ABL:従順をみる
IF ABL:従順 == 0
	TIMES A , 0.80
ELSEIF ABL:従順 == 1
	TIMES A , 0.90
ELSEIF ABL:従順 == 2
	TIMES A , 1.00
ELSEIF ABL:従順 == 3
	TIMES A , 1.10
ELSEIF ABL:従順 == 4
	TIMES A , 1.20
ELSE
	TIMES A , 1.30
ENDIF

;小人体型
SIF TALENT:小人体型
	TIMES B , 2.00

;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 500

;貞操観念持ちで恋慕、親愛持ち以外には常に反発のソースを追加する（セックス系のみ）
SIF TALENT:貞操観念 && (TALENT:恋慕 == 0 && TALENT:親愛 == 0)
	SOURCE:反発 += 1000

;貞操観念(処女はありえない)
SIF TALENT:貞操観念
	TIMES B, 3.00

SOURCE:快Ｖ += A/2
SOURCE:中毒充足 += A/4
SOURCE:痛み += B*3
SOURCE:不潔 += D*50
SOURCE:逸脱 += C
SOURCE:反発 += C

SIF FLAG:6
	PRINTFORML %CALLNAME%の腹部は触手に満たされ、大きく膨らんでいる…
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ｖ経験 += 3
PRINTL 
PRINTFORML %EXPNAME:0%＋3
T += 2

RETURN 1


