;-------------------------------------------------
;アナル舐め
;Ａ系調教コマンド
;-------------------------------------------------
@COM50

;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
;便意が高いと食糞するに派生
IF BASE:便意 >= 90
	CALL COM_ABLE286
	SIF RESULT == 1
		JUMP COM286
ENDIF

PRINTL アナル舐め
STR:0 = アナル舐め
CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 5
LOSEBASE:気力 += 30

SOURCE:快Ａ += 30

;調教者のTALENT:舌使いをみる
IF TALENT:PLAYER:舌使い
	TIMES SOURCE:快Ａ , 1.50
ELSEIF TALENT:PLAYER:猫舌
	TIMES SOURCE:快Ａ , 0.75
ENDIF

;着衣のとき
SIF (TEQUIP:下着（下） && TEQUIP:下着（下） != 5) || TEQUIP:特殊２ == 5 || TEQUIP:特殊２ == 6
	TIMES SOURCE:快Ａ , 0.70
SIF EXP:Ａ経験 >= 100 && PALAM:欲情 >= PALAMLV:3
	TIMES SOURCE:快Ａ , 1.20

SOURCE:液体 += 50
SOURCE:逸脱 += 40
SOURCE:反発 += 30


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 50

;スキンシップ好きだと情愛のソース追加
SIF TALENT:スキンシップ好き
	SOURCE:情愛 += 50


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ａ経験 += 1
PRINTL Ａ経験＋１

IF TALENT:PLAYER:猫舌
	EXP:PLAYER:嗜虐快楽経験 += 1
	PRINTFORML %CALLNAME:PLAYER%の嗜虐快楽経験＋１
ENDIF

;ゲイ経験とか加算
CALL COM_EXP(3)

;依存度ベクトル
TFLAG:33 = 3


RETURN 1


