;-------------------------------------------------
;玉を舐める
;Ｐ系調教コマンド
;-------------------------------------------------
@COM35

PRINTL 玉責め
STR:0 = 玉責め
CALL KOJO_MESSAGE_COM

;ストッキングびりびりフラグ
IF TEQUIP:特殊２ == 5 || TEQUIP:特殊２ == 6
	TFLAG:55 = 1
ENDIF

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 100
LOSEBASE:気力 += 250

SOURCE:快Ｃ += 20

IF TEQUIP:三角木馬
	SOURCE:痛み += 1500
ELSE
	SOURCE:痛み += 800
ENDIF

SOURCE:恐れ += 1300
SOURCE:露出 += 30
SOURCE:不潔 += 20
SOURCE:逸脱 += 100
SOURCE:反発 += 600


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 600

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;ゲイ経験とか加算
CALL COM_EXP(2)

;依存度ベクトル
TFLAG:33 = 3

;依存度補正値(服従で1、隷属で2）
IF TALENT:服従 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:隷属 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1

