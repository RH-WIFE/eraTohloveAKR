;-------------------------------------------------
;触手産卵
;触手系コマンド
;-------------------------------------------------
@COM369

PRINTL 触手産卵
STR:0 = 触手産卵

;着脱の処理
IF TEQUIP:触手膣内産卵 == 0
	TFLAG:56 = 2
	TEQUIP:バイブ = 1
	TEQUIP:触手膣内産卵 += 1
ELSE
	PRINTL どうしますか？
	;Ｖ拡張経験が少ないと卵は追加不可能
	IF EXP:Ｖ拡張経験 >= 500 && TEQUIP:触手膣内産卵 < 6
		PRINTL [0] さらに卵を産ませる
	ELSEIF EXP:Ｖ拡張経験 >= 200 && TEQUIP:触手膣内産卵 < 5
		PRINTL [0] さらに卵を産ませる
	ELSEIF EXP:Ｖ拡張経験 >= 50 && TEQUIP:触手膣内産卵 < 4
		PRINTL [0] さらに卵を産ませる
	ELSEIF EXP:Ｖ拡張経験 >= 20 && TEQUIP:触手膣内産卵 < 3
		PRINTL [0] さらに卵を産ませる
	ELSEIF EXP:Ｖ拡張経験 >= 4 && TEQUIP:触手膣内産卵 < 2
		PRINTL [0] さらに卵を産ませる
	ENDIF
	IF TEQUIP:触手腸内産卵 && TEQUIP:触手膣内産卵
		PRINTL [1] 膣栓だけ抜いてやる
		PRINTL [2] 両方の栓を抜いてやる
	ELSE
		PRINTL [1] 抜いてやる
	ENDIF
	$INPUT_LOOP_enema
	INPUT
	;さらに卵を産ませる
	IF RESULT == 0
		TFLAG:56 = 3
		TEQUIP:触手膣内産卵 += 1
	;片方だけ抜いてやる
	ELSEIF RESULT == 1
		IF TEQUIP:触手腸内産卵 && TEQUIP:触手膣内産卵
			TFLAG:56 = 1
		ELSE
			TFLAG:56 = 0
		ENDIF
	;両方の栓を抜いてやる
	ELSEIF RESULT == 2
		TFLAG:56 = 0
	ELSE
		GOTO INPUT_LOOP_enema
	ENDIF
ENDIF

CALL KOJO_MESSAGE_COM


;消費体力、気力
LOSEBASE:体力 += 100
LOSEBASE:気力 += 150

IF EXP:触手経験 < EXPLV:1
	TIMES LOSEBASE:体力 , 1.10
	TIMES LOSEBASE:気力 , 2.00
ELSEIF EXP:触手経験 < EXPLV:2
	TIMES LOSEBASE:体力 , 0.90
	TIMES LOSEBASE:気力 , 1.80
ELSEIF EXP:触手経験 < EXPLV:3
	TIMES LOSEBASE:体力 , 0.80
	TIMES LOSEBASE:気力 , 1.50
ELSEIF EXP:触手経験 < EXPLV:4
	TIMES LOSEBASE:体力 , 0.70
	TIMES LOSEBASE:気力 , 1.20
ELSEIF EXP:触手経験 < EXPLV:5
	TIMES LOSEBASE:体力 , 0.60
	TIMES LOSEBASE:気力 , 1.00
ELSE
	TIMES LOSEBASE:体力 , 0.50
	TIMES LOSEBASE:気力 , 0.50
ENDIF

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:快Ｖ += 80
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:快Ｖ += 250
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:快Ｖ += 600
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:快Ｖ += 1000
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:快Ｖ += 1300
ELSE
	SOURCE:快Ｖ += 1700
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ｖ , 0.40
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ｖ , 0.80
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ｖ , 1.00
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ｖ , 1.40
ELSE
	TIMES SOURCE:快Ｖ , 1.80
ENDIF

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:屈従 += 300
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:屈従 += 800
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:屈従 += 1400
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:屈従 += 1800
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:屈従 += 2100
ELSE
	SOURCE:屈従 += 2400
ENDIF

;ABL:マゾっ気をみる
IF ABL:マゾっ気 < 1
	SOURCE:痛み += 2000
	SOURCE:不潔 += 1000
	SOURCE:屈従 += 200
	SOURCE:逸脱 += 1000
	SOURCE:反発 += 2000
ELSEIF ABL:マゾっ気 <2
	SOURCE:痛み += 1600
	SOURCE:不潔 += 2000
	SOURCE:屈従 += 500
	SOURCE:逸脱 += 1000
	SOURCE:反発 += 1000
ELSEIF ABL:マゾっ気 < 3
	SOURCE:痛み += 1200
	SOURCE:不潔 += 1000
	SOURCE:屈従 += 800
	SOURCE:逸脱 += 1000
	SOURCE:反発 += 500
ELSEIF ABL:マゾっ気 < 4
	SOURCE:痛み += 800
	SOURCE:不潔 += 1000
	SOURCE:屈従 += 1200
	SOURCE:逸脱 += 1000
	SOURCE:反発 += 100
ELSEIF ABL:マゾっ気 < 5
	SOURCE:痛み += 600
	SOURCE:不潔 += 1000
	SOURCE:屈従 += 1500
	SOURCE:逸脱 += 1000
	SOURCE:反発 += 0
ELSE
	SOURCE:痛み += 400
	SOURCE:不潔 += 1000
	SOURCE:屈従 += 2000
	SOURCE:逸脱 += 1000
	SOURCE:反発 += 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	SOURCE:痛み += 800
ELSEIF PALAM:潤滑 < PALAMLV:2
	SOURCE:痛み += 500
ELSEIF PALAM:潤滑 < PALAMLV:3
	SOURCE:痛み += 300
ELSEIF PALAM:潤滑 < PALAMLV:4
	SOURCE:痛み += 120
ELSE
	SOURCE:痛み += 100
ENDIF

;小人体形
SIF TALENT:小人体型
	TIMES SOURCE:痛み , 2.00

;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 500

;貞操観念持ちで恋慕、親愛持ち以外には常に反発のソースを追加する（セックス系のみ）
SIF TALENT:貞操観念 && (TALENT:恋慕 == 0 && TALENT:親愛 == 0)
	SOURCE:反発 += 1000

;Ｖ敏感、鈍感をみる
;快Ｖ自体のチェックは後でまとめてやる
IF TALENT:Ｖ鈍感
	TIMES SOURCE:痛み , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TALENT:Ｖ敏感
	TIMES SOURCE:痛み , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

SIF TALENT:処女 == 1 && TALENT:貞操観念
	SOURCE:屈従 /= 3

PRINTL 
PRINTL Ｖ経験＋10
EXP:Ｖ経験 += 10
T = 1


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
STAIN:膣内 |= 32


RETURN 1


;--------------------------------------------------
;触手膣内産卵中
;--------------------------------------------------
@EQUIP_COM369
PRINTL ＜触手膣内産卵中＞
CALL EQUIP_MESSAGE_COM307

LOSEBASE:体力 += 30
LOSEBASE:気力 += 50

A = 110
B = 90

;触手経験
IF EXP:触手経験 < EXPLV:1
	TIMES A , 1.00
	TIMES B , 1.40
ELSEIF EXP:触手経験 < EXPLV:2
	TIMES A , 0.90
	TIMES B , 1.20
ELSEIF EXP:触手経験 < EXPLV:3
	TIMES A , 0.80
	TIMES B , 1.00
ELSEIF EXP:触手経験 < EXPLV:4
	TIMES A , 0.70
	TIMES B , 0.80
ELSEIF EXP:触手経験 < EXPLV:5
	TIMES A , 0.60
	TIMES B , 0.60
ELSE
	TIMES A , 0.50
	TIMES B , 0.40
ENDIF

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	A = 80
	B = 300
ELSEIF ABL:Ｖ感覚 == 1
	A = 250
	B = 800
ELSEIF ABL:Ｖ感覚 == 2
	A = 600
	B = 1400
ELSEIF ABL:Ｖ感覚 == 3
	A = 1000
	B = 1800
ELSEIF ABL:Ｖ感覚 == 4
	A = 1300
	B = 2100
ELSE
	A = 1700
	B = 2400
ENDIF

;EXP:Ｖ経験をみる
IF EXP:Ｖ経験 < EXPLV:1
	TIMES A , 0.50
	C = 2000
ELSEIF EXP:Ｖ経験 < EXPLV:2
	TIMES A , 1.00
	C = 300
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES A , 1.10
	C = 50
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES A , 1.20
	C = 10
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES A , 1.40
	C = 0
ELSE
	TIMES A , 1.60
	C = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES A , 0.40
	C += 800
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES A , 0.80
	C += 500
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES A , 1.00
	C += 300
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES A , 1.40
	C += 120
ELSE
	TIMES A , 1.80
	C += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES A , 1.10
ELSE
	TIMES A , 1.20
ENDIF

;ABL:従順をみる
IF ABL:従順 == 0
	TIMES A , 0.80
ELSEIF ABL:従順 == 1
	TIMES A , 0.90
ELSEIF ABL:従順 == 2
	TIMES A , 1.00
ELSEIF ABL:従順 == 3
	TIMES A , 1.10
ELSEIF ABL:従順 == 4
	TIMES A , 1.20
ELSE
	TIMES A , 1.30
ENDIF

;小人体形
SIF TALENT:小人体型
	TIMES C , 2.00

;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 500

;貞操観念持ちで恋慕、親愛持ち以外には常に反発のソースを追加する（セックス系のみ）
SIF TALENT:貞操観念 && (TALENT:恋慕 == 0 && TALENT:親愛 == 0)
	SOURCE:反発 += 1000

;Ｖ敏感、鈍感をみる
;快Ｖ自体のチェックは後でまとめてやる
IF TALENT:Ｖ鈍感
	TIMES SOURCE:痛み , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TALENT:Ｖ敏感
	TIMES SOURCE:痛み , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

SOURCE:快Ｖ += A
SOURCE:屈従 += B
SOURCE:痛み += C
SOURCE:逸脱 += B*2
SOURCE:恐れ += B

EXP:Ｖ経験 += 3
PRINTL Ｖ経験＋3
SIF TEQUIP:触手
	T += 1
;触手装着中は依存度ベクトル強制変更
;依存度ベクトル
TFLAG:33 = 5

;依存度補正値(服従で1、隷属で2　強制変更なので、これまでの数値をリセット）
IF TALENT:服従 && ASSIPLAY == 0
	TFLAG:30 = 2
ELSEIF TALENT:隷属 && ASSIPLAY == 0
	TFLAG:30 = 4
ENDIF


;--------------------------------------------------
;装着時メッセージ
;--------------------------------------------------
@EQUIP_MESSAGE_COM307
SIF FLAG:6 == 0
	RETURN 0

PRINTL 
PRINTFORML %CALLNAME%は子宮に触手の卵を押し込まれ、栓をされた状態で責められている
PRINTL 
IF ABL:マゾっ気 == 0
	PRINTFORMW %CALLNAME%は激痛のあまり、声も出ないようだ…
ELSEIF ABL:マゾっ気 == 1
	PRINTFORM %CALLNAME%は脂汗を浮かべ
	SIF TEQUIP:縄 == 0
		PRINT 、大きく膨れた腹部に手を添え
	PRINTW ている。
ELSEIF ABL:マゾっ気 == 2
	PRINTFORM %CALLNAME%は苦しそうな表情をしながらも、顔を紅潮させ大きく膨らんだ自分のお腹を
	IF TEQUIP:縄
		PRINTW 見続けている
	ELSE
		PRINTW 擦っている
	ENDIF
ELSEIF ABL:マゾっ気 == 3
	PRINTFORMW %CALLNAME%は排卵感に苛まれながらも、時折恍惚とした表情を浮かべている
ELSEIF ABL:マゾっ気 == 4
	PRINTFORMW %CALLNAME%はうっとりした表情を浮かべ、排卵感と腹痛を愉しんでいる
ELSE
	PRINTFORML 大きく膨らんでいる%CALLNAME%のお腹の中では
	PRINTL 呼吸にに合わせてコロコロと卵が動いているようだ
	PRINTFORMW 卵が子宮の襞を引っかいているのか、時折%CALLNAME%は艶っぽい悲鳴をあげている……
ENDIF

