;-------------------------------------------------
;口辱命令
;助手命令系コマンド
;-------------------------------------------------
@COM407

;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
;前回と今回の調教者が同じ
IF (ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)
	;前回コマンドがフェラか二本フェラ＝「二本フェラ」
	IF PREVCOM == 71 || PREVCOM == 516
		CALL COM_ABLE516
		IF RESULT == 1
			TFLAG:40 = 0
			TFLAG:41 = 0
			TFLAG:49 = 20
			JUMP COM516
		ENDIF
	;前回コマンドが助手を犯すのとき＝「助手をサンドイッチ」
	ELSEIF PREVCOM == 440 || PREVCOM == 441
		CALL COM_ABLE521
		IF RESULT == 1
			IF PREVCOM == 440
				TFLAG:40 = 11
			ELSE
				TFLAG:40 = 12
			ENDIF
			TFLAG:41 = 0
			TFLAG:49 = 13
			JUMP COM521
		ENDIF
	;前回コマンドが奴隷を犯すとき＝「サンドイッチ」
	ELSEIF PREVCOM == 101 || PREVCOM == 103 || PREVCOM == 111 || PREVCOM == 113
		CALL COM_ABLE515
		IF RESULT == 1
			IF PREVCOM == 101 || PREVCOM == 103
				TFLAG:40 = 1
			ELSE
				TFLAG:40 = 1
			ENDIF
			TFLAG:41 = 0
			TFLAG:49 = 13
			JUMP COM515
		ENDIF
	;前回コマンドがサンドイッチか、助手をサンドイッチのとき
	ELSEIF PREVCOM == 515 || PREVCOM == 521
		;主人が奴隷を犯してるときは「サンドイッチ」
		IF TFLAG:40 == 1 || TFLAG:40 == 2
			CALL COM_ABLE515
			IF RESULT == 1
				TFLAG:40 = 0
				TFLAG:49 = 13
				JUMP COM515
			ENDIF
		;主人が助手を犯してるときは「助手をサンドイッチ」
		ELSEIF TFLAG:40 == 11 || TFLAG:40 == 12
			CALL COM_ABLE521
			IF RESULT == 1
				TFLAG:40 = 0
				TFLAG:49 = 13
				JUMP COM521
			ENDIF
		ENDIF
	ENDIF
ENDIF

;何も読まずにこっちに来たときはフラグ解除
TFLAG:40 = 0
TFLAG:41 = 0
TFLAG:49 = 0
TFLAG:42 = 0

PRINTL 口辱命令
STR:0 = 口辱命令

;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;実行判定操作
CALL KOJO_DENIAL_PLUS

;合計を表示(24以上で実行)
PRINT  = 
PRINTV A

V = 24
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

;実行不可口上/実行判定操作
SIF A < V
	CALL KOJO_DENIAL

WAIT

;実行できない
SIF A < V
	RETURN 0

CALL KOJO_MESSAGE_COM


;実行者が奴隷であるフラグ
TFLAG:28 = 1


;-------------------------------------------------
;射精ゲージチェック（助手）
;-------------------------------------------------
B = 1000

CALL SHOOT_GAUGE_CHECK_AT

;再装填処理中は射精ゲージは20分の1に
SIF CFLAG:ASSI:9 > 0
	B /= 20

SIF PENIS(ASSI)
	BASE:ASSI:射精 += B

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 5
LOSEBASE:気力 += 50

SOURCE:性行動 += 300
SOURCE:達成感 += 50
SOURCE:屈従 += 100
SOURCE:不潔 += 60
SOURCE:逸脱 += 60
SOURCE:反発 += 60

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 60


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の口⇔助手のペニスの汚れが移動
STAIN:口 |= STAIN:ASSI:ペニス
STAIN:ASSI:ペニス = 2


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;フェラ経験
PRINTFORML %EXPNAME:22%＋１
EXP:フェラ経験 += 1

;奴隷が猫舌持ちの場合は、奴隷に嗜虐快楽経験を追加
IF TALENT:TARGET:猫舌
	EXP:嗜虐快楽経験 += 1
	PRINTFORML %EXPNAME:32%＋１
ENDIF

;奉仕快楽フラグ
TFLAG:100 = 1

;助手に快Ｃ・習得・恭順・否定の珠
JUEL:ASSI:快Ｃ += 50
JUEL:ASSI:恭順 += 50
JUEL:ASSI:否定 += 50
PRINTFORML %CALLNAME:ASSI%に%PALAMNAME:0%の珠＋50
PRINTFORML %CALLNAME:ASSI%に%PALAMNAME:5%の珠＋50
PRINTFORMW %CALLNAME:ASSI%に%PALAMNAME:100%の珠＋50

;ゲイ経験とか加算
CALL COM_EXP(3)

;依存度ベクトル
TFLAG:33 = 1


RETURN 1


