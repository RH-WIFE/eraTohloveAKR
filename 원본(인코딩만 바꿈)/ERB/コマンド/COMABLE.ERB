@COM_ABLE233
;-------------------------------------------------
;一括管理
SIF GLOBAL_COMABLE(233)
	RETURN RESULT

;主人が母乳体質でないと使えない
SIF TALENT:PLAYER:母乳体質 == 0
	RETURN 0

