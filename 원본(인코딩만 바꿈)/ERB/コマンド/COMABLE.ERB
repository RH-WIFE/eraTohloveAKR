;-------------------------------------------------
;280,イラマチオさせる
;-------------------------------------------------
@COM_ABLE280
;一括管理
SIF GLOBAL_COMABLE(280)
	RETURN RESULT
;着衣プレイ　ズボン、下着（貞操帯含む）はダメ。スカートはまくる。
SIF TEQUIP:下着（下） || BOTTOMS(TARGET) >= 4
	RETURN 0

;調教者が攻めだと駄目
SIF TALENT:PLAYER:攻め
	RETURN 0

;従順3未満のASSIの場合はだめ（マゾならＯＫ）
IF ASSIPLAY
	SIF ABL:ASSI:従順 < 3 && ABL:ASSI:マゾっ気 < 3
		RETURN 0
ENDIF
;奴隷が馬並みで、調教者が小人体型もしくは小柄体型だと不可
IF TALENT:馬並み
	SIF TALENT:PLAYER:小人体型 || TALENT:PLAYER:小柄体型
		RETURN 0
ENDIF

;奴隷にペニスがないとダメ（ペニバン不可）
SIF !PENIS(TARGET)
	RETURN 0

;Ｃ系道具使用中は不可
SIF CCHECK(TARGET)
	RETURN 0

;手足・目・口が拘束されていたらダメ
SIF TEQUIP:縄 || BIND_CHECK_A(1) || BIND_CHECK_A(2)
	RETURN 0

;ペニスが汚れていてる場合、汚れによっては実行できない
IF STAIN:ペニス & 2798 && !TALENT:PLAYER:汚れ無視
	;精液　汚臭敏感だとダメ（精液中毒or精液経験で緩和される）
	SIF STAIN:ペニス & 4 && (!ABL:PLAYER:精液中毒 || EXP:PLAYER:精液経験 < 30) && TALENT:PLAYER:汚臭敏感
		RETURN 0
	;アナル　汚臭敏感だとダメ（スカ経験で緩和される）
	SIF STAIN:ペニス & 8 && TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 15
		RETURN 0
	;粘液　汚臭敏感だとダメ（触手経験で緩和される）
	SIF STAIN:ペニス & 32 && TALENT:PLAYER:汚臭敏感 && (EXP:PLAYER:触手経験 < 50 || EXP:PLAYER:獣姦経験 < 30 || EXP:PLAYER:鰻風呂経験 < 10 || EXP:PLAYER:蟲風呂経験 < 10) < 50
		RETURN 0
	;血　吸血鬼でなく、汚臭敏感だとダメ
	SIF STAIN:ペニス & 64 && !TALENT:PLAYER:吸血鬼 && TALENT:PLAYER:汚臭敏感
		RETURN 0
	;尿　基本ダメ（汚臭鈍感はOK。スカ経験で緩和される）
	SIF STAIN:ペニス & 128 && !TALENT:PLAYER:汚臭鈍感 && ((TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 30) || EXP:PLAYER:スカ経験 < 15)
		RETURN 0
	;便　汚臭鈍感も含め、基本ダメ（スカ経験で緩和される）
	SIF STAIN:ペニス & 512 && ((TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 50) || (!TALENT:PLAYER:汚臭鈍感 && !TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 30) || (TALENT:PLAYER:汚臭鈍感 && EXP:PLAYER:スカ経験 < 15))
		RETURN 0
	;吐瀉物　汚臭鈍感も含め、基本ダメ（スカ経験で緩和される）
	SIF STAIN:ペニス & 2048 && ((TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 50) || (!TALENT:PLAYER:汚臭鈍感 && !TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 30) || (TALENT:PLAYER:汚臭鈍感 && EXP:PLAYER:スカ経験 < 15))
		RETURN 0
ENDIF

;新妻プレイ中/三角木馬乗馬中/地下室/触手調教中はダメ
SIF TEQUIP:エプロン || TEQUIP:三角木馬 || TEQUIP:地下室 || TEQUIP:触手
	RETURN 0
RETURN 1

