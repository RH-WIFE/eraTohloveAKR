	RETURN 0

;地下室/触手調教中はダメ
SIF TEQUIP:地下室 || TEQUIP:触手
	RETURN 0

;胸が汚れていてる場合、汚れによっては実行できない
IF STAIN:PLAYER:胸 & 2798 && !TALENT:汚れ無視
	;精液　汚臭敏感だとダメ（精液中毒or精液経験で緩和される）
	SIF STAIN:PLAYER:胸 & 4 && (!ABL:精液中毒 || EXP:精液経験 < 30) && TALENT:汚臭敏感
		RETURN 0
	;アナル　汚臭敏感だとダメ（スカ経験で緩和される）
	SIF STAIN:PLAYER:胸 & 8 && TALENT:汚臭敏感 && EXP:スカ経験 < 15
		RETURN 0
	;粘液　汚臭敏感だとダメ（触手経験で緩和される）
	SIF STAIN:PLAYER:胸 & 32 && TALENT:汚臭敏感 && (EXP:触手経験 < 50 || EXP:獣姦経験 < 30 || EXP:鰻風呂経験 < 10 || EXP:蟲風呂経験 < 10) < 50
		RETURN 0
	;　吸血鬼でなく、汚臭敏感だとダメ
	SIF STAIN:PLAYER:胸 & 64 && !TALENT:吸血鬼 && TALENT:汚臭敏感
		RETURN 0
	;尿　基本ダメ（汚臭鈍感はOK。スカ経験で緩和される）
	SIF STAIN:PLAYER:胸 & 128 && !TALENT:汚臭鈍感 && ((TALENT:汚臭敏感 && EXP:スカ経験 < 30) || EXP:スカ経験 < 15)
		RETURN 0
	;便　汚臭鈍感も含め、基本ダメ（スカ経験で緩和される）
	SIF STAIN:PLAYER:胸 & 512 && ((TALENT:汚臭敏感 && EXP:スカ経験 < 50) || (!TALENT:汚臭鈍感 && !TALENT:汚臭敏感 && EXP:スカ経験 < 30) || (TALENT:汚臭鈍感 && EXP:スカ経験 < 15))
		RETURN 0
	;吐瀉物　汚臭鈍感も含め、基本ダメ（スカ経験で緩和される）
	SIF STAIN:PLAYER:胸 & 2048 && ((TALENT:汚臭敏感 && EXP:スカ経験 < 50) || (!TALENT:汚臭鈍感 && !TALENT:汚臭敏感 && EXP:スカ経験 < 30) || (TALENT:汚臭鈍感 && EXP:スカ経験 < 15))
		RETURN 0
ENDIF

RETURN 1

