;-------------------------------------------------
;412,ザーメンキス
;-------------------------------------------------
@COM_ABLE412
;一括管理
SIF GLOBAL_COMABLE(412)
	RETURN RESULT
;直前にフェラするで射精させていないとダメ
;CFLAG108
;直前がフェラする、パイズリフェラする、イラマチオさせる、シックスナイン、コンドーム精飲(助手)、コンドーム精飲(主人)でないとそもそもダメ
SIF PREVCOM != 31 && PREVCOM != 241 && PREVCOM != 280 && PREVCOM != 503 && PREVCOM != 411 && PREVCOM != 413
	RETURN 0
;前回と今回の調教者が同じ
IF (ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)
	;直前のコマンドがフェラの場合
	IF PREVCOM == 31 || PREVCOM == 241 || PREVCOM == 280 || PREVCOM == 503
		;直前のフェラするで射精してないとダメ
		SIF TFLAG:10 < 1
			RETURN 0
	ENDIF
	;直前のコマンドがコンドーム精飲(助手)で前回の調教者が主人、またはコンドーム精飲(主人)で前回の調教者が助手ならダメ
	SIF (PREVCOM == 411 && TFLAG:50 == 0) || (PREVCOM == 413 && TFLAG:50)
		RETURN 0
ENDIF

;奴隷の口が使えないとダメ
SIF BIND_CHECK_B(2)
	RETURN 0


;地下室/触手調教中はダメ
SIF TEQUIP:地下室 || TEQUIP:触手 || TEQUIP:三角木馬
	RETURN 0

;口が汚れていてる場合、汚れによっては実行できない
IF STAIN:口 & 2798 && !TALENT:PLAYER:汚れ無視
	;精液　汚臭敏感だとダメ（精液中毒or精液経験で緩和される）
	SIF STAIN:口 & 4 && (!ABL:PLAYER:精液中毒 || EXP:PLAYER:精液経験 < 30) && TALENT:PLAYER:汚臭敏感
		RETURN 0
	;アナル　汚臭敏感だとダメ（スカ経験で緩和される）
	SIF STAIN:口 & 8 && TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 15
		RETURN 0
	;粘液　汚臭敏感だとダメ（触手経験で緩和される）
	SIF STAIN:口 & 32 && TALENT:PLAYER:汚臭敏感 && (EXP:PLAYER:触手経験 < 50 || EXP:PLAYER:獣姦経験 < 30 || EXP:PLAYER:鰻風呂経験 < 10 || EXP:PLAYER:蟲風呂経験 < 10) < 50
		RETURN 0
	;血　吸血鬼でなく、汚臭敏感だとダメ
	SIF STAIN:口 & 64 && !TALENT:PLAYER:吸血鬼 && TALENT:PLAYER:汚臭敏感
		RETURN 0
	;尿　基本ダメ（汚臭鈍感はOK。スカ経験で緩和される）
	SIF STAIN:口 & 128 && !TALENT:PLAYER:汚臭鈍感 && ((TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 30) || EXP:PLAYER:スカ経験 < 15)
		RETURN 0
	;便　汚臭鈍感も含め、基本ダメ（スカ経験で緩和される）
	SIF STAIN:口 & 512 && ((TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 50) || (!TALENT:PLAYER:汚臭鈍感 && !TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 30) || (TALENT:PLAYER:汚臭鈍感 && EXP:PLAYER:スカ経験 < 15))
		RETURN 0
	;吐瀉物　汚臭鈍感も含め、基本ダメ（スカ経験で緩和される）
	SIF STAIN:口 & 2048 && ((TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 50) || (!TALENT:PLAYER:汚臭鈍感 && !TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 30) || (TALENT:PLAYER:汚臭鈍感 && EXP:PLAYER:スカ経験 < 15))
		RETURN 0
ENDIF
RETURN 1

