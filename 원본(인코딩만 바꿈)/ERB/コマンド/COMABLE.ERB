;下着、被り物全て不可。
SIF TEQUIP:下着（上） || TOPS(TARGET)
	RETURN 0

;奴隷が母乳体質でないと使えない
SIF TALENT:母乳体質 == 0
	RETURN 0

;Ｂ系道具使用中は不可
SIF BCHECK(TARGET)
	RETURN 0

;奴隷の手が使えないとダメ
SIF BIND_CHECK_B(5)
	RETURN 0

;調教者にペニスがないとダメ（ペニバン不可）
SIF !PENIS(PLAYER)
	RETURN 0

;貞操帯はダメ
SIF TEQUIP:PLAYER:下着（下） == 5
	RETURN 0

;地下室/触手調教中/三角木馬搭乗中はダメ
SIF TEQUIP:地下室 || TEQUIP:触手 || TEQUIP:三角木馬
	RETURN 0

;胸が汚れていてる場合、汚れによっては実行できない
IF STAIN:胸 & 2798 && !TALENT:PLAYER:汚れ無視
	;精液　汚臭敏感だとダメ（精液中毒or精液経験で緩和される）
	SIF STAIN:胸 & 4 && (!ABL:PLAYER:精液中毒 || EXP:PLAYER:精液経験 < 30) && TALENT:PLAYER:汚臭敏感
		RETURN 0
	;アナル　汚臭敏感だとダメ（スカ経験で緩和される）
	SIF STAIN:胸 & 8 && TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 15
		RETURN 0
	;粘液　汚臭敏感だとダメ（触手経験で緩和される）
	SIF STAIN:胸 & 32 && TALENT:PLAYER:汚臭敏感 && (EXP:PLAYER:触手経験 < 50 || EXP:PLAYER:獣姦経験 < 30 || EXP:PLAYER:鰻風呂経験 < 10 || EXP:PLAYER:蟲風呂経験 < 10) < 50
		RETURN 0
	;　吸血鬼でなく、汚臭敏感だとダメ
	SIF STAIN:胸 & 64 && !TALENT:PLAYER:吸血鬼 && TALENT:PLAYER:汚臭敏感
		RETURN 0
	;尿　基本ダメ（汚臭鈍感はOK。スカ経験で緩和される）
	SIF STAIN:胸 & 128 && !TALENT:PLAYER:汚臭鈍感 && ((TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 30) || EXP:PLAYER:スカ経験 < 15)
		RETURN 0
	;便　汚臭鈍感も含め、基本ダメ（スカ経験で緩和される）
	SIF STAIN:胸 & 512 && ((TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 50) || (!TALENT:PLAYER:汚臭鈍感 && !TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 30) || (TALENT:PLAYER:汚臭鈍感 && EXP:PLAYER:スカ経験 < 15))
		RETURN 0
	;吐瀉物　汚臭鈍感も含め、基本ダメ（スカ経験で緩和される）
	SIF STAIN:胸 & 2048 && ((TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 50) || (!TALENT:PLAYER:汚臭鈍感 && !TALENT:PLAYER:汚臭敏感 && EXP:PLAYER:スカ経験 < 30) || (TALENT:PLAYER:汚臭鈍感 && EXP:PLAYER:スカ経験 < 15))
		RETURN 0
ENDIF

RETURN 1


