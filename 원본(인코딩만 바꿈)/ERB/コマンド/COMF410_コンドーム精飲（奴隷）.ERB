;-------------------------------------------------
;コンドーム精飲（奴隷）
;食ザー系コマンド
;-------------------------------------------------
@COM410

PRINTL コンドーム精飲（奴隷）
STR:0 = コンドーム精飲（奴隷）

CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 0
LOSEBASE:気力 += 50

SOURCE:性行動 += 420
SOURCE:達成感 += 150
SOURCE:屈従 += 500
SOURCE:逸脱 += 300
SOURCE:反発 += 300

;上のほうで計算した汚れデータ
SOURCE:不潔 += Y*40 + 100


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 100


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の口に汚れ移動
STAIN:口 |= 4


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;コンドームの精液を初期化
CFLAG:107 = 0


RETURN 1



