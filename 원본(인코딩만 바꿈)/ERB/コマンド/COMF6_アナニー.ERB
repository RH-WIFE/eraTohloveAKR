;-------------------------------------------------
;アナニー
;自慰系コマンド
;-------------------------------------------------
@COM6

;バイブ、ビデオ撮影の時に内容変化
;ビデオ撮影中なら公開オナニー
;野外プレイ中なら野外オナニー
;アナルバイブ装備でバイブオナニー
LOCALS = 

SIF TEQUIP:ビデオカメラ
	LOCALS = 公開
SIF TEQUIP:野外プレイ
	LOCALS = 野外

IF TEQUIP:拡張バルーン
	LOCALS = %LOCALS%バルーン
ELSEIF TEQUIP:アナルバイブ
	LOCALS = %LOCALS%バイブ
ELSEIF TEQUIP:ローターＡ挿入
	LOCALS = %LOCALS%ローター
ELSEIF TEQUIP:アナルビーズ
	LOCALS = %LOCALS%ビーズ
ENDIF

LOCALS = %LOCALS%アナニー

STR:0 = %LOCALS%
PRINTFORML %STR:0%

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;実行判定操作
CALL KOJO_DENIAL_PLUS

;合計を表示(35以上で実行)
PRINT  = 
PRINTV A

;難易度上昇
;公開で+10、野外で+10
V = 38
SIF TEQUIP:ビデオカメラ
	V += 10
SIF TEQUIP:野外プレイ
	V += 10

SIF TEQUIP:拡張バルーン
	V += 10
SIF TEQUIP:アナルバイブ
	V += 5
SIF TEQUIP:アナルビーズ
	V += 5
SIF TEQUIP:ローターＡ挿入
	V += 3

SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

;実行不可口上/実行判定操作
SIF A < V
	CALL KOJO_DENIAL

WAIT

;実行できない
SIF A < V
	RETURN 0

CALL KOJO_MESSAGE_COM


;実行者が奴隷であるフラグ
TFLAG:28 = 1

;ストッキングびりびりフラグ
IF TEQUIP:特殊２ == 5 || TEQUIP:特殊２ == 6
	TFLAG:55 = 1
ENDIF


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 20
LOSEBASE:気力 += 70

SOURCE:快Ａ += 80
SOURCE:痛み += 80

IF TEQUIP:アナルバイブ || TEQUIP:ローターＡ挿入 || TEQUIP:アナルビーズ || TEQUIP:拡張バルーン
	TIMES SOURCE:快Ａ , 1.50

	SIF TEQUIP:拡張バルーン
		SOURCE:痛み += 200
	SIF TEQUIP:アナルバイブ
		SOURCE:痛み += 200
	SIF TEQUIP:アナルビーズ
		SOURCE:痛み += 200
	SIF TEQUIP:ローターＡ挿入
		SOURCE:痛み += 130

	SOURCE:屈従 += 150
	SOURCE:反発 += 150
ENDIF


;Aへの苦痛は先に処理しておく
;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES SOURCE:痛み , 3.00
	TIMES SOURCE:反発 , 1.50
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:痛み , 1.20
	TIMES SOURCE:反発 , 1.20
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES SOURCE:痛み , 0.80
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES SOURCE:痛み , 0.60
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES SOURCE:痛み , 0.40
ELSEIF EXP:Ａ経験 >= EXPLV:5
	TIMES SOURCE:痛み , 0.20
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:痛み , 1.20
	TIMES SOURCE:反発 , 1.20
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:痛み , 1.00
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:痛み , 0.60
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:痛み , 0.30
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:痛み , 0.20
ENDIF

SOURCE:性行動 += 100
SOURCE:恐れ += 250
SOURCE:中毒充足 += 250
SOURCE:露出 += 450
SOURCE:屈従 += 600
SOURCE:逸脱 += 350
SOURCE:反発 += 200


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 450

IF TEQUIP:ビデオカメラ || TEQUIP:野外プレイ
	SELECTCASE ABL:露出癖
		CASE 0
			TIMES SOURCE:反発 , 2.00
		CASE 1
			SOURCE:欲情 += 10
			TIMES SOURCE:反発 , 1.20
		CASE 2
			SOURCE:欲情 += 20
			TIMES SOURCE:反発 , 1.00
		CASE 3
			SOURCE:欲情 += 50
			TIMES SOURCE:反発 , 0.70
		CASE 4
			SOURCE:欲情 += 75
			TIMES SOURCE:反発 , 0.50
		CASE 5
			SOURCE:欲情 += 80
			TIMES SOURCE:反発 , 0.30
		CASE 6
			SOURCE:欲情 += 100
			TIMES SOURCE:反発 , 0.10
		CASE 7
			SOURCE:欲情 += 120
			TIMES SOURCE:反発 , 0.10
		CASE 8
			SOURCE:欲情 += 150
			TIMES SOURCE:反発 , 0.10
		CASE 9
			SOURCE:欲情 += 180
			TIMES SOURCE:反発 , 0.10
		CASEELSE
			SOURCE:欲情 += 200
			TIMES SOURCE:反発 , 0.10
	ENDSELECT

	;ビデオ
	IF TEQUIP:ビデオカメラ
		SOURCE:露出 += 50
		SOURCE:液体 += 50
		SOURCE:欲情 += 50
		SOURCE:反発 += 80
	ENDIF
	;野外プレイ
	IF TEQUIP:野外プレイ
		SOURCE:露出 += 100
		SOURCE:屈従 += 100
		SOURCE:反発 +=80
	ENDIF
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES SOURCE:反発 , 1.50
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:反発 , 1.20
ENDIF

;中毒による中毒充足のソースは先に処理しておく
;自慰中毒を見る
IF ABL:自慰中毒 == 1
	TIMES SOURCE:快Ａ , 1.00
	TIMES SOURCE:中毒充足 , 0.00
ELSEIF ABL:自慰中毒 == 2
	TIMES SOURCE:快Ａ , 1.10
	TIMES SOURCE:中毒充足 , 0.30
ELSEIF ABL:自慰中毒 == 3
	TIMES SOURCE:快Ａ , 1.20
	TIMES SOURCE:中毒充足 , 0.60
ELSEIF ABL:自慰中毒 == 4
	TIMES SOURCE:快Ａ , 1.30
	TIMES SOURCE:中毒充足 , 0.90
ELSEIF ABL:自慰中毒 == 5
	TIMES SOURCE:快Ａ , 1.50
	TIMES SOURCE:中毒充足 , 1.20
ELSEIF ABL:自慰中毒 == 6
	TIMES SOURCE:快Ａ , 1.50
	TIMES SOURCE:中毒充足 , 1.50
ELSEIF ABL:自慰中毒 == 7
	TIMES SOURCE:快Ａ , 1.70
	TIMES SOURCE:中毒充足 , 1.80
ELSEIF ABL:自慰中毒 == 8
	TIMES SOURCE:快Ａ , 2.00
	TIMES SOURCE:中毒充足 , 2.30
ELSEIF ABL:自慰中毒 == 9
	TIMES SOURCE:快Ａ , 2.50
	TIMES SOURCE:中毒充足 , 3.00
ELSEIF ABL:自慰中毒 >= 10
	TIMES SOURCE:快Ａ , 3.50
	TIMES SOURCE:中毒充足 , 5.00
ENDIF


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の手（手袋）⇔奴隷のＡ腸内の汚れが移動
;道具を使う場合は移動しない
IF TEQUIP:アナルバイブ || TEQUIP:ローターＡ挿入 || TEQUIP:アナルビーズ || TEQUIP:拡張バルーン
ELSE
	CALL COM_STAIN
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ａ経験 += 1
PRINTL Ａ経験＋１
;ビデオ撮影時は経験にプラス
IF TEQUIP:ビデオカメラ
	EXP:自慰経験 += 2
	PRINTL 自慰経験＋２
	EXP:調教自慰経験 += 2
	PRINTL 調教自慰経験＋２
	
	IF !EXP:公開オナニー && !TALENT:淫乱 && !TALENT:娼婦
		EXP:異常経験 += 1
		PRINTL 公開オナニー初経験で異常経験＋１
	ENDIF
	EXP:公開オナニー += 1
	PRINTL 公開オナニー経験＋１
ELSE
	EXP:自慰経験 += 1
	PRINTL 自慰経験＋１
	EXP:調教自慰経験 += 1
	PRINTL 調教自慰経験＋１
ENDIF

;ゲイ経験とか加算
CALL COM_EXP(1)

;依存度ベクトル
TFLAG:33 = 3

;依存度補正値(服従で1、隷属で2）
IF TALENT:服従 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:隷属 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

RETURN 1


