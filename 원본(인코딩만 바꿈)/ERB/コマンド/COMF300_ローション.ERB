@COM300

IF TEQUIP:触手
	PRINTL 触手体液塗布
	STR:0 = 触手体液塗布
ELSE
	PRINTL ローション
	STR:0 = ローション
ENDIF

CALL KOJO_MESSAGE_COM


;使ったアイテム減らす
SIF TEQUIP:触手 == 0
	ITEM:40 -= 1


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 0
LOSEBASE:気力 += 0

SOURCE:液体 += 5000
SOURCE:露出 += 100


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;ゲイ経験とか加算
CALL COM_EXP(1)

;依存度ベクトル
TFLAG:33 = 0

RETURN 1

