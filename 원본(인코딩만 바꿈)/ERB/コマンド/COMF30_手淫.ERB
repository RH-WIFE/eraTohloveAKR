;-------------------------------------------------
;手淫する
;Ｐ系調教コマンド
;-------------------------------------------------
@COM30

PRINTL 手淫する
STR:0 = 手淫する
CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 5
LOSEBASE:気力 += 30

SOURCE:快Ｃ += 80
SIF TEQUIP:電動オナホール
	TIMES SOURCE:快Ｃ , 1.50

;着衣のとき
SIF TEQUIP:下着（下） || TEQUIP:特殊２ == 5 || TEQUIP:特殊２ == 6
	TIMES SOURCE:快Ｃ , 0.90
SIF BOTTOMS(TARGET)
	TIMES SOURCE:快Ｃ , 0.70

SOURCE:恐れ += 20
SOURCE:欲情 += 30
SOURCE:露出 += 20
SOURCE:逸脱 += 15
SOURCE:反発 += 15

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 20

;スキンシップ好きだと情愛のソース追加
SIF TALENT:スキンシップ好き
	SOURCE:情愛 += 50

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;調教者の手⇔奴隷のＣ（下着、ズボン系衣類）の汚れが移動
;道具を使う場合は汚れは移動しない
IF TEQUIP:電動オナホール
ELSE
	CALL COM_STAIN
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;ゲイ経験とか加算
CALL COM_EXP(1)

;依存度ベクトル
TFLAG:33 = 1


RETURN 1



