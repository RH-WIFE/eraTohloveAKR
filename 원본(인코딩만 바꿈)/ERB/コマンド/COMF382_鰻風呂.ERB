;-------------------------------------------------
;鰻風呂
;人外系コマンド
;-------------------------------------------------
@COM382

PRINTL 鰻風呂
STR:0 = 鰻風呂

CALL KOJO_MESSAGE_COM

F = 1
R = 1
;Vに道具を挿入している場合
SIF TALENT:オトコ || TEQUIP:バイブ || TEQUIP:貞操帯
	F = 0

;Aに道具を挿入している場合
SIF TEQUIP:アナルバイブ || TEQUIP:アナルビーズ || TEQUIP:拡張バルーン
	R = 0

;V経験を伴うコマンドのフラグ(処女膜再生対応)
SIF F && (TALENT:処女 || TALENT:再生処女)
	TFLAG:19 |= 1

;ストッキングびりびりフラグ
IF TEQUIP:特殊２ == 5 || TEQUIP:特殊２ == 6
	TFLAG:55 = 1
ENDIF


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 400
LOSEBASE:気力 += 650

SOURCE:快Ｃ += 100
SOURCE:快Ｂ += 100
SOURCE:痛み += 1000
SOURCE:恐れ += 2000
SOURCE:屈従 += 1200
SOURCE:不潔 += 800
SOURCE:逸脱 += 1500
SOURCE:反発 += 1500
;Vに道具を挿入していない場合
IF F
	SOURCE:快Ｖ += 350
	SOURCE:痛み += 500
	SOURCE:恐れ += 500
ENDIF
;Aに道具を挿入していない場合
IF R
	SOURCE:快Ａ += 350
	SOURCE:痛み += 500
	SOURCE:恐れ += 500
ENDIF

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;Vに道具を挿入していない場合のみ見る
IF F
	;Vへの苦痛は先に処理しておく
	;EXP:Ｖ経験をみる（処女の反感は別途処理済みなので省略）
	IF EXP:Ｖ経験 < EXPLV:1
		TIMES SOURCE:痛み , 2.50
	ELSEIF EXP:Ｖ経験 < EXPLV:2
		TIMES SOURCE:痛み , 0.80
		TIMES SOURCE:反発 , 1.20
	ELSEIF EXP:Ｖ経験 < EXPLV:3
		TIMES SOURCE:痛み , 0.40
	ELSEIF EXP:Ｖ経験 < EXPLV:4
		TIMES SOURCE:痛み , 0.10
	ELSEIF EXP:Ｖ経験 < EXPLV:5
		TIMES SOURCE:痛み , 0.00
	ELSEIF EXP:Ｖ経験 >= EXPLV:5
		TIMES SOURCE:痛み , 0.00
	ENDIF

	;PALAM:潤滑をみる
	IF PALAM:潤滑 < PALAMLV:1
		TIMES SOURCE:痛み , 1.20
		TIMES SOURCE:反発 , 1.50
	ELSEIF PALAM:潤滑 < PALAMLV:2
		TIMES SOURCE:痛み , 0.70
		TIMES SOURCE:反発 , 1.20
	ELSEIF PALAM:潤滑 < PALAMLV:3
		TIMES SOURCE:痛み , 0.40
	ELSEIF PALAM:潤滑 < PALAMLV:4
		TIMES SOURCE:痛み , 0.10
	ELSEIF PALAM:潤滑 >= PALAMLV:4
		TIMES SOURCE:痛み , 0.10
	ENDIF
ENDIF

;Aに道具を挿入していない場合のみ見る
IF R
	;Aへの苦痛は先に処理しておく
	;EXP:Ａ経験をみる
	IF EXP:Ａ経験 < EXPLV:1
		TIMES SOURCE:痛み , 3.00
		TIMES SOURCE:反発 , 3.00
	ELSEIF EXP:Ａ経験 < EXPLV:2
		TIMES SOURCE:痛み , 1.20
		TIMES SOURCE:反発 , 2.00
	ELSEIF EXP:Ａ経験 < EXPLV:3
		TIMES SOURCE:痛み , 0.80
		TIMES SOURCE:反発 , 1.50
	ELSEIF EXP:Ａ経験 < EXPLV:4
		TIMES SOURCE:痛み , 0.60
		TIMES SOURCE:反発 , 1.20
	ELSEIF EXP:Ａ経験 < EXPLV:5
		TIMES SOURCE:痛み , 0.40
	ELSEIF EXP:Ａ経験 >= EXPLV:5
		TIMES SOURCE:痛み , 0.20
	ENDIF

	;PALAM:潤滑をみる
	IF PALAM:潤滑 < PALAMLV:1
		TIMES SOURCE:痛み , 1.20
		TIMES SOURCE:反発 , 2.00
	ELSEIF PALAM:潤滑 < PALAMLV:2
		TIMES SOURCE:痛み , 1.00
		TIMES SOURCE:反発 , 1.50
	ELSEIF PALAM:潤滑 < PALAMLV:3
		TIMES SOURCE:痛み , 0.60
	ELSEIF PALAM:潤滑 < PALAMLV:4
		TIMES SOURCE:痛み , 0.30
	ELSEIF PALAM:潤滑 >= PALAMLV:4
		TIMES SOURCE:痛み , 0.20
	ENDIF
ENDIF

;特にハードなコマンドでは、異常経験に応じて効果を減らす
IF EXP:異常経験 < EXPLV:1
	TIMES SOURCE:恐れ , 1.20
	TIMES SOURCE:逸脱 , 1.20
	TIMES SOURCE:反発 , 1.20
ELSEIF EXP:異常経験 < EXPLV:2
	TIMES SOURCE:恐れ , 0.90
	TIMES SOURCE:逸脱 , 0.90
	TIMES SOURCE:反発 , 0.90
ELSEIF EXP:異常経験 < EXPLV:3
	TIMES SOURCE:恐れ , 0.80
	TIMES SOURCE:逸脱 , 0.80
	TIMES SOURCE:反発 , 0.80
ELSEIF EXP:異常経験 < EXPLV:4
	TIMES SOURCE:恐れ , 0.70
	TIMES SOURCE:逸脱 , 0.70
	TIMES SOURCE:反発 , 0.70
ELSEIF EXP:異常経験 < EXPLV:5
	TIMES SOURCE:恐れ , 0.60
	TIMES SOURCE:逸脱 , 0.60
	TIMES SOURCE:反発 , 0.60
ELSE
	TIMES SOURCE:恐れ , 0.50
	TIMES SOURCE:逸脱 , 0.50
	TIMES SOURCE:反発 , 0.50
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;粘液の汚れ
IF TEQUIP:頭部装飾
	STAIN:頭部装飾 = 32
	STAIN:頭部装飾 = 128
ENDIF
IF TEQUIP:特殊３
	STAIN:目装飾 = 32
	STAIN:目装飾 = 128
ENDIF

STAIN:髪 = 32
STAIN:髪 = 128
STAIN:口 = 32

IF TEQUIP:特殊１ == 1
	STAIN:手袋 = 32
	STAIN:手袋 = 128
ELSE
	STAIN:手 = 32
ENDIF

STAIN:胸 = 32
STAIN:ペニス = 32
STAIN:ヴァギナ = 32
IF F
	STAIN:ヴァギナ = 32
	STAIN:膣内 = 32
	;膣内に破瓜の血の汚れを付加
	SIF TALENT:処女 == 1 || TALENT:再生処女 == 1
		STAIN:膣内 |= 64
ENDIF
STAIN:アナル = 32
SIF R
	STAIN:腸内 = 32
STAIN:足 = 32
IF TEQUIP:特殊２
	STAIN:足装飾 = 32
	STAIN:足装飾 = 128
ENDIF




;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;V経験
IF F > 0
	EXP:Ｖ経験 += 2
	PRINTL Ｖ経験＋２
ENDIF

;A経験
IF R > 0
	EXP:Ａ経験 += 2
	PRINTL Ａ経験＋２
ENDIF

;鰻風呂初経験のとき、異常経験追加
IF EXP:鰻風呂経験 == 0
	EXP:異常経験 += 1
	PRINTL 鰻風呂初経験で異常経験＋１
ENDIF
EXP:鰻風呂経験 += 1

;依存度ベクトル
TFLAG:33 = 5

;依存度補正値(服従で1、隷属で2）
IF TALENT:服従 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:隷属 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1


