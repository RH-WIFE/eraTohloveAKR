;-------------------------------------------------
;触手乳腺責め
;触手系コマンド
;-------------------------------------------------
@COM361

PRINTL 触手乳腺責め
STR:0 = 触手乳腺責め
CALL KOJO_MESSAGE_COM


;着脱の処理
IF TEQUIP:触手乳腺挿入 == 0
	TEQUIP:触手乳腺挿入 = 1
ELSE
	TEQUIP:触手乳腺挿入 = 0
ENDIF


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 150
LOSEBASE:気力 += 300

IF EXP:触手経験 < EXPLV:1
	TIMES LOSEBASE:体力 , 1.10
	TIMES LOSEBASE:気力 , 2.00
ELSEIF EXP:触手経験 < EXPLV:2
	TIMES LOSEBASE:体力 , 0.90
	TIMES LOSEBASE:気力 , 1.80
ELSEIF EXP:触手経験 < EXPLV:3
	TIMES LOSEBASE:体力 , 0.80
	TIMES LOSEBASE:気力 , 1.50
ELSEIF EXP:触手経験 < EXPLV:4
	TIMES LOSEBASE:体力 , 0.70
	TIMES LOSEBASE:気力 , 1.20
ELSEIF EXP:触手経験 < EXPLV:5
	TIMES LOSEBASE:体力 , 0.60
	TIMES LOSEBASE:気力 , 1.00
ELSE
	TIMES LOSEBASE:体力 , 0.50
	TIMES LOSEBASE:気力 , 0.50
ENDIF

IF TALENT:母乳体質
	LOSEBASE:体力 /= 2
	LOSEBASE:気力 /= 2
ENDIF

;ABL:Ｂ感覚をみる
IF ABL:Ｂ感覚 == 0
	SOURCE:快Ｂ += 50
ELSEIF ABL:Ｂ感覚 == 1
	SOURCE:快Ｂ += 100
ELSEIF ABL:Ｂ感覚 == 2
	SOURCE:快Ｂ += 200
ELSEIF ABL:Ｂ感覚 == 3
	SOURCE:快Ｂ += 800
ELSEIF ABL:Ｂ感覚 == 4
	SOURCE:快Ｂ += 1600
ELSE
	SOURCE:快Ｂ += 2800
ENDIF

;Ｂ敏感・鈍感　陥没乳首
IF TALENT:Ｂ敏感 || TALENT:陥没乳首
	TIMES SOURCE:快Ｂ , 1.20
ELSEIF TALENT:Ｂ鈍感
	TIMES SOURCE:快Ｂ , 0.70
ENDIF

;EXP:触手経験をみる
IF EXP:触手経験 < EXPLV:1
	SOURCE:痛み += 10000
	SOURCE:不潔 += 10000
	SOURCE:逸脱 += 10000
ELSEIF EXP:触手経験 < EXPLV:2
	SOURCE:痛み += 5000
	SOURCE:不潔 += 6500
	SOURCE:逸脱 += 8000
ELSEIF EXP:触手経験 < EXPLV:3
	SOURCE:痛み += 3000
	SOURCE:不潔 += 4500
	SOURCE:逸脱 += 6000
ELSEIF EXP:触手経験 < EXPLV:4
	SOURCE:痛み += 2000
	SOURCE:不潔 += 3000
	SOURCE:逸脱 += 4000
ELSEIF EXP:触手経験 < EXPLV:5
	SOURCE:痛み += 1000
	SOURCE:不潔 += 1500
	SOURCE:逸脱 += 2000
ELSE
	SOURCE:痛み += 500
	SOURCE:不潔 += 750
	SOURCE:逸脱 += 1000
ENDIF

;胸がない・貧乳・巨乳・爆乳
IF BREAST(TARGET)
	TIMES SOURCE:痛み , 1.75
ELSEIF TALENT:貧乳
	TIMES SOURCE:痛み , 1.50
ELSEIF TALENT:爆乳
	TIMES SOURCE:快Ｂ , 1.50
ELSEIF TALENT:巨乳
	TIMES SOURCE:快Ｂ , 1.20
ENDIF

;EXP:触手経験をみる
IF EXP:触手経験 < EXPLV:1
	SOURCE:欲情 += 0
	TIMES SOURCE:性行動 , 0.50
	TIMES SOURCE:達成感 , 0.50
	TIMES SOURCE:不潔 , 2.00
	TIMES SOURCE:逸脱 , 4.00
ELSEIF EXP:触手経験 < EXPLV:2
	SOURCE:欲情 += 60
	TIMES SOURCE:性行動 , 0.80
	TIMES SOURCE:達成感 , 0.80
	TIMES SOURCE:不潔 , 1.50
	TIMES SOURCE:逸脱 , 1.80
ELSEIF EXP:触手経験 < EXPLV:3
	SOURCE:欲情 += 180
	TIMES SOURCE:性行動 , 1.00
	TIMES SOURCE:達成感 , 1.00
	TIMES SOURCE:不潔 , 1.00
	TIMES SOURCE:逸脱 , 1.00
ELSEIF EXP:触手経験 < EXPLV:4
	SOURCE:欲情 += 360
	TIMES SOURCE:性行動 , 1.20
	TIMES SOURCE:達成感 , 1.20
	TIMES SOURCE:不潔 , 0.50
	TIMES SOURCE:逸脱 , 0.70
ELSEIF EXP:触手経験 < EXPLV:5
	SOURCE:欲情 += 640
	TIMES SOURCE:性行動 , 1.50
	TIMES SOURCE:達成感 , 1.50
	TIMES SOURCE:不潔 , 0.10
	TIMES SOURCE:逸脱 , 0.20
ELSE
	SOURCE:欲情 += 900
	TIMES SOURCE:性行動 , 2.00
	TIMES SOURCE:達成感 , 2.00
	TIMES SOURCE:不潔 , 0.00
	TIMES SOURCE:逸脱 , 0.10
ENDIF


IF TEQUIP:触手乳腺挿入
	SIF TALENT:母乳体質
		STAIN:胸 |= 16
	TEQUIP:触手乳腺挿入 = 0
	T = 0
ELSE
	IF TALENT:母乳体質 == 0
		IF PALAM:欲情 > PALAMLV:5
			UP:恐怖 += 3000
			;オトコ母乳取得コンフィグオフのときオトコ/半陰半陽/カントボーイは取得不可
			SIF ((FLAG:2 & 256) == 0 && (BREAST(TARGET) || TALENT:絶壁)) || (FLAG:2 & 256)
				TALENT:母乳体質 = 1
			MAXBASE:母乳 = 10000
		ENDIF
	ELSE
		TIMES SOURCE:痛み , 0.60
	ENDIF
	;初体験のとき異常経験+1
	IF EXP:Ｍ経験 == 0
		EXP:異常経験 += 1
		PRINTL Ｍ拡張初経験で異常経験＋1
	ENDIF
	TEQUIP:触手乳腺挿入 = 1
	T = 1
ENDIF


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ｍ経験 += 1
PRINTL Ｍ経験＋1

RETURN 1



;--------------------------------------------------
;触手乳腺挿入中
;--------------------------------------------------
@EQUIP_COM361
PRINTL ＜触手乳腺挿入中＞

LOSEBASE:体力 += 50
LOSEBASE:気力 += 75

;ABL:Ｂ感覚をみる
IF ABL:Ｂ感覚 == 0
	A = 20
ELSEIF ABL:Ｂ感覚 == 1
	A = 60
ELSEIF ABL:Ｂ感覚 == 2
	A = 100
ELSEIF ABL:Ｂ感覚 == 3
	A = 250
ELSEIF ABL:Ｂ感覚 == 4
	A = 400
ELSE
	A = 600
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES A , 1.10
ELSE
	TIMES A , 1.20
ENDIF

;EXP:触手経験をみる
IF EXP:触手経験 <= EXPLV:0
	TIMES A , 0.80
	SOURCE:中毒充足 += 10
	SOURCE:逸脱 += 2500
ELSEIF EXP:触手経験 <= EXPLV:1
	TIMES A , 0.90
	SOURCE:中毒充足 += 40
	SOURCE:逸脱 += 1500
ELSEIF EXP:触手経験 <= EXPLV:2
	TIMES A , 1.00
	SOURCE:中毒充足 += 100
	SOURCE:屈従 += 10
	SOURCE:逸脱 += 500
ELSEIF EXP:触手経験 <= EXPLV:3
	TIMES A , 1.10
	SOURCE:中毒充足 += 150
	SOURCE:屈従 += 40
	SOURCE:逸脱 += 150
ELSEIF EXP:触手経験 <= EXPLV:4
	TIMES A , 1.20
	SOURCE:中毒充足 += 300
	SOURCE:屈従 += 70
	SOURCE:逸脱 += 10
ELSE
	TIMES A , 1.30
	SOURCE:中毒充足 += 450
	SOURCE:屈従 += 100
ENDIF

;EXP:触手経験をみる
IF EXP:触手経験 <= EXPLV:0
	TIMES A , 0.80
	SOURCE:痛み += 500
ELSEIF EXP:触手経験 <= EXPLV:1
	TIMES A , 1.00
	SOURCE:痛み += 100
ELSEIF EXP:触手経験 <= EXPLV:2
	TIMES A , 1.30
	SOURCE:痛み += 50
ELSEIF EXP:触手経験 <= EXPLV:3
	TIMES A , 1.60
	SOURCE:痛み += 10
ELSEIF EXP:触手経験 <= EXPLV:4
	TIMES A , 2.00
ELSE
	TIMES A , 2.25
ENDIF

;EXP:墳乳経験をみる
IF EXP:噴乳経験 <= EXPLV:0
	TIMES A , 0.80
ELSEIF EXP:噴乳経験 <= EXPLV:1
	TIMES A , 1.00
ELSEIF EXP:噴乳経験 <= EXPLV:2
	TIMES A , 1.30
ELSEIF EXP:噴乳経験 <= EXPLV:3
	TIMES A , 1.60
ELSEIF EXP:噴乳経験 <= EXPLV:4
	TIMES A , 2.00
ELSE
	TIMES A , 2.25
ENDIF

;Ｂ鈍感とＢ敏感と陥没乳首
IF TALENT:Ｂ敏感
	TIMES A , 1.40
ELSEIF TALENT:Ｂ鈍感
	TIMES A , 0.60
ELSEIF TALENT:陥没乳首
	TIMES A , 1.20
ENDIF

;巨乳と爆乳、貧乳と絶壁
IF TALENT:爆乳
	TIMES A , 1.20
ELSEIF TALENT:巨乳
	TIMES A , 1.50
ELSEIF TALENT:貧乳
	TIMES SOURCE:痛み , 1.50
ELSEIF TALENT:絶壁 || BREAST(TARGET)
	TIMES SOURCE:痛み , 1.75
ENDIF
SOURCE:快Ｂ += A

STAIN:胸 |= 32
T += 1

RETURN 1



