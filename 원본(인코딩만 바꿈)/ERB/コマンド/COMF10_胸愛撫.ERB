;-------------------------------------------------
;胸愛撫
;Ｂ系調教コマンド
;-------------------------------------------------
@COM10

PRINTL 胸愛撫
STR:0 = 胸愛撫
CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 5
LOSEBASE:気力 += 20

SOURCE:快Ｂ += 80
;着衣プレイ
SIF TEQUIP:下着（上） || TOPS(TARGET)
	TIMES SOURCE:快Ｂ , 0.70

SOURCE:情愛 += 50
;スキンシップ好きだと情愛のソース追加
SIF TALENT:スキンシップ好き
	SOURCE:情愛 += 50

SOURCE:性行動 += 60
SOURCE:露出 += 10
SOURCE:不潔 += 20
SOURCE:反発 += 20


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 20



;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;ゲイ経験とか加算
CALL COM_EXP(5)

;依存度ベクトル
TFLAG:33 = 1


RETURN 1


