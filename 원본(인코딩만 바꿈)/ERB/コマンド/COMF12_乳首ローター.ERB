;-------------------------------------------------
;乳首ローター
;Ｂ系調教コマンド
;-------------------------------------------------
@COM12

PRINTL 乳首ローター
STR:0 = 乳首ローター
CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 5
LOSEBASE:気力 += 30

SOURCE:快Ｂ += 150
;着衣プレイ
SIF TEQUIP:下着（上） || TOPS(TARGET)
	TIMES SOURCE:快Ｂ , 0.70

SOURCE:露出 += 20
SOURCE:逸脱 += 30
SOURCE:反発 += 30


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 30


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;ゲイ経験とか加算
CALL COM_EXP(1)

;依存度ベクトル
TFLAG:33 = 1


RETURN 1




