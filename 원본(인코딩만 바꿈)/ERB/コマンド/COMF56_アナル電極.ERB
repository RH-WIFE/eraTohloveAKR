;-------------------------------------------------
;アナル電極
;Ａ系調教コマンド
;-------------------------------------------------
@COM56

PRINTL アナル電極
STR:0 = アナル電極
CALL KOJO_MESSAGE_COM

;ストッキングびりびりフラグ
IF TEQUIP:特殊２ == 5 || TEQUIP:特殊２ == 6
	TFLAG:55 = 1
ENDIF


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
SOURCE:快Ａ += 500

IF TEQUIP:地下室 || TFLAG:56 == 3
	;消費体力、気力
	LOSEBASE:体力 += 300
	LOSEBASE:気力 += 450
	
	SOURCE:痛み += 5000
	SOURCE:恐れ += 3000
	SOURCE:屈従 += 1600
ELSEIF TFLAG:56 == 2
	;消費体力、気力
	LOSEBASE:体力 += 200
	LOSEBASE:気力 += 350
	
	SOURCE:痛み += 3500
	SOURCE:恐れ += 1500
	SOURCE:屈従 += 1200
ELSE
	;消費体力、気力
	LOSEBASE:体力 += 100
	LOSEBASE:気力 += 250
	
	SOURCE:痛み += 1500
	SOURCE:恐れ += 500
	SOURCE:屈従 += 800
ENDIF

SOURCE:露出 += 300
SOURCE:逸脱 += 1200
SOURCE:反発 += 1200

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 1200

;フィストなどのAへの大きな苦痛は先に処理しておく
;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES SOURCE:痛み , 3.00
	TIMES SOURCE:反発 , 4.00
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:痛み , 2.50
	TIMES SOURCE:反発 , 3.50
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES SOURCE:痛み , 2.40
	TIMES SOURCE:反発 , 3.00
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES SOURCE:痛み , 2.00
	TIMES SOURCE:反発 , 2.50
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES SOURCE:痛み , 1.80
	TIMES SOURCE:反発 , 2.00
ELSEIF EXP:Ａ経験 >= EXPLV:5
	TIMES SOURCE:痛み , 1.60
	TIMES SOURCE:反発 , 1.50
ENDIF


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;Ａ経験
EXP:Ａ経験 += 5
PRINTL Ａ経験＋５

;電極初体験のとき、異常経験追加
IF EXP:電極経験 == 0
	EXP:異常経験 += 1
	PRINTL 電極初経験で異常経験＋１
ENDIF
EXP:電極経験 += 1

;ゲイ経験とか加算
CALL COM_EXP(1)

;依存度ベクトル
TFLAG:33 = 5

;依存度補正値(服従で1、隷属で2）
IF TALENT:服従 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:隷属 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1

