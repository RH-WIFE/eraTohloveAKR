;-------------------------------------------------
;乳スパンキング
;ＳＭ系コマンド
;-------------------------------------------------
@COM141

PRINTL 乳スパンキング
STR:0 = 乳スパンキング
CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
SOURCE:快Ｂ += 20

IF TEQUIP:地下室
	;消費体力、気力
	LOSEBASE:体力 += 50
	LOSEBASE:気力 += 20
	
	SOURCE:痛み += 1000
ELSE
	;消費体力、気力
	LOSEBASE:体力 += 30
	LOSEBASE:気力 += 10
	
	SOURCE:痛み += 450
ENDIF
;着衣のとき、痛みが軽減
SIF TOPS(TARGET) || TEQUIP:下着（上）
	TIMES SOURCE:痛み , 0.70

SOURCE:恐れ += 400
;地下室にいると恐れのソース追加
SIF TEQUIP:地下室
	TIMES SOURCE:恐れ , 2.00

SOURCE:逸脱 += 50
SOURCE:反発 += 300

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;苦痛のパラメータによる苦痛のソース増加は先に処理しておく
IF PALAM:苦痛 < PALAMLV:1
	TIMES SOURCE:痛み , 1.00
ELSEIF PALAM:苦痛 < PALAMLV:2
	TIMES SOURCE:痛み , 1.50
ELSEIF PALAM:苦痛 < PALAMLV:3
	TIMES SOURCE:痛み , 2.00
ELSEIF PALAM:苦痛 < PALAMLV:4
	TIMES SOURCE:痛み , 2.50
ELSEIF PALAM:苦痛 >= PALAMLV:4
	TIMES SOURCE:痛み , 3.00
ENDIF


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷のＢ（上衣類）⇔調教者の手の汚れが移動
;地下室にいるときは道具を使用するので、汚れない
IF TEQUIP:地下室
ELSE
	CALL COM_STAIN
ENDIF



;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;ゲイ経験とか加算
CALL COM_EXP(2)

;依存度ベクトル
TFLAG:33 = 3

;依存度補正値(服従で1、隷属で2）
IF TALENT:服従 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:隷属 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

RETURN 1



