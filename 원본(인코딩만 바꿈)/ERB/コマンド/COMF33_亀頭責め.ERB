;-------------------------------------------------
;亀頭責め
;Ｐ系調教コマンド
;-------------------------------------------------
@COM33

PRINTL 亀頭責め
STR:0 = 亀頭責め
CALL KOJO_MESSAGE_COM

;ストッキングびりびりフラグ
IF TEQUIP:特殊２ == 5 || TEQUIP:特殊２ == 6
	TFLAG:55 = 1
ENDIF


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 100
LOSEBASE:気力 += 200

SOURCE:快Ｃ += 350

SOURCE:痛み += 200
SIF TALENT:Ｃ敏感
	SOURCE:痛み += 200
SOURCE:恐れ += 200
SOURCE:露出 += 120
SOURCE:屈従 += 160
SOURCE:逸脱 += 80
SOURCE:反発 += 80


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 80

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
CALL COM_STAIN


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;ゲイ経験とか加算
CALL COM_EXP(1)

;依存度ベクトル
TFLAG:33 = 3

RETURN 1



