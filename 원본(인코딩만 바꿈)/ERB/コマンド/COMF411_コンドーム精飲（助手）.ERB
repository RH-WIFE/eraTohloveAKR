;-------------------------------------------------
;コンドーム精飲（助手）
;食ザー系コマンド
;-------------------------------------------------
@COM411

PRINTL コンドーム精飲（助手）
STR:0 = コンドーム精飲（助手）
CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 0
LOSEBASE:気力 += 30

SOURCE:液体 += 100
SOURCE:屈従 += 800
SOURCE:逸脱 += 200
SOURCE:反発 += 200


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 50

;助手と奴隷の相性によって、全体のソースが変化
T = NO:TARGET
A = NO:ASSI
SOURCE:液体 *= RELATION:A
SOURCE:屈従 *= RELATION:A
SOURCE:鬱屈 *= RELATION:A
SOURCE:逸脱 *= RELATION:A
SOURCE:反発 *= RELATION:A
SOURCE:液体 /= 100
SOURCE:屈従 /= 100
SOURCE:鬱屈 /= 100
SOURCE:逸脱 /= 100
SOURCE:反発 /= 100


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;助手の口に精液汚れ
STAIN:ASSI:口 |= 4


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;コンドームの精液を初期化
CFLAG:107 = 0


RETURN 1



