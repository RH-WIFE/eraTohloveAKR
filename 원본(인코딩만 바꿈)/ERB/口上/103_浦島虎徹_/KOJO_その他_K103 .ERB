	CALL PALAM_MESSAGE_A
	;髪
	IF CFLAG:106 == 1
		CFLAG:221 = 1
	;顔
	ELSEIF CFLAG:106 == 2
		IF RAND:2 == 0
		PRINTFORM 「…趣味としてはわからないでもないけど、
			IF TALENT:オトコ == 1
				PRINTFORMW 俺とか、男にしてもあんまり楽しくないんじゃないのかな」
			ELSE
				PRINTFORMW フツーの女の子にした方がいいんじゃない？」
			ENDIF
		ELSE
			PRINTFORMW 「なにするんだよぉ！」
		ENDIF
		CFLAG:222 = 1
	;胸
	ELSEIF CFLAG:106 == 3
		CFLAG:223 = 1
	;腹
	ELSEIF CFLAG:106 == 4
		CFLAG:224 = 1
	;腋
	ELSEIF CFLAG:106 == 5
		CFLAG:225 = 1
	;太腿
	ELSEIF CFLAG:106 == 6
		CFLAG:226 = 1
	;割れ目
	ELSEIF CFLAG:106 == 7
		CFLAG:227 = 1
	;尻
	ELSEIF CFLAG:106 == 8
		CFLAG:228 = 1
	;ゴム
	ELSEIF CFLAG:106 == 9
