;-------------------------------------------------
;COM306, 酩酊プレイ FIRSTTIME(406)
;-------------------------------------------------
@KOJO_COM_K174_306
#DIMS DYNAMIC DRINK_NAME
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;地の文呼び出し(カラー変更対応)
	CALL TRAIN_MESSAGE_COLOR
	
	;反発なら弾く
	SIF (CFLAG:5005 & 32)
		RETURN LOCAL
	
	;真友ルート服従なら弾く(隷属は可)
	SIF (CFLAG:5001 & 2) && TALENT:服従
		RETURN LOCAL
	
	;水心子が下戸、清麿も下戸
	IF TALENT:下戸 && TALENT:PLAYER:下戸 == 1
		PRINTFORML 「\@(CFLAG:5005 & 1)?私#僕\@も清麿もほとんど飲めないのに、なぜ酒があるんだ」
		RETURN LOCAL
	;水心子が下戸、清麿は上戸か普通
	ELSEIF TALENT:下戸
		PRINTFORML 「清麿に飲ませるよりはいいが……　ぅ、っ……」
		RETURN LOCAL
	;水心子が上戸か普通
	ELSE
		;清麿が下戸
		IF TALENT:PLAYER:下戸 == 1
			PRINTFORMW 『水心子　ちょっとでいいから分けてくれないかな？』
			PRINTFORMW 「絶対だめだ」
			PRINTFORMW 『えー、ケチ』
			IF (CFLAG:5001 & 2) && TALENT:隷属
				PRINTFORMW 「清麿はお酒弱いだろう　それに、清麿の刀工は、」
			ELSE
				PRINTFORMW 「ケチとはなんだ　清麿はお酒弱いだろう　それに、清麿の刀工は……」
			ENDIF
			PRINTFORMW 『隙あり』
			PRINTFORMW %CALLNAME:PLAYER%は%CALLNAME%の唇を奪った
			PRINTFORMW 「んっ、んむ……　っんん　……っ、ぷは」
			PRINTFORMW 『うん　おいしい』
			IF (CFLAG:5001 & 2) && TALENT:隷属
				PRINTFORMW 「……清麿　僕は君を心配してるんだぞ」
				PRINTFORMW 『うん　でも、少しぐらいなら大丈夫だよ』
				PRINTFORMW 「そうかもしれないけど……」
			ELSE
				PRINTFORMW 「おい、清麿！　話している最中にキスをするんじゃない」
				PRINTFORMW 『あはは　隙を見せるのが悪いんだよ』
			ENDIF
		;清麿が上戸か普通
		;よくよく考えれば服従隷属でもここ通るけど、相性いいし口移しぐらいはセーフですよね
		ELSE
			;飲ませた酒を格納
			IF TFLAG:76 == 1
				DRINK_NAME = 甘酒
			ELSEIF TFLAG:76 == 2
				DRINK_NAME = ビール
			ELSEIF TFLAG:76 == 3
				DRINK_NAME = 日本酒
			ENDIF
			PRINTFORMW 『水心子　ちょっとでいいから分けてくれないかな？』
			PRINTFORMW 「少しだけだぞ」
			PRINTFORMW 『あ、せっかくだし口移しがいいな』
			IF (CFLAG:5001 & 2) && TALENT:隷属
				PRINTFORMW 「ああ、分かった」
			ELSE
				PRINTFORMW 「注文が多いなぁ」
			ENDIF
			PRINTFORMW %CALLNAME%は口内に幾分か%DRINK_NAME%を含むと、口移しで%CALLNAME:PLAYER%に飲ませた
			PRINTFORMW 「んっ、んん……、っんむ、ぅ……、ぷは」
			PRINTFORMW 「これで満足か」
			PRINTFORMW 『うん　ありがとう　普段の100倍は美味しく感じたよ』
			IF (CFLAG:5001 & 2) && TALENT:隷属
				PRINTFORMW 「そうか　それは良かった」
			ELSE
				PRINTFORMW 「100倍って……　大げさだな　清麿は」
			ENDIF
		ENDIF
		RETURN LOCAL
	ENDIF
	
ENDIF
RETURN LOCAL

