;-------------------------------------------------
;COM2, 何もしない FIRSTTIME(102)
;-------------------------------------------------
@KOJO_COM_K174_2
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;地の文呼び出し(カラー変更対応)
	CALL TRAIN_MESSAGE_COLOR
	
	;道具がついているとき
	IF ALLCHECK(TARGET) == 1
		;発情イベント
		IF (CFLAG:5004 & 4)
			PRINTFORML 「あっ…ぅあっ…　いぃ、きもち、い…っ、んあっ…あっ、は、ぁ……」
			PRINTFORML %CALLNAME%は夢中で玩具から与えられる快楽を貪っている
			SELECTCASE RAND:3
				CASE 2
					PRINTFORML だらしなく開かれた口からは、唾液がこぼれてしまっている…
				CASE 1
					IF !TEQUIP:アイマスク
						PRINTFORML %CALLNAME%の瞳は、すっかり興奮に溶かされてしまっている…
					ELSE
						PRINTFORML 自身のよりイイところに玩具が当たるように、%CALLNAME%は身をよじっている…
					ENDIF
				CASEELSE
					PRINTFORML 自身のよりイイところに玩具が当たるように、%CALLNAME%は身をよじっている…
			ENDSELECT
		;反発　もしくは未陥落　もしくは恋慕陥落で思慕対象以外
		ELSEIF (CFLAG:5005 & 32) || !KANRAKU(TARGET) || KANRAKU(TARGET) == 4
			PRINTFORML 「ぁ……、ぐっ　っ……ん、は…っ……ぅ…」
			PRINTFORML %CALLNAME%は時折ビクッと身体を跳ねさせながら、声を殺して快感に耐えている
		ELSE
			PRINTFORML 「っ、ぁ…　っあ　あっ、んん…　はっ、はっぁ…ぅ、ぁああっ」
			PRINTFORML %CALLNAME%は嬌声を上げながら、玩具のなすがままにされている
		ENDIF
	;道具ついてない、蕩け強
	ELSEIF (CFLAG:5005 & 16)
		PRINTFORML 赤い顔の%CALLNAME%はしきりに浅い呼吸を繰り返している
		;発情イベント
		IF (CFLAG:5004 & 4)
			;反発　もしくは未陥落　もしくは恋慕陥落で思慕対象以外
			IF (CFLAG:5005 & 32) || !KANRAKU(TARGET) || KANRAKU(TARGET) == 4
			;真友ルート妄執服従√
			ELSEIF (CFLAG:5002 & 128) && (KANRAKU(TARGET) == 3) 
				PRINTFORML 「清麿……　その、はやく……」
				PRINTFORML 『うん　もう少し我慢してみようか』
			;真友ルート隷属
			ELSEIF (CFLAG:5005 & 2) && TALENT:隷属 
				PRINTFORML 「き、清麿……　はやく……」
				PRINTFORML 『どうしたんだい、なにか僕に意見でもあるのかな？』
				PRINTFORML 「いや　なんでも、ないが…っ…その……」
			;それ以外
			ELSE
				PRINTFORML 「清麿……　っ、焦らさないでくれ……」
			ENDIF
		ENDIF
	ELSE
		;反発　もしくは未陥落　もしくは恋慕陥落で思慕対象以外
		IF (CFLAG:5005 & 32) || !KANRAKU(TARGET) || KANRAKU(TARGET) == 4
			PRINTFORML %CALLNAME%は怪訝そうな顔でこちらを見ている
		;陥落済み
		ELSE
			;真友ルート服従隷属　もしくは妄執恋慕√で妄信なし
			IF ((CFLAG:5002 & 2 ) && KANRAKU(TARGET) == 3) || ((CFLAG:5002 & 4) && !TALENT:妄信)
				PRINTFORML 「清麿……？」
				PRINTFORML %CALLNAME%は不安そうにこちらを見ている
			ELSE
				PRINTFORML %CALLNAME%は不思議そうにこちらを見ている
			ENDIF
		ENDIF
	ENDIF
	PRINTW
ENDIF
RETURN LOCAL

