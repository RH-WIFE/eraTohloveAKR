				PRINTFORML 「ほら　ここがいいのだろう？」
			CASE 1
				PRINTFORML 「そこまで期待されては、私も応えざるを得ないな」
			CASE 0
				PRINTFORML 「まったく……　清麿の趣味は理解できないな」
		ENDSELECT
	;サド　もしくはサドっ気３以上
	ELSE
		SELECTCASE RAND:4
			CASE 3
				PRINTFORML 「前々から言おうと思っていたのだが、その……頭は大丈夫か？」
			CASE 2
				PRINTFORML 「ふむ……　ここがいいのか」
			CASE 1
				PRINTFORML 「分かった　では、早速……」
			CASE 0
				;靴を履いている時
				IF TEQUIP:靴 > 0
					PRINTFORML 「これで痛みより快楽が勝るというのが、不思議だな」
					PRINTFORML 「……いや、痛いからこそいいのか？」
				;靴下とかストッキング
				ELSEIF TEQUIP:特殊２ > 0
					PRINTFORML 「うわっ　中まで染みて……！」
				ELSE
					PRINTFORML 「脈打つ感覚が分かるというのも素足ならでは、か」
				ENDIF
		ENDSELECT
	ENDIF
	
	PRINTFORM %CALLNAME%は
				
	;靴を履いている時
	IF TEQUIP:靴
		A = 9
		CALL CLOTHES_NAME
	ELSEIF TEQUIP:特殊２
		A = 2
		CALL CLOTHES_NAME
	ENDIF
	
	;サド　連続実行足コキ
	IF (CFLAG:5005 & 512) && PREVCOM == 80 && RAND:2
		SELECTCASE RAND:2
			CASE 1
				PRINTFORMW \@ TEQUIP:靴 || TEQUIP:特殊２?を履いた足で#足で\@陰茎を蹴り上げた
			CASE 0
				PRINTFORMW \@ TEQUIP:靴 || TEQUIP:特殊２?を履いた足で#足で\@陰茎を踏み潰した
		ENDSELECT
	;ほか
	ELSE
		SELECTCASE RAND:5
			CASE 4
				PRINTFORMW \@ TEQUIP:靴 || TEQUIP:特殊２?を履いた足で#足で\@陰茎をぐりぐりと踏んでいる
			CASE 3
				PRINTFORMW \@ TEQUIP:靴 || TEQUIP:特殊２?のかかとに#かかとに\@力を込め踏みつけた
			CASE 2
				PRINTFORMW \@ TEQUIP:靴 || TEQUIP:特殊２?のつま先で#つま先で\@陰茎を撫で上げた
			CASE 1
				PRINTFORMW \@ TEQUIP:靴 || TEQUIP:特殊２?の足裏で#足裏で\@陰茎を軽く蹴りつけた
			CASE 0
				PRINTFORMW \@ TEQUIP:靴 || TEQUIP:特殊２?を履いた足で#足で\@挟み込むように陰茎を刺激した
		ENDSELECT
	ENDIF
	
ENDIF
RETURN LOCAL

