;-------------------------------------------------
;COM202, スキンシップ FIRSTTIME(302)
;純愛系バリアントでやれ
;-------------------------------------------------
@KOJO_COM_K174_202
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;怯えなら弾く
	IF (CFLAG:5005 & 64)
		PRINTFORML %CALLNAME:PLAYER%はスキンシップを図ろうと、%CALLNAME%に触れた
		PRINTFORMW %CALLNAME%は身体をこわばらせ、息を呑んでいる…
		;おまけ
		SOURCE:恐れ += 20
		RETURN LOCAL
	ENDIF

	;反発なら弾く
	IF (CFLAG:5005 & 32)
		PRINTFORML %CALLNAME:PLAYER%は%CALLNAME%とのスキンシップを図った
		PRINTFORMW %CALLNAME%は無反応を貫いている…
		RETURN LOCAL
	ENDIF
	
	;地の文呼び出し(カラー変更対応)
	CALL TRAIN_MESSAGE_COLOR
	PRINTW
	
	;妄執フラグオン　もしくは真友ルート服従隷属ならストレス値減少
	SIF (CFLAG:5005 & 4) || ((CFLAG:5001 & 2) && (TALENT:服従 || TALENT:隷属))
		CFLAG:10 -= 1
	
	;真友ルート服従隷属なら以下弾く
	SIF (CFLAG:5001 & 2) && (TALENT:服従 || TALENT:隷属)
		RETURN LOCAL
	
	;蕩け強
	IF (CFLAG:5005 & 16)
		PRINTFORML %CALLNAME%を抱きしめてみた
		PRINTFORML 「………。」
		PRINTFORML 無言のまま、%CALLNAME%も抱きしめ返すも、どことなく落ち着かない様子だ
		PRINTFORML 「清麿　これもいいのだが……　\@(CFLAG:5005 & 1)?私#僕\@は、」
		;正面抱きなら耳、後ろ抱きなら腕のイメージ
		PRINTFORML 言って、%CALLNAME%は%CALLNAME:MASTER%の\@RAND:2 == 0 ?耳#腕\@に軽くキスをする
		PRINTFORML 「もっと別のことがしたい、というか……」
		RETURN LOCAL
	ENDIF
	
	;多分ここが一番平和でイチャイチャしてる
	SELECTCASE RAND:10
		CASE 9
			PRINTFORML %CALLNAME%の頬をこねてみた
			PRINTFORML 「清麿　それ、楽しいか？」
			PRINTFORML 『うん』
			PRINTFORML しばらくの間、%CALLNAME%の頬をひたすらもちもちこね続けた
		CASE 8
			PRINTFORML %CALLNAME%の頬をこねてみた
			PRINTFORML 「清磨？」
			PRINTFORML 戸惑った様子の%CALLNAME%にかまわず、%CALLNAME:MASTER%はひたすら無言で頬をこね続ける
			PRINTFORML 「そっちがその気なら……、」
			PRINTFORML %CALLNAME%も対抗してこちらの頬をこねてきた
			PRINTFORML 「お、おぉ　これは、なかなか……！」
			PRINTFORML %CALLNAME%は他人の頬をこねる快感に目覚めたようだ…
		CASE 7
			PRINTFORML %CALLNAME%の手を握ってみた
			PRINTFORML 「どうしたんだ　突然？」
			PRINTFORML 『なんだか、握ってみたくなったんだ』
			PRINTFORML 「そうか」
			PRINTFORML はにかんだ%CALLNAME%も、手を握り返してきた
		CASE 6
			PRINTFORML %CALLNAME%の手を握ってみた
			PRINTFORML 「清磨の手はあたたかいな」
			PRINTFORML 『水心子の手もあたたかいよ』
			PRINTFORML しばらくの間、手を握りあっていた
		CASE 5
			PRINTFORML %CALLNAME%を抱きしめてみた
			PRINTFORML 「清磨？」
			PRINTFORML 戸惑った様子を見せる%CALLNAME%を更に強く抱きしめると、%CALLNAME%も抱きしめ返してきた
			PRINTFORML しばらくの間、無言で抱きあっていた
		CASE 4
			PRINTFORML %CALLNAME%を抱きしめてみた
			PRINTFORML 「清磨？」
			PRINTFORML 不思議そうにしながらも、%CALLNAME%も抱きしめ返してきた
			PRINTFORML 『しばらく、このままでいい？』
			PRINTFORML 「……ああ」
			PRINTFORML %CALLNAME%が更に強く抱きしめ返すと、%CALLNAME:MASTER%も殊更に力を込めた
		CASE 3
			PRINTFORML %CALLNAME%を抱きしめてみた
			PRINTFORML そして、そのまま吸ってみた
			PRINTFORML 「ん？」
			PRINTFORML すぅー　はぁー
			PRINTFORML 「あ、あれ　もしかして吸ってないか？」
			PRINTFORML すうぅー　はあぁー
			PRINTFORML 「えっと……、………。」
			PRINTFORML しばらくの間、%CALLNAME%は無言で吸われていた
		CASE 2
			PRINTFORML %CALLNAME%を抱きしめてみた
			PRINTFORML 「清磨？」
			PRINTFORML 不思議そうな声をあげる%CALLNAME%
			PRINTFORML 『水心子成分の充電中』
			PRINTFORML 「ぷっ、あはは　充電って、なんだそれは　清磨は面白いことを言うな」
			PRINTFORML 笑いながらも%CALLNAME%は%CALLNAME:MASTER%を抱きしめ返してきた
		CASE 1
			PRINTFORML %CALLNAME%にマッサージ（健全）をしてみた
			PRINTFORMW うつ伏せになってもらい、凝ってそうな箇所をひたすら揉んでいく
			PRINTFORML 「っ…ん、ぅ……　は、っ…」
			PRINTFORMW ………。
			PRINTFORML 「あっ……　そこ…っ、……」
			PRINTFORMW ………………。
			PRINTFORML 「あ、ぁああ……　…は、ぁ…、いい……」
			PRINTFORML ………………………。
		CASEELSE
			PRINTFORML %CALLNAME%の黒子の数を数えてみた
			PRINTFORML 「そ、そんなところに黒子はない！」
			PRINTFORML 『見てみないと分からないだろう？　さぁ、脱いでほしいな』
	ENDSELECT
	PRINTW
ENDIF
RETURN LOCAL

;-------------------------------------------------
;COM203, まったりする FIRSTTIME(303)
;-------------------------------------------------
@KOJO_COM_K174_203
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	
	;地の文呼び出し(カラー変更対応)
	CALL TRAIN_MESSAGE_COLOR
	
	;怯え、反発なら弾く
	SIF (CFLAG:5005 & 64) || (CFLAG:5005 & 32)
		RETURN LOCAL
	
	PRINT しばらくの間、
	
	SELECTCASE RAND:3
		CASE 2
			IF !(TALENT:達磨 || TALENT:両腕欠損 || TALENT:PLAYER:達磨 || TALENT:PLAYER:両腕欠損) && RAND:2 == 0
				PRINT 手を握り合って
			ELSE
				PRINT 寄り添って
			ENDIF
		CASE 1
			PRINT 窓の外を眺めながら
		CASEELSE
			PRINT ベッドの上で
	ENDSELECT
	
	SELECTCASE RAND:4
		CASE 3
			PRINTL ゴロゴロしていた
		CASE 2
			PRINTL おしゃべりをして時間を過ごした
		CASE 1
			PRINTL うたた寝をした
		CASEELSE
			PRINTL ぼんやりと時間を過ごした
	ENDSELECT
	
ENDIF
RETURN LOCAL

