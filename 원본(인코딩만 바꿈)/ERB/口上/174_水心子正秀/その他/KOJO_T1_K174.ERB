;-------------------------------------------------
;A_16, 足を使った奉仕で射精
;-------------------------------------------------
@KOJO_MESSAGE_PALAMCNG_A_K174_16
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;地の文呼び出し(カラー変更対応)
	CALL PALAM_MESSAGE_A_COLOR
	
	;反発
	IF (CFLAG:5005 & 32)
		PRINTFORML 「………。」
		PRINTFORMW %CALLNAME%は嫌悪感に顔を歪めている
		RETURN LOCAL
	ENDIF

	;サド
	IF (CFLAG:5005 & 512)
		SELECTCASE RAND:5
			CASE 4
				PRINTFORML 「\@ TFLAG:16 == 2?こんなに#こんなので\@出して恥ずかしくないのか？」
				PRINTFORML 「……あぁ、恥ずかしくないから出してるんだったな」
				PRINTFORMW %CALLNAME%は見下した目でこちらを見ている
			CASE 3
				PRINTFORML 「ほら」
				PRINTFORML %CALLNAME%は精液で\@ TFLAG:16 == 2?べっとりと汚れた#汚れた\@足をこちらに差し出している
				PRINTFORMW どうやら舐めろと言っているようだ
			CASE 2
				PRINTFORML 「誰が出していいと言った？」
				PRINTFORML 「……少々、躾が必要だな」
				PRINTFORMW %CALLNAME%は射精したばかりの陰茎を、さらにしごき上げた
			CASE 1
				PRINTFORML 「そんなに気持ちよかったのか？」
				PRINTFORMW %CALLNAME%は射精したばかりの陰茎を足で弄びながら、こちらに問いかけている
			CASE 0
				PRINTFORML 「こんなのがいいとは……」
				PRINTFORMW %CALLNAME%はため息をつきながら、こちらを見ている
		ENDSELECT
		RETURN LOCAL
	ENDIF
	
	;ほか
	SELECTCASE RAND:3
		CASE 2
			PRINTFORML 「こ、こんなので出すとは……」
		CASE 1
			PRINTFORML 「うわ……」
		CASE 0
			;マゾ
			IF (CFLAG:5005 & 256)
				PRINTFORML 「いや　まあ、気持ちは分かるけど……」
			ELSE
				PRINTFORML 「いや　まあ、趣味は人それぞれだからな……」
			ENDIF
	ENDSELECT
	;妄執
	IF (CFLAG:5005 & 4)
		PRINTFORMW %CALLNAME%は少々顔を赤らめている
	ELSE
		PRINTFORMW %CALLNAME%は若干引いた目でこちらを見ている
	ENDIF
	
ENDIF
RETURN LOCAL

