;-------------------------------------------------
;奴隷の絶頂
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	PRINTL
	;地の文呼び出し(カラー変更対応)
	CALL PALAM_MESSAGE_B_COLOR
	
	;怯え
	IF (CFLAG:5005 & 64)
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「…やっ……、ぁ…、んっ、ぁ、っ……っ、い、…っ」
			CASE 1
				PRINTFORML 「ぁ…、ぃ、ひっ……、ぁ、ぁいっ、ぁ…ぁ……」
			CASE 0
				PRINTFORML 「ぐ、っ…、ぅ……うっ、ぁ…、や、、…っ、んんぁっ…」
		ENDSELECT
		
	;絶頂度4以上　ランダム
	ELSEIF TFLAG:29 >= 4 && RAND:2
		SELECTCASE RAND:6
			CASE 5
				PRINTFORML 「ああっ、す、…すごいのっ、きて…るっ、ぅ…んん…、っ、あっ、ひ、ぁあああ！」
			CASE 4
				PRINTFORML 「っ、また、ぁあっ　あ、っひ、ぁ　ああああっ…、ぁっ、また、ぁ…っ、っ、ぁああっ」
			CASE 3
				PRINTFORML 「あ…ぇ、えっ…、だ、め…っぇ…そん…っ、なっ、っあっ…や、あっ、っぁあ、っ！」
			CASE 2
				PRINTFORML 「ぁ…ああ、ぁあっ……も…っやめ、や…っし、しんじゃ…っ、～～～っ！」
			CASE 1
				PRINTFORML 「ひ、っぁああ…こん、な…ぁっ、あ、ぃ…っも、やっ…め……っ」
			CASE 0
				PRINTFORML 「ぁっ、や…っ、ぁん……ぃ、はぁっ、やめ…、あたま、…お、かしく…っぅ、んうぅっ」
		ENDSELECT
		PRINTFORML %CALLNAME%は生理的な涙で顔をぐしゃぐしゃにしながら、快感に悶えている…
		
	;反発蕩け　もしくは　反発+強絶頂
	ELSEIF (CFLAG:5005 & 128) || ((CFLAG:5005 & 32) && TFLAG:29 >= 2)
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「っも…は、な、……っ、せ、ぇっ…、んあ、ぁ……っ、あ、ぁ…」
			CASE 1
				PRINTFORML 「ぁ、やだ…、やめ…っ…、っ、ぁ……あ、ぁああっ」
			CASE 0
				PRINTFORML 「い、っ……、ぁ、いっ、ぎ…たく、……なっ、や、っ、あぁっ」
		ENDSELECT
		IF TFLAG:29 >= 4
			PRINTFORML %CALLNAME%は生理的な涙で顔をぐしゃぐしゃにしながら、襲い来る快感の波に悶えている…
		ELSE
			PRINTFORML %CALLNAME%は襲い来る快感の波に、懸命に耐えている…
		ENDIF
		
	;反発
	ELSEIF (CFLAG:5005 & 32)
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「ぅ…、…っん、ん……ぅ、んん、ん…っ…ぃ……、うっ、んんっ……」
			CASE 1
				PRINTFORML 「ん…、ぅ……っ、ぁ…ん……、んぅうう、ぐっ…、ぅっん…」
			CASE 0
				PRINTFORML 「あ……、ぅ、っんぅ…ぅ、ぐ、ぅうう……っ、っ、んぅ…」
		ENDSELECT
		PRINTFORML %CALLNAME%は襲い来る快感の波に歯を食いしばり、懸命に耐えている…
		
	;蕩け強　もしくは強絶頂
	ELSEIF (CFLAG:5005 & 16) || TFLAG:29 >= 2
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「やっ…、あ……、ぁっひ、んんっ…イっ…んっ、ぁ…っあ、あああっ」
			CASE 1
				PRINTFORML 「そこっ…、ぁ、ぃ…、ぃい…っ、ぁ……も、っイっ、ぅ……」
			CASE 0
				PRINTFORML 「だ、ぁ…っ、ぁあ…だめ…っ、イっ、て、ぇ…ん、ぁ……はあっ、んっ、ぃ…っ」
		ENDSELECT
		IF TFLAG:29 >= 4
			PRINTFORML %CALLNAME%は生理的な涙で顔をぐしゃぐしゃにしながら、快感に悶えている…
		ELSE
			PRINTFORML %CALLNAME%は生理的な涙を流しながら、快感に喘いでいる…
		ENDIF
		
	;蕩け弱　もしくは絶頂
	ELSEIF (CFLAG:5005 & 8) || TFLAG:29
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「も、ぅ……、っ、あ、あ、やっ……もっ、ん、イっ…っ、て…」
			CASE 1
				PRINTFORML 「ぁ…ぁ、は…あっ、あぁ…も、……っ、イっ、いぃ…」
			CASE 0
				PRINTFORML 「あ、っ…ぁぇ……っ、イ、っ、…、ぅ…んん……」
		ENDSELECT
		PRINTFORML %CALLNAME%は快感に喘いでいる…
	ENDIF
	
	;発情イベント
	IF (CFLAG:5004 & 4)
		;ペニスがある場合は射精を伴う場合に禁欲ポイントが減る
		;ここでは帰る
		SIF PENIS(TARGET)
			RETURN LOCAL
		
		;ペニスがないか、子供サイズの場合はＣ絶頂かＶ絶頂で禁欲ポイントが減る
		IF (!PENIS(TARGET) || TALENT:子供サイズ) && (NOWEX:0 || NOWEX:1) && (EX:0 == NOWEX:0) && (EX:1 == NOWEX:1)
			PRINTL 
			PRINTFORML 「はぁ……　はぁ……　あは……」
			PRINTFORML 待ち焦がれた絶頂に、%CALLNAME%は幸せそうに余韻に浸り、頬を緩ませている…
		ENDIF
	ENDIF
	
	PRINTFORMW 
	RETURN LOCAL
ENDIF


