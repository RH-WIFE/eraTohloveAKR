@KOJO_MESSAGE_PALAMCNG_B_K174_1
;-------------------------------------------------
;奴隷の初絶頂 FIRSTTIMR(61～65)
;-------------------------------------------------
;初めてＣ絶頂 FIRSTTIME(61)
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL &&  NOWEX:0 > 0 &&FIRSTTIME(61)
	;地の文呼び出し(カラー変更対応)
	CALL PALAM_MESSAGE_B_COLOR
	;自慰
	IF SELECTCOM == 4
		PRINTFORML 「あ、…っ、ぁん…な、にか…、きて…っ、…ぁ、…んんっ！」
	ELSE
