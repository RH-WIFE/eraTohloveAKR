;=================================================
;調教者の射精時のテキスト
;=================================================
;-------------------------------------------------
;射精箇所変更時のテキスト　有効時はここで関数終了　CFLAG:221～230
;-------------------------------------------------
IF CFLAG:106
	CALL PALAM_MESSAGE_A
	;髪
	IF CFLAG:106 == 1
		;陥落済み+精液中毒3Lもしくは奉仕精神5L
		IF (ABL:奉仕精神 >= 5 || ABL:精液中毒 >= 3) && KANRAKU(TARGET)
			PRINTFORML 「かみくさ…、くさいのにっ%UNICODE(0x2661)%　どうし…て…%UNICODE(0x2661)%」
			PRINTFORMW %CALLNAME%は髪を汚されたというのに興奮を隠しきれない様子だ
		;それ以外
		ELSE
			PRINTFORMW %CALLNAME%は髪伝いに落ちてくる白濁に絶句している
		ENDIF
		CFLAG:221 = 1

	;顔
	ELSEIF CFLAG:106 == 2
		;陥落済み+精液中毒3Lもしくは奉仕精神5L
		IF (ABL:奉仕精神 >= 5 || ABL:精液中毒 >= 3) && KANRAKU(TARGET)
			IF RAND:2 == 0
				PRINTFORML 「ははっ　あぁくさい…%UNICODE(0x2661)%」
			ELSE
				PRINTFORML 「どろどろでくさくて　ほんと…%UNICODE(0x2661)%」
			ENDIF
			PRINTFORMW %CALLNAME%は息を荒げながら、顔にかかった精液に触れている
		;それ以外
		ELSE
			PRINTFORMW %CALLNAME%は顔にかかった精液を必死になってぬぐっている
		ENDIF
		
		CFLAG:222 = 1

	;胸
	ELSEIF CFLAG:106 == 3
		;陥落済み+精液中毒3Lもしくは奉仕精神5L
		IF (ABL:奉仕精神 >= 5 || ABL:精液中毒 >= 3) && KANRAKU(TARGET)
			;上衣、全身上着、下着（上）を着用
			IF TOPS(TARGET) || TEQUIP:上衣 || TEQUIP:下着（上）
				IF RAND:2 == 0
					PRINTFORML 「服　べたべたに…っ%UNICODE(0x2661)%」
				ELSE
					PRINTFORML 「まったく%UNICODE(0x2661)%　服をこんな　べたべたにして…っ　どうしてくれるのかな%UNICODE(0x2661)%」
				ENDIF
			;上着てない
			ELSE
				PRINTFORML 「ぬるぬるしてて　くさい…%UNICODE(0x2661)%」
			ENDIF
			PRINTFORMW %CALLNAME%の顔は恍惚を浮かべている
		;それ以外
		ELSE
			;上衣、全身上着、下着（上）を着用
			IF TOPS(TARGET) || TEQUIP:上衣 || TEQUIP:下着（上）
				PRINTFORMW %CALLNAME%は服が張り付く感覚に露骨な不快感を示している
			;上着てない
			ELSE
				;従順3以上か陥落済み
				IF ABL:従順 >= 3 || (KANRAKU(TARGET) && KANRAKU(TARGET) < 4)
					;PRINTFORMW 
				ELSE
					PRINTFORMW %CALLNAME%は顔をしかめている
				ENDIF
			ENDIF
		ENDIF
		CFLAG:223 = 1

	;腹
	ELSEIF CFLAG:106 == 4
		CFLAG:224 = 1

	;腋
	ELSEIF CFLAG:106 == 5
		CFLAG:225 = 1

	;太腿
	ELSEIF CFLAG:106 == 6
		CFLAG:226 = 1

	;割れ目
	ELSEIF CFLAG:106 == 7
		;陥落済み+精液中毒3Lもしくは奉仕精神5L
		IF (ABL:奉仕精神 >= 5 || ABL:精液中毒 >= 3) && KANRAKU(TARGET)
			IF RAND:3 == 0
				;ズボン系の全身上着か下着着用
				IF BOTTOMS(TARGET) >= 4 || TEQUIP:下着（下）
					PRINTFORML 「こんな…　漏らしたみたいな%UNICODE(0x2661)%」
				;はいてない
				ELSE
					PRINTFORML 「どろどろで　汚いのに%UNICODE(0x2661)%　どうして俺は…っ%UNICODE(0x2661)%」
				ENDIF
			ELSEIF RAND:2 == 0
				PRINTFORML 「%UNICODE(0x2661)%　においだけで　どうしてこんな…%UNICODE(0x2661)%」
			ELSE
				PRINTFORML 「あつくて%UNICODE(0x2661)%　濃厚で%UNICODE(0x2661)%　こんなの出されたら…」
			ENDIF
			PRINTFORMW %CALLNAME%は股の生暖かい感触に身体をむずむずさせている
		;陥落済み
		ELSEIF KANRAKU(TARGET)
			PRINTFORMW %CALLNAME%は放たれた精液に顔を赤らめている
		;それ以外
		ELSE
			PRINTFORMW %CALLNAME%は放たれた精液に侮蔑を込めた視線を送っている
		ENDIF
		CFLAG:227 = 1

	;尻
	ELSEIF CFLAG:106 == 8
		;陥落済み+精液中毒3Lもしくは奉仕精神5L
		IF (ABL:奉仕精神 >= 5 || ABL:精液中毒 >= 3) && KANRAKU(TARGET)
			IF RAND:4 == 0
				PRINTFORML 「ぬるぬるなのに、気持ち悪いのに…っ%UNICODE(0x2661)%」
			ELSEIF RAND:3 == 0
				PRINTFORML 「こんなにおいでっ　どうしてこんな%UNICODE(0x2661)%」
			ELSEIF RAND:2 == 0
				PRINTFORML 「くさいのに　濃厚な…っ%UNICODE(0x2661)%　こんなの出されたら…」
			ELSE
				PRINTFORML 「どろどろで　汚いのに、どうして俺はこんなに…%UNICODE(0x2661)%」
			ENDIF
			PRINTFORMW %CALLNAME%は臀部のぬるぬるした感触に身体をむずむずさせている
		;それ以外
		ELSE
			PRINTFORMW 臀部の感触に%CALLNAME%は顔をこわばらせている
		ENDIF
		CFLAG:228 = 1

	;ゴム
	ELSEIF CFLAG:106 == 9
		;セックスで射精
		IF SEXPLAY(TARGET) == 1
			;陥落済み+精液中毒3Lもしくは奉仕精神5L
			IF (ABL:奉仕精神 >= 5 || ABL:精液中毒 >= 3) && KANRAKU(TARGET) && KANRAKU(TARGET) < 4
				IF RAND:2 == 0
					PRINTFORML 「っ…どうしてゴムに……　あぁ、濃厚な…%UNICODE(0x2661)%　のが、こんなに…%UNICODE(0x2661)%」
				ELSE
					PRINTFORML 「ゴムのにおいと混ざって　余計くさい…のに%UNICODE(0x2661)%」
				ENDIF
				PRINTFORMW %CALLNAME%はたっぷりと満たされたコンドームを前にうっとりとしている
			;陥落済み
			ELSEIF KANRAKU(TARGET)
				PRINTFORMW 顔を赤くした%CALLNAME%は白濁でたっぷりと満たされたコンドームから目を背けている
			;それ以外
			ELSE
				PRINTFORML %CALLNAME%は嫌悪感でいっぱいになりつつも、中に直接出されなかったことに安堵している様子だ
			ENDIF
			CFLAG:229 = 2
		;アナルセックスで射精
		ELSEIF SEXPLAY(TARGET) == 2
			;陥落済み+精液中毒3Lもしくは奉仕精神5L
			IF (ABL:奉仕精神 >= 5 || ABL:精液中毒 >= 3) && KANRAKU(TARGET) && KANRAKU(TARGET) < 4
				IF RAND:2 == 0
					PRINTFORML 「ゴムに…　でも濃いぃ%UNICODE(0x2661)%　のが、こんなに…っ%UNICODE(0x2661)%」
				ELSE
					PRINTFORML 「ゴムのにおいと混ざって　余計くさい…のに%UNICODE(0x2661)%」
				ENDIF
				PRINTFORMW %CALLNAME%は白濁でたっぷりと満たされたコンドームを前にうっとりとしている
			;陥落済み
			ELSEIF KANRAKU(TARGET)
				PRINTFORMW 顔を赤くした%CALLNAME%はたっぷりと満たされたコンドームから目を背けている
			;それ以外
			ELSE
				PRINTFORMW %CALLNAME%は嫌悪感を隠そうともしていない
			ENDIF
			CFLAG:229 = 1
		;それ以外を実行時
		ELSE
			;陥落済み+精液中毒3Lもしくは奉仕精神5L
			IF (ABL:奉仕精神 >= 5 || ABL:精液中毒 >= 3) && KANRAKU(TARGET) && KANRAKU(TARGET) < 4
				PRINTFORML 「どろどろが%UNICODE(0x2661)%　どろどろがたくさん…%UNICODE(0x2661)%」
				PRINTFORMW %CALLNAME%は白濁でたっぷりと満たされたコンドームを前にうっとりとしている
			;陥落済み
			ELSEIF KANRAKU(TARGET)
				PRINTFORMW 顔を赤くした%CALLNAME%はたっぷりと満たされたコンドームから目を背けている
			;それ以外
			ELSE
				PRINTFORMW %CALLNAME%は吐きそうな顔をしている
			ENDIF
			CFLAG:230 = 1
		ENDIF
		
	ENDIF
	RETURN 0
ENDIF

;-------------------------------------------------
;手、口以外で射精　CFLAG:231
;TFLAG:9 == １は射精、２は大量射精
;対応コマンドはキス、素股、足コキ、アナル強制奉仕、ダブル素股
;-------------------------------------------------
IF TFLAG:9 == 1 || TFLAG:9 == 2
	CALL PALAM_MESSAGE_A
	CFLAG:231 = 1
ENDIF

;-------------------------------------------------
;助手を犯すで射精　CFLAG:232
;-------------------------------------------------
IF TFLAG:7 > 0
	CALL PALAM_MESSAGE_A
ENDIF

;-------------------------------------------------
;口で射精　CFLAG:233
;-------------------------------------------------
IF TFLAG:0 > 0
	CALL PALAM_MESSAGE_A
	;陥落済み+精液中毒3Lもしくは奉仕精神5L
	IF (ABL:奉仕精神 >= 5 || ABL:精液中毒 >= 3) && KANRAKU(TARGET)
		IF RAND:3 == 0
			PRINTFORML 「んっ…んぐ…っ、んちゅ…　ぅ…んっ　おいし…%UNICODE(0x2661)%」
		ELSEIF RAND:2 == 0
			PRINTFORML 「%UNICODE(0x2661)%　こんな…、ぁあ…濃くてくさい…っ%UNICODE(0x2661)%」
		ELSE
			PRINTFORML 「汚いのに…どろどろなのに…っ、うまい…な　んて…ぇ%UNICODE(0x2661)%」
		ENDIF
		PRINTFORMW %CALLNAME%は放たれた精液を味わうように咀嚼し飲み込んでいる
	;陥落済み
	ELSEIF KANRAKU(TARGET)
		IF RAND:2 == 0
			PRINTFORML 「くさい…」
		ELSE
			PRINTFORML 「んぐっ　うぅ…　んっ、ぷはっ」
		ENDIF
		PRINTFORMW %CALLNAME%は放たれた精液をなんとか飲み込んでいる
	;それ以外
	ELSE
		IF RAND:2 == 0
			PRINTFORML 「…気持ち悪い」
		ELSE
			PRINTFORML 「…汚い」
		ENDIF
		PRINTFORMW 放たれた精液に%CALLNAME%はえずき、咳き込んでいる
	ENDIF
	CFLAG:233 = 1
ENDIF

;-------------------------------------------------
;手で射精　CFLAG:234
;-------------------------------------------------
IF TFLAG:1 > 0
	CALL PALAM_MESSAGE_A
	;陥落済み+精液中毒3Lもしくは奉仕精神5L
	IF (ABL:奉仕精神 >= 5 || ABL:精液中毒 >= 3) && KANRAKU(TARGET)
		IF RAND:2 == 0 && KANRAKU(TARGET) < 4
			PRINTFORMW 「見えるかな　%CSTR:51%ので手がこんなになってしまったよ%UNICODE(0x2661)%」
		ELSE
			PRINTFORML 「まったく……%UNICODE(0x2661)%」
		ENDIF
		PRINTFORMW %CALLNAME%は手についた精液で、にちゃにちゃと音を立てている
	;陥落済み
	ELSEIF KANRAKU(TARGET)
		PRINTFORML 「うわ　どろどろ……」
		PRINTFORMW %CALLNAME%は手についた精液を見て眉根を寄せた
	;それ以外
	ELSE
		PRINTFORML 「…はあ」
		PRINTFORMW %CALLNAME%はため息をつくと、手についた精液を床に擦りつけぬぐっている
	ENDIF
	CFLAG:234 = 1
ENDIF

;-------------------------------------------------
;胸で射精　CFLAG:235
;-------------------------------------------------
IF TFLAG:34 > 0
	CALL PALAM_MESSAGE_A
	;陥落済み+精液中毒3Lもしくは奉仕精神5L
	IF (ABL:奉仕精神 >= 5 || ABL:精液中毒 >= 3) && KANRAKU(TARGET)
		IF RAND:2 == 0
			;胸が母乳で汚れているかどうか
			IF STAIN:胸 & 16
				PRINTFORMW 「母乳と混ざって%UNICODE(0x2661)%　よけい　どろどろ…%UNICODE(0x2661)%」
			ELSE
				PRINTFORMW 「%UNICODE(0x2661)%　母乳でも出したみたいで…%UNICODE(0x2661)%」
			ENDIF
		ELSE
			PRINTFORMW 「ぬるぬる…%UNICODE(0x2661)%」
		ENDIF
	;陥落済み
	ELSEIF KANRAKU(TARGET)
		IF RAND:3 == 0
			;絶壁やオトコ
			IF !BREAST(TARGET)
				PRINTFORMW 「こんな真っ平らのどこがいいのかな」
			ELSE
				PRINTFORMW 「そんなにきもちいいのかな」
			ENDIF
			PRINTFORMW %CALLNAME%は不思議そうにぺたぺた胸を触っている
		ELSEIF RAND:2 == 0
			;胸が母乳で汚れているかどうか
			IF STAIN:胸 & 16
				PRINTFORMW 「母乳と混ざってぬるぬるだ…」
			ELSE
				PRINTFORMW 「母乳でも出たみたいな…」
			ENDIF
		ELSE
			PRINTFORMW 「べたべただ…　ははっ」
		ENDIF
	;それ以外
	ELSE
		
	ENDIF
	CFLAG:235 = 1
ENDIF

;-------------------------------------------------
;足で射精　CFLAG:236
;-------------------------------------------------
IF TFLAG:16 > 0
	CALL PALAM_MESSAGE_A
	;陥落済み+サドっ気5以上
	IF ABL:サドっ気 >= 5 && KANRAKU(TARGET)
		;大量射精で分岐追加
		IF RAND:5 == 0 && TFLAG:16 >= 2
			PRINTFORML 「踏まれてこんなに射精するなんて救いようがないな」
			PRINTFORML 「俺の足はそんなに良かったのか？　…気持ち悪い」
			PRINTFORMW %CALLNAME%は%CALLNAME:PLAYER%を侮蔑を含んだ視線を向けている
		ELSEIF RAND:4 == 0 && TFLAG:16 >= 2
			PRINTFORML 「うわ　どろどろ…　俺の足を孕ませるつもりかな？」
			PRINTFORML 「こんなとこに射精しても無駄打ちでしかないのに」
			PRINTFORML %CALLNAME%は鼻でせせら笑った
		
		ELSEIF RAND:3 == 0
			PRINTFORML 「おい　なに勝手に気持ちよくなってるんだ」
			PRINTFORML 「悪い子にはおしおきしないといけないな」
			PRINTFORMW %CALLNAME%は%CALLNAME:PLAYER%のペニスをかかとで踏みつけた
		ELSEIF RAND:2 == 0
			PRINTFORML 「そうかそうか　%CSTR:51%は足で喜ぶ変態くんだったか」
			PRINTFORML 「なら　これはご褒美だよ」
			PRINTFORMW %CALLNAME%は射精したばかりの陰茎をさらにしごきあげた
		ELSE
			PRINTFORML 「足が汚れた　どうしてくれるのかな」
			PRINTFORML 「ほら　舐めて綺麗にするんだよ」
			PRINTFORMW %CALLNAME%は汚れた足をこちらに差し出した
		ENDIF
	;陥落済み
	ELSEIF KANRAKU(TARGET)
		PRINTFORML 「足なんかでっ…」
		PRINTFORMW %CALLNAME%は汚された足を見て顔を赤らめている
	;それ以外
	ELSE
		PRINTFORML 「………。」
		PRINTFORMW %CALLNAME%は冷めた目でこちらを見ている
	ENDIF
	CFLAG:236 = 1
ENDIF

