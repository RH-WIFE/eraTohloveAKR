A = 6
REPEAT 12
	B = COUNT + 1
	F = COUNT
	
	;非表示のとき非表示
	SIF CFLAG:121 & 1
		CONTINUE
	
	;カソック:長谷部以外は選択不可
	SIF B == 8 && C != 118
