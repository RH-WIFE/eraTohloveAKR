PRINTL □ 上衣 ／ 下衣 □---------------------------------------------------------
E = 0
A = 5
REPEAT 25
	B = COUNT + 1
	F = COUNT
	
	;非表示のとき非表示
	SIF CFLAG:121 & 2
		CONTINUE
	
	SIF B >= 23 && B <= 24
		CONTINUE
	
	;ウエディングドレス：
	;アイテム持ってないとダメ
	;ターゲットが恋慕でも親愛でもないとダメ
	;助手調教中はダメ
	SIF B == 4 && (ITEM:39 == 0 || !TALENT:恋慕 || !TALENT:親愛 || ASSIPLAY)
		CONTINUE
	;ボンテージスーツ:
	;主人、助手、調教対象のいずれかがマゾorサド、もしくは、
	;主人、助手、調教対象のいずれかが禁断の知識を持ってないとダメ
	SIF B == 10 && (G == 0 && I == 0)
		CONTINUE
	;狩衣:オトコじゃないと選択不可
	SIF B == 10 && MALE(TARGET) == 0
		CONTINUE
	
	;既にその衣装を着てれば色を分ける
	IF TEQUIP:A == B
		;女物はピンク
		IF B == 1 || B == 2 || B == 3 || B == 4 || B == 5 || B == 6 || B == 9 || B == 12
			SETCOLOR 0xFF9999
		;男物は水色
		ELSEIF B == 17
			SETCOLOR 0x6699FF
		;共用はキャラの性別に合わせる
		ELSE
			IF MALE(TARGET)
				SETCOLOR 0x6699FF
			ELSE
				SETCOLOR 0xFF9999
			ENDIF
		ENDIF
	ENDIF

	;↓以下は共通の制限
	CALL CLOTHES_LIST2
	CALL CLOTHES_LIST3
	
