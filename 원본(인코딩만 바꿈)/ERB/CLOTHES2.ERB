	B = COUNT + 1
	F = COUNT
	
	;非表示のとき非表示
	SIF CFLAG:121 & 256
		CONTINUE
	
	;アイマスク：アイテム持ってないとダメ
	SIF B == 2 && ITEM:15 == 0
		CONTINUE
