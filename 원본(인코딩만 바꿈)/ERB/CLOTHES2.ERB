;====================================================
;ここから衣装のあるなし設定。購入条件とか
;====================================================
@CLOTHES_LIST
;主人、助手、調教対象のサド・マゾを数えてみる
G = TALENT:MASTER:サド + TALENT:MASTER:マゾ + TALENT:サド + TALENT:マゾ
SIF ASSI > -1
	G += TALENT:ASSI:サド + TALENT:ASSI:マゾ
;主人、助手、調教対象の禁断の知識を数えてみる
I = TALENT:MASTER:禁断の知識 + TALENT:禁断の知識
SIF ASSI > -1
	I += TALENT:ASSI:禁断の知識
;陥落キャラ数を数えてみる
J = FLAG:30 + FLAG:31 + FLAG:32
;陥落キャラ数のうち女の子の人数を数えてみる
K = 0
REPEAT CHARANUM
	SIF CFLAG:COUNT:41 && MALE(COUNT) == 0
		K += 1
REND
;助手の番号を記録
C:1 = -1
SIF ASSI >= 0
	C:1 = NO:ASSI

;----------------------------------------------------
;頭部装飾
;----------------------------------------------------
RESETCOLOR
PRINTL □ 頭装飾 ／ 顔装飾 □-----------------------------------------------------
A = 7
REPEAT 9
	B = COUNT + 1
	F = COUNT
	X = 30 + B
	
	;非表示のとき非表示
	SIF CFLAG:121 & 128
		CONTINUE
	
	;耳アイテム：アイテム持ってないとダメ
	;デフォで動物耳持ちのキャラだとダメ
	IF B < 4
		;着衣設定が通常衣装のいずれか　キャラが五月雨江/村雲江　の場合はいぬみみを表示
		IF FLAG:1 >= 0 && FLAG:0 <= 6 && (C == 192 || C == 200) && B == 2 
		
		;鳴狐、の場合はいぬみみ（きつねみみ）を表示
		ELSEIF C == 23 || C == 24
		
		ELSE
			SIF ITEM:X == 0
				CONTINUE
			SIF TALENT:動物耳 && TALENT:動物耳 <= 10
				CONTINUE
		ENDIF
	ENDIF
	;ヘッドセット：アイテム持ってないとダメ
	SIF B == 4 && ITEM:34 == 0
		CONTINUE
	;ウエディングドレス：アイテム持ってないとダメ
	;ターゲットが恋慕でも親愛でもないとダメ
	;助手調教中はダメ
	SIF B == 5 && (ITEM:39 == 0 || !TALENT:TARGET:恋慕 || !TALENT:TARGET:親愛 || ASSIPLAY)
		CONTINUE
	
	;既にその衣装を着てれば色を分ける
