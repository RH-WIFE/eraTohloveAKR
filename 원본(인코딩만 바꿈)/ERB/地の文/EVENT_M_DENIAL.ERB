;実行判定不可のときのテキスト
;-------------------------------------------------
;地の文呼び出し
;-------------------------------------------------
@DENIAL_MESSAGE
RESULT = 0
TRYCALLFORM DENIAL_MESSAGE_{SELECTCOM}
SIF !RESULT
	CALL DENIAL_MESSAGE_EX

;-------------------------------------------------
;汎用
;-------------------------------------------------
@DENIAL_MESSAGE_EX
LOCALS = 
SELECTCASE SELECTCOM
	CASE 1
		LOCALS = %CALLNAME%にキスする
	CASE 3
		LOCALS = %CALLNAME%に秘貝開帳させる
	CASE 4
		LOCALS = %CALLNAME%に自慰させる
	CASE 5
		LOCALS = %CALLNAME%に胸自慰させる
	CASE 6
		LOCALS = %CALLNAME%にアナニーさせる
	CASE 7
		LOCALS = %CALLNAME%に両穴オナニーさせる
	;手淫、フェラチオ、パイズリ、縦パイズリ、パイズリフェラ、指チュパ、足コキ、腋コキ、授乳手コキ
	CASE 70,71,73,74,75,77,80,82,528
		LOCALS = %CALLNAME%に%STR:0%させる
	CASE 85
		LOCALS = %CALLNAME%に足舐めさせる
	CASE 86
		LOCALS = %CALLNAME%に玉揉みさせる
	CASE 87
		LOCALS = %CALLNAME%に玉舐めさせる
	;セックス、Ａセックス
	CASE 100 TO 116
		LOCALS = %CALLNAME%に%STR:0%させる
	CASE 150
		LOCALS = %CALLNAME%を家具にする
	CASE 181
		LOCALS = %CALLNAME%を肉便器にする
	;飲尿させる、食糞させる
	CASE 184,185
		LOCALS = %CALLNAME%に%STR:0%
	CASE 206
		LOCALS = %CALLNAME%にプレゼントさせる
	;愛撫させる、胸を揉ませる、指挿入れさせる、アナル愛撫させる
	CASE 220,221,223,225
		LOCALS = %CALLNAME%に%STR:0%
	CASE 222
		LOCALS = %CALLNAME%にクンニさせる
	CASE 224
		LOCALS = %CALLNAME%にアナル舐めさせる
	;乳の揉み合い、貝合わせ、兜合わせ
	CASE 243,244,245
		LOCALS = %CALLNAME%と%STR:0%する
	;主人受けセックス
	CASE 260 TO 265
		LOCALS = %CALLNAME%に%STR:0%
	;イラマチオさせる、罵ってもらう、叩いてもらう、鞭で打ってもらう
	CASE 280,281,283,284
		LOCALS = %CALLNAME%に%STR:0%
	CASE 314
		LOCALS = %CALLNAME%の毛を剃る
	CASE 317
		LOCALS = %CALLNAME%に泡踊りさせる
	;ダブルフェラ、ダブルパイズリ、ダブル素股
	CASE 420 TO 422
		LOCALS = %CALLNAME%に%STR:0%させる
	;助手を犯させる
	CASE 430,431
		LOCALS = %CALLNAME%に%STR:0%
	CASEELSE 
		LOCALS = %CALLNAME%に%STR:0%
ENDSELECT

IF STRLENS(LOCALS)
	PRINTFORML %LOCALS%には、もう少し調教が必要なようだ
ELSE
	PRINTFORML %CALLNAME%に%STR:0%させるには、もう少し調教が必要なようだ
ENDIF



