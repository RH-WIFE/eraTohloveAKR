;-------------------------------------------------
;1,キスする
;-------------------------------------------------
@MESSAGE_COM1
IF TALENT:気絶
	PRINTFORML %CALLNAME:MASTER%のキスで%CALLNAME%は意識を取り戻したようだ…
	RETURN 0
ENDIF

;逆調教モード
IF TEQUIP:逆調教
	PRINTFORML %CALLNAME%は%CALLNAME:PLAYER%に近づき、唇を重ねてきた…
;通常モード
ELSE
	;助手が陥落対象
	IF ASSI > -1 && MASTER_SEARCH2(TARGET,ASSI)
		;主人調教中
		IF MASTER == PLAYER
			;主人の陥落対象が助手か奴隷
			IF MASTER_SEARCH2(MASTER,ASSI) || MASTER_SEARCH2(MASTER,TARGET)
				PRINTFORML %CALLNAME:MASTER%は%CALLNAME:ASSI%に見せつけるようにして%CALLNAME%の唇をむさぼった…
			ELSE
				PRINTFORML %CALLNAME:ASSI%の目を気にする%CALLNAME%の唇を奪い、欲望のままにむさぼった…
			ENDIF
		;助手調教中
		ELSE
			PRINTFORML %CALLNAME%は\@ KANRAKU(TARGET) == 4 ? 愛しそう # 嬉しそう \@に%CALLNAME:PLAYER%にすり寄り、
			PRINTFORML 積極的に舌を絡めあっている…
			;主人の陥落対象が助手か奴隷
			SIF MASTER_SEARCH2(MASTER,ASSI) || MASTER_SEARCH2(MASTER,TARGET)
				PRINTFORML その様子は傍から見ていて嫉妬心を覚えるほどだ…
		ENDIF
	;助手調教中
	ELSEIF ASSI > -1 && ASSI == PLAYER
		PRINTFORML %CALLNAME:PLAYER%は%CALLNAME%の顔に近づき、唇に唇を重ねた…
	ELSE
		IF KANRAKU(TARGET) > 0 && KANRAKU(TARGET) < 4
			PRINTFORML %CALLNAME:PLAYER%が%CALLNAME%の顔に唇を近付けると、
			PRINTFORM %CALLNAME%は\@ BIND_CHECK_B(5) ? 身体を擦りよせ # 首に手をまわして抱きつき \@
			SELECTCASE KANRAKU(TARGET)
				CASE 1
					PRINTL 唇に唇を重ねた…
				CASE 2
					PRINTL 情熱的に舌を絡めてきた…
				CASEELSE
					PRINTL キスに応えた…
			ENDSELECT
		ELSE
			PRINTFORM %CALLNAME%の口腔を隅々まで舐め回
			;従順LV3以上＋ゲイ経験2000以上
			IF ABL:従順 >= 3 && EXP:ゲイ経験 >= 2000
				PRINTFORM し、舌を絡めあいながら唾液を交換した…
			;従順LV3以上
			ELSEIF ABL:従順 >= 3
				IF TALENT:反抗的 || TALENT:臆病 || TALENT:スキンシップ苦手 || TALENT:恥じらい || TALENT:貞操観念 || MARK:反発刻印
					PRINTFORM すと、%CALLNAME%もおずおずと舌を絡めてきた…
				ELSE
					PRINTFORM すと、%CALLNAME%も積極的に舌を絡めてきた…
				ENDIF
			ELSE
				PRINTFORM した…
			ENDIF
		ENDIF
	ENDIF
ENDIF


