SIF UP:快Ｃ + PALAM:快Ｃ - DOWN:快Ｃ >= PALAMLV:4
	DOWN:快Ｃ = UP:快Ｃ + PALAM:快Ｃ - PALAMLV:4 + 1
SIF UP:快Ｖ + PALAM:快Ｖ - DOWN:快Ｖ >= PALAMLV:4
	DOWN:快Ｖ = UP:快Ｖ + PALAM:快Ｖ - PALAMLV:4 + 1
