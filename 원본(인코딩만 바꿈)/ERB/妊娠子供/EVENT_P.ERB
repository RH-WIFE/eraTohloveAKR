	PRINTFORML %CALLNAME:ARG%は子供の様子を見にやってきた
PRINTL どの部屋へ様子を見に行きますか？
VARSET LOCAL
IF ARG:1
	LOCAL:5 = MASTER
	MASTER = ARG
	CALL MASTER_CHANGE_OFF
	CALL MASTER_CHANGE_ON
ENDIF
REPEAT CHARANUM
	IF COUNT == MASTER
		CONTINUE
	;現在臨月か[育児中]のキャラを羅列、一度訪れたら消す
	ELSEIF !LOCAL:(10 + COUNT) && ((TALENT:COUNT:妊娠 && (CFLAG:COUNT:110 - 3) <= DAY) || TALENT:COUNT:育児中)
		;育児中の場合、育てている子供を検索　実子を優先して表示する
		IF TALENT:COUNT:育児中
			FOR LOCAL, 1, CHARANUM
				IF CFLAG:LOCAL:115 == NO:COUNT && CFLAG:LOCAL:116
					;LOCAL:1にまだ実子が入ってなければ子供の登録番号を入れる
					SIF !LOCAL:2
						LOCAL:1 = LOCAL
					;子供が実子の場合、実子フラグを立てる
					IF CFLAG:COUNT:114 == NO:LOCAL
						LOCAL:2 += 1
					;子供が養子の場合、養子フラグを立てる
					ELSE
						LOCAL:3 += 1
					ENDIF
				ENDIF
			NEXT
		ENDIF
		PRINTFORM [{COUNT,2}]%CALLNAME:COUNT%
		IF TALENT:COUNT:育児中
			PRINTFORML と%CALLNAME:(LOCAL:1)%\@ LOCAL:2 + LOCAL:3 >= 2 ? たち # \@
		ELSE
			PRINTL 
		ENDIF
	ENDIF
REND
PRINTFORML [1000]戻る

$INPUT_LOOP
INPUT
IF RESULT == 1000
	IF ARG:1
		MASTER = LOCAL:5
		CALL MASTER_CHANGE_OFF
		CALL MASTER_CHANGE_ON
	ENDIF
	RETURN 0
ELSEIF RESULT < 0 || RESULT >= CHARANUM
	GOTO INPUT_LOOP
ELSEIF RESULT == MASTER
	GOTO INPUT_LOOP
ELSEIF !TALENT:RESULT:妊娠 && (CFLAG:RESULT:110 - 3) <= DAY && !TALENT:RESULT:育児中
	GOTO INPUT_LOOP
ELSE
	;育児中の場合、育てている子供を検索　実子を優先して表示する
	LOCAL = 0
	LOCAL:1 = 0
	LOCAL:2 = 0
	LOCAL:3 = 0
	IF TALENT:RESULT:育児中
		FOR LOCAL, 1, CHARANUM
			IF CFLAG:LOCAL:115 == NO:RESULT && CFLAG:LOCAL:116
				;LOCAL:1にまだ実子が入ってなければ子供の登録番号を入れる
				SIF !LOCAL:2
					LOCAL:1 = LOCAL
				;子供が実子の場合、実子フラグを立てる
				IF CFLAG:RESULT:114 == NO:LOCAL
					LOCAL:2 += 1
				;子供が養子の場合、養子フラグを立てる
				ELSE
					LOCAL:3 += 1
				ENDIF
			ENDIF
		NEXT
	ENDIF
	PRINTFORM %CALLNAME:MASTER%は%CALLNAME:RESULT%
	SIF TALENT:RESULT:育児中
		PRINTFORM と%CALLNAME:(LOCAL:1)%\@ LOCAL:2 + LOCAL:3 > 1 ? たち # \@
	PRINTL のところへ向かった
	PRINTW 
	;訪問済み
	LOCAL:(10 + RESULT)++
	;育児室のイベント口上
	A = TARGET
	TARGET = RESULT
	TFLAG:13 = 13
	CALL KOJO_MESSAGE_EVENT
	TARGET = A
	PRINTL 
	RESTART
ENDIF


RETURN 1


