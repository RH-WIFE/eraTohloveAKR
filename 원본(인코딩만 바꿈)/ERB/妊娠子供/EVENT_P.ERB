	MAXBASE:PREGNANT:体力 += 500
	TALENT:PREGNANT:育児中 = 0
	IF !TALENT:PREGNANT:貧乳 && !TALENT:PREGNANT:絶壁
		SIF BREAST(PREGNANT)
			PRINTFORMW %CALLNAME:PREGNANT%の胸は母乳を与える役割を終えたことで小さくなりました
		;妊娠時に大きくなった胸のサイズを一段小さくする
		CALL REVERSE_B_SIZE
	ENDIF
	CALL CLEAR_FLAG
ENDIF

