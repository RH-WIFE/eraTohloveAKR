			LOCAL:4 = COUNT
	REND
	;親離れする子供に対する相性をリセット
	IF !LOCAL:4
		REPEAT CHARANUM
			RELATION:COUNT:(NO:ASSI) = 0
		REND
	ENDIF
	DELCHARA LOCAL:2
