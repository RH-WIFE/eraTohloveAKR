CFLAG:PREGNANT:105 = 0
CFLAG:PREGNANT:110 = 0
CFLAG:PREGNANT:111 = 0
CFLAG:PREGNANT:114 = 0
CFLAG:PREGNANT:702 = 0
CFLAG:PREGNANT:703 = 0
