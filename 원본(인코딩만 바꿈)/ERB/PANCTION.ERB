	;調教したキャラを保存
	CFLAG:M:719 = NO:TARGET
	;口上主が助手として前回調教したキャラ番号をリセット
	;助手として入った直後でなく間を空けても口上を変える場合は↓の行をコメントアウト
	CFLAG:M:717 = 0
ENDIF
;助手がいる場合面識判定を更新
IF ASSI > 0
	;口上主が助手の場合
	IF NO:ASSI == 1
		A = 650+NO:TARGET
		SIF CFLAG:ASSI:A <= 1
			CFLAG:ASSI:A = 2
		;口上主が助手として前回調教したキャラ番号として保存
		CFLAG:ASSI:717 = NO:TARGET
	;口上主が調教対象の場合
	ELSEIF NO:TARGET == 1
		A = 650+NO:ASSI
		SIF CFLAG:TARGET:A <= 1
			CFLAG:TARGET:A = 2
	ENDIF
ENDIF
RETURN 0


;--------------------------------------------------
;助手プレイ内容保存関数@ASSIDATA
;対象の@KOJO_MESSAGE_COM_ASSI_X1の下でCALL
;対象が助手として調教にどの程度関わったかをCFLAG:211～215に保存します。
;211（前回の調教で調教者として一度以上調教コマンドを実行した）
;212（1=助手として奴隷の処女を奪った、2=加えてその奴隷が恋慕、3と4はそれぞれ1と2と同じで、口上主がペニスを使用）
;213（口上主が助手として処女を奪った数の累計）
;214（1=助手であった口上主の処女を主人が奪った、2=奴隷に奪わせた）
;215（助手としての仕事がどの程度あったか。
;	0は一切出番なし、1はカメラのみ、2は軽い行為参加、3は奴隷同士の行為、4は主人との行為、5はそもそも奴隷がオトコ）
;また、「口上主（助手）→調教対象」の面識判定も更新、口上主のCFLAG:650台を使用
;--------------------------------------------------
@ASSIDATA
;主人が選択するコマンドで助手の出番があったか記録
;騎乗位する・助手（のアナルを）を犯させる・３Ｐ（奴隷同士の行為）
IF SELECTCOM == 400 || SELECTCOM == 401 || SELECTCOM == 402 || SELECTCOM == 403 || SELECTCOM == 404 || SELECTCOM == 405 || SELECTCOM == 406 || SELECTCOM == 407 || SELECTCOM == 430 || SELECTCOM == 431 || SELECTCOM == 515 || SELECTCOM == 517 || SELECTCOM == 518 || SELECTCOM == 519 || SELECTCOM == 520 || SELECTCOM == 521 || SELECTCOM == 522 || SELECTCOM == 523 || SELECTCOM == 524 || SELECTCOM == 525
	;プレイ内容更新
	SIF CFLAG:ASSI:215 <= 2
		CFLAG:ASSI:215 = 3
	;相手がオトコ
	SIF TALENT:オトコ
		CFLAG:ASSI:215 = 5
	;助手（口上主）が処女でＶ挿入（奴隷に奪われた）
	SIF TALENT:ASSI:処女 && (SELECTCOM == 430)
		CFLAG:ASSI:214 = 2
;助手を犯す／助手にアナルを犯される（主人との行為）
ELSEIF SELECTCOM == 440 || SELECTCOM == 441 || SELECTCOM == 270 || SELECTCOM == 271
	;プレイ内容更新
	SIF CFLAG:ASSI:215 <= 3
		CFLAG:ASSI:215 = 4
	;助手（口上主）が処女でＶ挿入（主人に捧げた）
	SIF TALENT:ASSI:処女 && SELECTCOM == 440
		CFLAG:ASSI:214 = 1
;ダブルフェラ、ダブルパイズリ、ダブル素股、二本フェラ（軽めの行為）
ELSEIF SELECTCOM == 420 || SELECTCOM == 421 || SELECTCOM == 422 || SELECTCOM == 516
	;プレイ内容更新
	SIF CFLAG:ASSI:215 <= 1
		CFLAG:ASSI:215 = 2
;ビデオカメラ（撮影担当やらされていると解釈）
ELSEIF SELECTCOM == 310 && FLAG:22 == 2
	;プレイ内容更新
	SIF CFLAG:ASSI:215 == 0
		CFLAG:ASSI:215 = 1
ENDIF

;処女喪失の記録
IF TALENT:処女
	;正常位・後背位・騎乗位（ペニス使用）
	IF (SELECTCOM == 100 || SELECTCOM == 101 || SELECTCOM == 106) && (TALENT:ASSI:121 || TALENT:ASSI:122 || TALENT:ASSI:包茎)
		SIF TALENT:嫉妬
			CFLAG:ASSI:212 = 4
		SIF TALENT:嫉妬 == 0
			CFLAG:ASSI:212 = 3
		CFLAG:ASSI:213 += 1
		A = 650+NO:TARGET
		IF CFLAG:ASSI:A == 5
			CFLAG:ASSI:A = 7
		ELSEIF CFLAG:ASSI:A <= 4
			CFLAG:ASSI:A = 6
		ENDIF
	;バイブ・正常位・後背位・指挿入れ
	ELSEIF SELECTCOM == 43 || SELECTCOM == 100 || SELECTCOM == 101 || SELECTCOM == 41
		SIF TALENT:嫉妬
			CFLAG:ASSI:212 = 2
		SIF TALENT:嫉妬 == 0
			CFLAG:ASSI:212 = 1
		CFLAG:ASSI:213 += 1
		A = 650+NO:TARGET
		IF CFLAG:ASSI:A == 5
			CFLAG:ASSI:A = 7
		ELSEIF CFLAG:ASSI:A <= 4
			CFLAG:ASSI:A = 6
		ENDIF
	ENDIF
ENDIF

;面識判定の更新
IF ASSIPLAY
	CFLAG:211 = 1
	A = 650+NO:TARGET
	IF CFLAG:ASSI:A == 4
		CFLAG:ASSI:A = 5
	ELSEIF CFLAG:ASSI:A <= 2
		CFLAG:ASSI:A = 3
	ENDIF
ENDIF
RETURN 0


