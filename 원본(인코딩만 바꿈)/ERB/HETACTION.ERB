;=============================================================================
;erahetaAの仕様に関する関数群
;=============================================================================
;-------------------------------------------------
;駆け落ちイベント用「調教を強制終了させる」機能
;NEXTCOM = 999でこの処理が呼び出せる
;TFLAG:400 == 1してから呼び出せばキャラを消去出来る
;TFLAG:400 == 0なら消去せず日付だけを送る
;TFLAG:401にキャラ番号を入れることで駆け落ち相手の購入可能フラグもいじれます（要へたくしょん拡張）
;999は調教終了なので、今後も使う事がないはず。という事で占有
;-------------------------------------------------
@COM999
;消去される場合用の処理
IF TFLAG:400 == 1
	;奴隷が脱走して手元からいなくなる。
	T = TARGET
	A = ASSI
	DELCHARA T
	IF A != -1
		IF A > T
			ASSI = A-1
		ELSE
			ASSI = A
		ENDIF
	ENDIF
	TARGET = -1
	;-----------------------
	;テンプレ用：あんみつ専用処理
	;IF TFLAG:401 == 1
	;	FLAG:1023 = 0
	;連れられて逃げる方（清光）をX、清光を連れて行く方（安定）をYとして、
	;Xに清光のキャラ番号、Yに安定のキャラ番号-1を入れることで、駆け落ち相手が買えなくなる
	;ELSEIF TFLAG:401 == 85
	;	FLAG:1086 == 0
	;ENDIF
	;-----------------------
	;-----------------------
	;テンプレ
	;IF TFLAG:401 == 1
	;	FLAG:1023 = 0
	;ELSEIF TFLAG:401 == X
	;	FLAG:10Y == 0
	;ENDIF
	;-----------------------
ENDIF
;消去の有無にかかわらず行われる、調教を強制終了して日付を送る処理
REPEAT CHARANUM
	;気力の回復
	IF TALENT:気絶
		TALENT:気絶 -= 1
	ELSE
		BASE:COUNT:気力 = MAXBASE:COUNT:気力
	ENDIF

	;労役中のキャラは体力回復処理無し
	SIF CFLAG:COUNT:12 > 0
		CONTINUE

	;体力回復停止処理
	IF CFLAG:COUNT:6 > 0
		CFLAG:COUNT:6 -= 1
		CONTINUE
	ENDIF

	;体力の回復（午後の調教後は夜の休みが入るので回復大きい）
	IF TIME == 0
		A = MAXBASE:COUNT:体力 / 15
	ELSE
		A = MAXBASE:COUNT:体力 / 5
	ENDIF

	;回復早い、回復遅い、発情期、不死身
	IF TALENT:COUNT:回復早い
		TIMES A , 1.50
	ELSEIF TALENT:COUNT:回復遅い || TALENT:COUNT:発情期
		TIMES A , 0.75
	ELSEIF TALENT:COUNT:不死身
		TIMES A , 1.80
	ENDIF

	;日光浴、月光浴、吸血鬼
	IF TIME == 0
		SIF TALENT:COUNT:日光浴
			A += 100
	ELSE
		SIF TALENT:COUNT:吸血鬼
			TIMES A , 1.50
		SIF TALENT:COUNT:月光浴
			A += 100
	ENDIF

	;治療
	IF ASSI >= 0
		SIF TALENT:ASSI:治療
		A += 150
	ENDIF

	;休憩フラグ（休憩フラグが経っている、もしくは調教対象でない）
	IF FLAG:0 || TARGET != COUNT
		A += 100
		CFLAG:COUNT:10 -= 1
	ENDIF

	;ショップで休憩を選択時
	IF FLAG:0 == 2
		TIMES A , 2.00
		CFLAG:COUNT:10 -= 1
	ENDIF

	BASE:COUNT:体力 += A
	SIF BASE:COUNT:体力 > MAXBASE:COUNT:体力
		BASE:COUNT:体力 = MAXBASE:COUNT:体力

	;ストレス値が0未満の場合は0にする
	SIF CFLAG:COUNT:10 < 0
		CFLAG:COUNT:10 = 0

REND

CALL EVENTCHECK_ABL
FLAG:0 = 0

;午後なら次の日、午前なら午後にする
IF TIME == 1
	FLAG:81 += 1
	FLAG:82 += 1

	;日付変更時のイベント呼び出し
	CALL EVENT_NEXTDAY

	DAY = DAY+1
	PRINTW 一日が終わった……
	TIME = 0
	;バッドエンドの判定
	SIF DAY > 50 && FLAG:5 == 0
		CALL BADENDING_1
	SIF MONEY < 0 && FLAG:5 == 0
		CALL BADENDING_2

	;朝になった時のイベント呼び出し
	CALL EVENT_NEWDAY
	CALL EVENT_NEWDAY_SELF
ELSE
	;全体の妊娠判定の処理
	CALL IN_VAGINA_ANAL_ALL

	PRINTL 
	PRINTW 夜になりました
	TIME = 1
ENDIF

BEGIN SHOP


;-------------------------------------------------
;主人の誕生日サーチ機能
;CALL MASTERBIRTHDAYでこの処理が呼び出せる
;当日が誕生日であればB:1に1が、一週間以内に誕生日が来るならB:1に2が代入されて返ってくる
;-------------------------------------------------
@MASTERBIRTHDAY
;入れ子を空にする
B:1 = 0



;-------------------------------------------------
;胸のサイズを比較する機能
;AとBに比較したい相手の登録番号を入れてCALL
;B:1に大きかった方の登録番号、B:2にその差を返します。引き分けの時はB:1が-1になります
;登録番号は@NOなりTARGETとASSIで判定するなりして使ってください
;-------------------------------------------------
@COMPARE_BSIZE
A:1 = 0
A:2 = 0
B:1 = 0
B:2 = 0
IF TALENT:A:絶壁
	A:1 = 0
ELSEIF TALENT:A:貧乳
	A:1 = 1
ELSEIF TALENT:A:巨乳
	A:1 = 3
ELSEIF TALENT:A:爆乳
	A:1 = 4
ELSE
	A:1 = 2
ENDIF

IF TALENT:B:絶壁
	A:2 = 0
ELSEIF TALENT:B:貧乳
	A:2 = 1
ELSEIF TALENT:B:巨乳
	A:2 = 3
ELSEIF TALENT:B:爆乳
	A:2 = 4
ELSE
	A:2 = 2
ENDIF

IF A:1 > A:2
	B:1 = A
	B:2 = A:1
	B:2 -= A:2
ELSEIF A:1 < A:2
	B:1 = B
	B:2 = A:2
	B:2 -= A:2
ELSE
	B:1 = -1
	B:2 = 0
ENDIF

;-------------------------------------------------
;下のサイズを比較する機能
;AとBに比較したい相手の登録番号を入れてCALL
;B:1に大きかった方の登録番号、B:2にその差を返します。引き分けの時はB:1が-1になります
;登録番号は@NOなりTARGETとASSIで判定するなりして使ってください
;-------------------------------------------------
@COMPARE_CSIZE
A:1 = 0
A:2 = 0
B:1 = 0
B:2 = 0
IF TALENT:A:子供サイズ
	A:1 = 0
ELSEIF TALENT:A:短小
	A:1 = 1
ELSEIF TALENT:A:巨根
	A:1 = 3
ELSEIF TALENT:A:馬並み
	A:1 = 4
ELSE
	A:1 = 2
ENDIF

IF TALENT:B:子供サイズ
	A:2 = 0
ELSEIF TALENT:B:短小
	A:2 = 1
ELSEIF TALENT:B:巨根
	A:2 = 3
ELSEIF TALENT:B:馬並み
	A:2 = 4
ELSE
	A:2 = 2
ENDIF

IF A:1 > A:2
	B:1 = A
	B:2 = A:1
	B:2 -= A:2
ELSEIF A:1 < A:2
	B:1 = B
	B:2 = A:2
	B:2 -= A:1
ELSE
	B:1 = -1
	B:2 = 0
ENDIF

;=============================================================================
;面識機能に関する関数群
;=============================================================================
;-------------------------------------------------
;記録機能用の@EVENT関数
;-------------------------------------------------
@EVENTEND
	CALL SAVE_ACQUAINTANCE_ASSI
	PREVCOM:829 = NO:TARGET
	IF ASSI >= 0
		PREVCOM:830 = NO:ASSI
	ELSE
		PREVCOM:830 = -1
	ENDIF
	PREVCOM:845 = NO:MASTER
@EVENTTURNEND
	CALL SAVE_MEMBER
	CALL SAVE_MEMBER_MASTER

;-------------------------------------------------
;ここ一週間の助手と調教対象を記録する機能
;PREVCOM:800以降を盛大に使用します
;801 ～ 814 = ここ一週間の奴隷のキャラ番号
;815 ～ 828 = ここ一週間の助手
;829 保存用
;830 保存用
;-------------------------------------------------
@SAVE_MEMBER
;奴隷のキャラ番号を登録
I = 814
REPEAT 14
	IF I != 801
		PREVCOM:I = PREVCOM:(I-1)
	ELSE
		IF TARGET > 0
			IF PREVCOM:829 == NO:TARGET
				PREVCOM:801 = NO:TARGET
			ELSE
				PREVCOM:801 = -1
			ENDIF
		ELSE
			PREVCOM:801 = -1
		ENDIF
	ENDIF
	I -= 1
REND
;助手のキャラ番号を登録
I = 828
REPEAT 14
	IF I != 815
		PREVCOM:I = PREVCOM:(I-1)
	ELSE
		IF ASSI >= 0
			IF PREVCOM:830 == NO:ASSI
				PREVCOM:815 = NO:ASSI
			ELSE
				PREVCOM:815 = -1
			ENDIF
		ELSE
			PREVCOM:815 = -1
		ENDIF
	ENDIF
	I -= 1
REND
PREVCOM:829 = -1
PREVCOM:830 = -1

;-------------------------------------------------
;ここ一週間の主人を記録する機能　※eraToloveA02私家改造版にて追記
;念のため関数分けました
;PREVCOM:800以降を盛大に使用します
;831 ～ 844 = ここ一週間の主人のキャラ番号
;845 保存用
;-------------------------------------------------
@SAVE_MEMBER_MASTER
;助手のキャラ番号を登録
I = 844
REPEAT 14
	IF I != 831
