		IF MASTER >= 0
			IF PREVCOM:845 == NO:MASTER
				PREVCOM:831 = NO:MASTER
			ELSE
				PREVCOM:831 = -1
			ENDIF
		ELSE
			PREVCOM:831 = -1
		ENDIF
	ENDIF
	I -= 1
REND
PREVCOM:845 = -1

;-------------------------------------------------
;日常での面識を更新する機能
;へたくしょん導入で勝手に保存されるようになります
;更新には口上側の制御が必要なので注意
;-------------------------------------------------
@SAVE_ACQUAINTANCE_DAILY_LIFE
;Bが登場しない場合は無視
IF B > 0 && B < CHARANUM
	G = (2099 + NO:A)
	H = (2099 + NO:B)
	CFLAG:A:H |= 1
	CFLAG:B:G |= 1
	;Cが居るならCの分も判定
	IF C > 0 && C < CHARANUM && C != B
		I = (2099 + NO:C)
		CFLAG:A:I |= 1
		CFLAG:B:I |= 1
		CFLAG:C:G |= 1
		CFLAG:C:H |= 1
	ENDIF
ENDIF

;-------------------------------------------------
;助手調教での面識を更新する機能
;へたくしょん導入で勝手に保存されるようになります
;これも更新には口上側の制御が必要なので注意
;-------------------------------------------------
@SAVE_ACQUAINTANCE_ASSI
H = (2099 + NO:TARGET)
I = (2099 + NO:MASTER)
CFLAG:TARGET:I |= 2
CFLAG:MASTER:H |= 2
;ASSIがいない場合は無視
IF ASSI >= 0
	G = (2099 + NO:ASSI)
	CFLAG:ASSI:H |= 2
	CFLAG:ASSI:I |= 2
	CFLAG:TARGET:G |= 2
	CFLAG:MASTER:G |= 2
ENDIF
IF ASSIPLAY
	CFLAG:ASSI:H |= 8
	CFLAG:TARGET:G |= 4
ELSE
	CFLAG:MASTER:H |= 8
	CFLAG:TARGET:I |= 4
ENDIF

;-------------------------------------------------
;ここ一週間の調教対象をサーチする機能
;Rに調教対象のキャラ番号を入れてCALL
;D:1に調教対象と会った日を返す。D:2に実際に会ったのが何回前かを返す
;-------------------------------------------------
@GET_MEMBER_TARGET
;調教対象サーチ
I = 814
D:1 = -1
D:2 = -1
REPEAT 14
	SIF PREVCOM:I == R
		D:1 = (I - 800)
	I -= 1
REND
IF D:1 >= 0
	D:2 = D:1
	;割って余りが出ない時は、そのまま÷2で日付が出る
	IF (D:1 % 2) == 0
		D:1 /= 2
	;割って余りが出る時は、今が昼か夜かで結果が変わる
	ELSE
		;昼なら前日なので、1を足す
		IF TIME == 0
			D:1 += 1
		;夜ならその日のうちに会ってるので1を引く
		ELSE
			D:1 -= 1
		ENDIF
		D:1 /= 2
	ENDIF
ENDIF

;-------------------------------------------------
;ここ一週間の助手をサーチする機能
;Rに助手のキャラ番号を入れてCALL
;D:3に助手と会った日を、D:4に実際に会ったのが何回前か返す
;当日の助手？ 助手口上で判定すればいいと思うよ！
;-------------------------------------------------
@GET_MEMBER_ASSI
;助手サーチ
I = 828
D:3 = -1
D:4 = -1
REPEAT 14
	SIF PREVCOM:I == R
		D:3 = (I - 814)
	I -= 1
REND
IF D:3 >= 0
	D:4 = D:3
	;割って余りが出ない時は、そのまま÷2で日付が出る
	IF (D:3 % 2) == 0
		D:3 /= 2
	;割って余りが出る時は、今が昼か夜かで結果が変わる
	ELSE
		;昼なら前日なので、1を足す
		IF TIME == 0
			D:3 += 1
		;夜ならその日のうちに会ってるので1を引く
		ELSE
			D:3 -= 1
		ENDIF
		D:3 /= 2
	ENDIF
ENDIF

;-------------------------------------------------
;ここ一週間の主人をサーチする機能
;Rに主人のキャラ番号を入れてCALL
;D:5に助手と会った日を、D:6に実際に会ったのが何回前か返す
